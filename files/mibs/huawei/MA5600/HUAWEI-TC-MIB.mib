        ifAdminStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                up(1),
                down(2),
                testing(3),
-- huawei expand
                block(65536),               -- 阻塞操作
                unblock(65537),             -- 解阻塞操作
                reset(65538),               -- 复位
                localloopback(65539),       -- 近端环回
                remoteloopback(65540),      -- 远端环回
                payloadloopback(65541),     -- 有效负载环回
                unloopback(65542),          -- 解环回
                linkrxblock(65543),         -- 链路接收方向阻塞
                linktxblock(65544),         -- 链路发送方向阻塞
                linkallblock(65545),        -- 链路双向同时阻塞
                linkrxunblock(65546),       -- 链路接收方向解阻塞
                linktxunblock(65547),       -- 链路发送方向解阻塞
                    linkallunblock(65548),     -- 链路双向同时解阻塞
                    adslloopbackafe(65549),    -- ADSL端口AFE环回
                    adslloopbackhybrid(65550)  -- ADSL端口HYBRID环回
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The current operational state of the interface.  The
                testing(3) state indicates that no operational packets
                can be passed.  If ifAdminStatus is down(2) then
                ifOperStatus should be down(2).  If ifAdminStatus is
                changed to up(1) then ifOperStatus should change to
                up(1) if the interface is ready to transmit and
                receive network traffic; it should change to
                dormant(5) if the interface is waiting for external
                actions (such as a serial line waiting for an incoming
                connection); it should remain in the down(2) state if
                and only if there is a fault that prevents it from
                going to the up(1) state; it should remain in the
                notPresent(6) state if the interface has missing
                (typically, hardware) components.

                huawei mib expand from 65536 .


                Because  management states in the standard MIB can't satisfy
                the request of all kinds of the product ,
                the extension is performed

                "
            ::= { ifEntry 7 }

        ifOperStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                up(1),
                down(2),
                testing(3),
                unknown(4),
                dormant(5),
                notPresent(6),
                lowerLayerDown(7),
                activing(65536),
                block(65537),
                config(65538),
                localloopback(65539),           -- 近端环回
                remoteloopback(65540),          -- 远端环回
                payloadloopback(65541),         -- 有效负载环回
                blockandlocalloopback(65542),   -- 阻塞并且近端环回
                blockandremoteloopback(65543),  -- 阻塞并且远端环回
                blockandpayloadloopback(65544), -- 阻塞并且有效负载环回
                linkrxblock(65545),             -- 链路接收方向阻塞
                linktxblock(65546),             -- 链路发送方向阻塞
                linkallblock(65547),            -- 链路双向同时阻塞
                fault(65548),                   -- 故障
                unconfig(65549),                -- 未配置
                            empty(65550),                  -- 空
                    adslloopbackafe(65551),    -- ADSL端口AFE环回
                    adslloopbackhybrid(65552)  -- ADSL端口HYBRID环回
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "
                "
            ::= { ifEntry 8 }

