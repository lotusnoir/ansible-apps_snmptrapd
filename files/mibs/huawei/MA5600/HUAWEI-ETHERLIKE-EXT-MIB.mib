--			
-- hwFlowIndexQueryTable
--
		hwFlowIndexQueryTable OBJECT-TYPE
			SYNTAX SEQUENCE OF HwFlowIndexQueryEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" 
				Index of service virtual port query table. 
				"
			::= { hwExtSrvFlow 5 }
			
		hwFlowIndexQueryEntry OBJECT-TYPE
		SYNTAX HwFlowIndexQueryEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" The entry of service virtual port query table. "
			INDEX { ifIndex,
			        hwFlowBridgePortType,hwFlowPortExtPara1,hwFlowPortExtPara2, hwFlowPortExtPara3,
			        hwFlowMulitiServiceType,hwFlowMulitiServicePara
			       }
			::= { hwFlowIndexQueryTable 1 }
			
		HwFlowIndexQueryEntry ::=
		SEQUENCE { 
			hwFlowBridgePortType
				INTEGER,
			hwFlowPortExtPara1
				INTEGER,
			hwFlowPortExtPara2
				INTEGER,
			hwFlowPortExtPara3
				INTEGER,
			hwFlowMulitiServiceType
				INTEGER,
			hwFlowMulitiServicePara
				INTEGER,
			hwExtSrvFlowIndex
				INTEGER
			 }
						
		hwFlowBridgePortType OBJECT-TYPE
			SYNTAX INTEGER
			             {
			             vc_port(1),
			             ethernet_ptm(2),
			             gem_port(3)
			             }
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				The mode of service virtual port.
				"
			::= { hwFlowIndexQueryEntry 1 }
			
		hwFlowPortExtPara1 OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				The service virtual port parameter1.
				If hwFlowBridgePortType is VC Port, the parameter is VPI.
				If hwFlowBridgePortType is Ethernet/PTM, the parameter is of no effect.
				If hwFlowBridgePortType is GEM Port, the parameter is GEM Port ID.
				"
			::= { hwFlowIndexQueryEntry 2 }
			
		hwFlowPortExtPara2 OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				The service virtual port parameter2.
				If hwFlowBridgePortType is VC Port, the parameter is VCI.
				If hwFlowBridgePortType is Ethernet/PTM, the parameter is of no effect.
				If hwFlowBridgePortType is GEM Port, the parameter is of no effect.
				"
			::= { hwFlowIndexQueryEntry 3 }
			
		hwFlowPortExtPara3 OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				The service virtual port parameter3.
				"
			::= { hwFlowIndexQueryEntry 4 }
			
		hwFlowMulitiServiceType OBJECT-TYPE
			SYNTAX INTEGER
			             {
			              by_uservlan(1),
			              by_encap(2),
			              by_8021p(3),
			              single_service(255)
			             }
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				The mode of Multi-service.
				"
			::= { hwFlowIndexQueryEntry 5 }
			
		hwFlowMulitiServicePara OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				 The Multi-service parameter.
				 If hwFlowMulitiServiceType is by_uservlan, the bound of parameter 
				 is 1 to 4095 or 0(priority tagged) or 65534(TLS) or 65535(untagged).
				 If hwFlowMulitiServiceType is by_encap, the parameter is IPoE(1) or PPPoE(2).
				 If hwFlowMulitiServiceType is by_8021p,
				 the parameter is the combination of 802.1p priority(0 to 7) and User VLAN(0 to 4095)
				 bit value:       31              16 15      13    12     11         0
				 combination:     |     reserve     | priority | reserve | User VLAN |
				 If hwFlowMulitiServiceType is single_service, the parameter is of no effect.
				"
			::= { hwFlowIndexQueryEntry 6 }

		hwExtSrvFlowIndex OBJECT-TYPE
			SYNTAX INTEGER (1..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"
				A unique value to identify this service virtual port. 
				"
			::= { hwFlowIndexQueryEntry 7 }	
	           			
---------------------------------------------------------------------
-- hwStaticMacTable
---------------------------------------------------------------------
		hwStaticMacTable OBJECT-TYPE
			SYNTAX SEQUENCE OF HwStaticMacEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" Static MAC address table  "
			::= { hwEthernet 11 }
			
		hwStaticMacEntry OBJECT-TYPE
		SYNTAX HwStaticMacEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" Static MAC address table property "
			INDEX { hwStaticMacIndex }
			::= { hwStaticMacTable 1 }
			
		HwStaticMacEntry ::=
		SEQUENCE { 
			hwStaticMacIndex
				Unsigned32,
			hwStaticMacIfIndex
				Unsigned32,
			hwStaticMacVlanID
				Unsigned32,
			hwStaticMacVPI
				Unsigned32,
			hwStaticMacVCI
				Unsigned32,
			hwStaticMacFlowID
				Unsigned32,
			hwStaticMacMacAddr
				MacAddress,
			hwStaticMacRowStatus
				INTEGER
			 }

		hwStaticMacIndex   OBJECT-TYPE
			SYNTAX Unsigned32 (1..1024)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The mux number of static mac can be set is 1024 for MA5600
				and 512 for others, and any mac accepted is stored in a control 
				struct.This leaf just signals the index of the exact mac control 
				struct."
			::= { hwStaticMacEntry 1 }
			
		hwStaticMacIfIndex OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"In mac control struct, mac is stored together with the port
				information accordingly. It gives the port type, such as ADSL,
				ETH,ect, and the frame/slot/port/."
			::= { hwStaticMacEntry 2 }
			
		hwStaticMacVlanID OBJECT-TYPE
			SYNTAX Unsigned32 (1..4093)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"when set static mac to eth port, a vlan ID must be provided together.
				It is just the vlan ID"
			::= { hwStaticMacEntry 3 }
			
		hwStaticMacVPI OBJECT-TYPE
			SYNTAX Unsigned32 (0..4095,8191)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The VPI of a PVC"
			::= { hwStaticMacEntry 4 }
			
		hwStaticMacVCI OBJECT-TYPE
			SYNTAX Unsigned32 (32..65535)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The VCI of a PVC"
			::= { hwStaticMacEntry 5 }
			
		hwStaticMacFlowID OBJECT-TYPE
			SYNTAX Unsigned32 (0..8191)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Flow ID,index to the service virtual port"
			::= { hwStaticMacEntry 6 }
			
		hwStaticMacMacAddr OBJECT-TYPE
			SYNTAX MacAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"the mac address, and its format is XX-XX-XX-XX-XX-XX.
				The exception is 00-00-00-00-00-00, and also broadcast mac
				and all multicast mac are invalid.				
				"
			::= { hwStaticMacEntry 7 }
			
		hwStaticMacRowStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				    active(1),
				    notInService(2),
				    notReady(3),
				    createAndGo(4),   
				    createAndWait(5),
				    destroy(6), 
				    modify(7)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"the variable is used to decide what operation will be done
				when set from SNMP. The supported operations are create and
				destroy.
				"
			::= { hwStaticMacEntry 8 }

------------------------------------------------------------------			
-- hwStaticMacNextFreeIndex
------------------------------------------------------------------			
    hwStaticMacNextFreeIndex OBJECT-TYPE
			SYNTAX INTEGER 
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
			"  This object contains an appropriate value to
 	          be used for hwStaticMacIndex when creating
 	          entries in the hwStaticMacTable. The value
 	          0 indicates that no unassigned entries are
 	          available. To obtain the hwStaticMacIndex
 	          value for a new entry, the manager issues a
 	          management protocol retrieval operation to obtain
 	          the current value of this object.  After each
 	          retrieval, the agent should modify the value to
 	          the next unassigned index.
 	            After a manager retrieves a value the agent will
 	          determine through its local policy when this index
 	          value will be made available for reuse.  "
 	        ::= { hwEthernet 12 }	
 	        
