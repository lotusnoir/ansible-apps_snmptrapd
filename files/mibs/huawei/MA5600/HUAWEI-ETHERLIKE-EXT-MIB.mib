		hwAlarmEthernetPortDeActive NOTIFICATION-TYPE
			OBJECTS { hwPortIndex }
			STATUS current
			DESCRIPTION 
				"alarm description: port deactive
				explaination： LAN port deactive
				resolving method:check port status is active
				"
			::= { hwEthernetAlarmTrapsOids 5 }
