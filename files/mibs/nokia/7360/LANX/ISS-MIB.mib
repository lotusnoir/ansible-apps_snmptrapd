issRateControl OBJECT IDENTIFIER
    ::= { iss 4 }

issRateCtrlTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IssRateCtrlEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A table to control the rate limiting parameters 
         either for the entire switch or for each interface in the switch.
          ALCATEL NOTE :
          TABLESIZE    : 25 
          DESCR        : NA"
    ::= { issRateControl 1 }

issRateCtrlEntry OBJECT-TYPE
    SYNTAX      IssRateCtrlEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "An entry appears in this table for each interface in the switch.
         
         In addition to that an entry with index '0' is created in this 
         table by default.
         
         If 'issRateCtrlStatus' of the entry '0' is made 'valid', 
         then global control is chosen and the values against entry '0'
         is applicable for all the interfaces of the switch.
         
         If 'issRateCtrlStatus' of the entry '0' is made 'invalid', then 
         interface-wise  control is chosen & the values against each 
         interface index is applicable for that interface.
         
         At a particular point of time, either global control will be 
         applicable or interface-wise control is applicable. Both will 
         not be considered together.
          Row creation : A RATE-CONTROL entry shall be created when the system is initialized.     
          Row deletion : NA.
         	Index usage : The indexe is issRateCtrlIndex.
           	   Note that entries will be presented to the SNMP manager in full
           	   lexicographical order :  sorted on issRateCtrlIndex"
    INDEX       { issRateCtrlIndex }
    ::= { issRateCtrlTable 1 }

IssRateCtrlEntry ::= SEQUENCE {
    issRateCtrlIndex                INTEGER,
    issRateCtrlDLFLimitStatus       INTEGER,
    issRateCtrlBCASTLimitStatus     INTEGER,
    issRateCtrlMCASTLimitStatus     INTEGER,
    issRateCtrlLimitValue           INTEGER,
    issRateCtrlStatus               INTEGER
}

issRateCtrlIndex OBJECT-TYPE
    SYNTAX      INTEGER (0..65535)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The interface index for which the configuration in this 
         entry applies.
         
         If any configuration is made in this table for the index '0',
         that configuration will be considered as global configuration 
         and is applicable for all the interfaces of the switch.
           ALCATEL NOTE:
           ACCESS	:   NA         		USAGE		:    NA
           PERSIST:   YES          	INSRVMOD	:    NA
           RANGE	:   DEP  	        DEFVALUE	:    NA
           UNITS	:   NA           	SPARSE		:    NO
           DESCR	:   DEP(RANGE)The range is the maximum number of physical ports available in 
         		     the system. The value can be got from the object issSystemNumberOfPorts
         		     in fsiss.mib."
    ::= { issRateCtrlEntry 1 }

issRateCtrlDLFLimitStatus OBJECT-TYPE
    SYNTAX      INTEGER { enabled(1), disabled(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Provides control to enable or disable Destination Lookup Failure
         packet transmission control over the interface.
         
         This value for entry '0' controls DLF Limit Status for all the 
         interfaces in the switch.
           ALCATEL NOTE:
           ACCESS	:   NA         		USAGE		:    NA
           PERSIST:   YES          	INSRVMOD	:    YES
           RANGE	:   NA  	        DEFVALUE	:    NA
           UNITS	:   NA           	SPARSE		:    NO
           DESCR	:   NA"
    DEFVAL      { disabled }
    ::= { issRateCtrlEntry 2 }

issRateCtrlBCASTLimitStatus OBJECT-TYPE
    SYNTAX      INTEGER { enabled(1), disabled(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Provides control to enable or disable broadcast
         packet transmission control over the interface.
         
         This value for entry '0' controls broadcast Limit Status
         for all the interfaces in the switch.
          ALCATEL NOTE:
          ACCESS	:   NA         		USAGE		:    NA
          PERSIST:   YES          	INSRVMOD	:    YES
          RANGE	:   NA  	        DEFVALUE	:    NA
          UNITS	:   NA           	SPARSE		:    NO
          DESCR	:   NA"
    DEFVAL      { disabled }
    ::= { issRateCtrlEntry 3 }

issRateCtrlMCASTLimitStatus OBJECT-TYPE
    SYNTAX      INTEGER { enabled(1), disabled(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Provides control to enable or disable multicast 
         packet transmission control over theinterface.
         
         This value for entry '0' controls multicast traffic Limit Status
         for all the interfaces in the switch.
           ALCATEL NOTE:
           ACCESS	:   NA         		USAGE		:    NA
           PERSIST:   YES          	INSRVMOD	:    YES
           RANGE	:   NA  	        DEFVALUE	:    NA
           UNITS	:   NA           	SPARSE		:    NO
           DESCR	:   NA"
    DEFVAL      { disabled }
    ::= { issRateCtrlEntry 4 }

issRateCtrlLimitValue OBJECT-TYPE
    SYNTAX      INTEGER (0..2147483647)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Allows to configure the limiting value for the maximum number
         of packets that can be transmitted over this interface.
         
         The LimitValue for entry '0' is applicable for 
         for all the interfaces in the switch.
           ALCATEL NOTE:
           ACCESS	:   NA         		USAGE		:    NA
           PERSIST:   YES          	INSRVMOD	:    YES
           RANGE  : 0..0x1ffffff        DEFVALUE	:    0
           UNITS	:   NA           	SPARSE		:    NO
           DESCR	:   NA"
    ::= { issRateCtrlEntry 5 }

issRateCtrlStatus OBJECT-TYPE
    SYNTAX      INTEGER { valid(1), invalid(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Speficies the validity of the entry.
         
         If the 'Rate Control Status' for entry '0' is made 'valid',
         then global control is chosen in the system.
         It signifies that the values against entry '0' is applicable
         for all the interfaces of the switch.
         
         If the 'Rate Control Status' for entry '0' is made 'invalid',
         then interface-wise control is chosen.
         Then the values against each interface index is applicable for
         that interface.
         
         By default, 'issRateCtrlStatus' will be 'invalid' for entry '0' 
         and 'valid' for all other entries. (ie) by default, 
         interface-wise configuration is enabled in the system.
         
         The status of entries other than the zeroth(0th) entry can not be
         made 'invalid'.
            ALCATEL NOTE:
            ACCESS	:   NA         		USAGE		:    NA
            PERSIST:   YES          	INSRVMOD	:    YES
            RANGE	:   NA 	  		DEFVALUE	:    NA
            UNITS	:   NA           	SPARSE		:    NO
            DESCR	:   NA"
    ::= { issRateCtrlEntry 6 }

