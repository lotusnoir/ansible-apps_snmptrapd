    DESCRIPTION 
        "A single TYPE11 Opaque Link State Advertisement.
         Row creation : An entry shall be created dynamically. 
         Row deletion : An entry shall be deleted dynamically.
         Index usage : the index is the Opaque Type,Link State Id,Router Id.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on Opaque Type,Link State Id,Router Id."
    INDEX       { fsMIStdOspfContextId, fsMIOspfType11LsdbOpaqueType, 
                  fsMIOspfType11LsdbLsid, fsMIOspfType11LsdbRouterId }
    ::= { fsMIOspfType11LsdbTable 1 }

FsMIOspfType11LsdbEntry ::= SEQUENCE {
    fsMIOspfType11LsdbOpaqueType        INTEGER,
    fsMIOspfType11LsdbLsid              IpAddress,
    fsMIOspfType11LsdbRouterId          RouterID,
    fsMIOspfType11LsdbSequence          INTEGER,
    fsMIOspfType11LsdbAge               INTEGER,
    fsMIOspfType11LsdbChecksum          INTEGER,
    fsMIOspfType11LsdbAdvertisement     OCTET STRING
}

fsMIOspfType11LsdbOpaqueType OBJECT-TYPE
    SYNTAX      INTEGER (1..11)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Opaquetype  of  the  link  state  advertisement.
         ALCATEL NOTE:
           ACCESS:      NA          USAGE:       NA
           PERSIST:     NO          INSRVMOD:    NA
           RANGE:       NA          DEFVALUE:    NA    
           UNITS:       NA          SPARSE:      NO
           DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 1 }

fsMIOspfType11LsdbLsid OBJECT-TYPE
    SYNTAX      IpAddress
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Link State ID identifies the opaque type and
         the opaque ID of the received Opaque LSA for an application.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 2 }

fsMIOspfType11LsdbRouterId OBJECT-TYPE
    SYNTAX      RouterID
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The 32 bit number that uniquely identifies the
         originating router in the Autonomous System.
         ALCATEL NOTE:
          ACCESS:      NA          USAGE:       NA
          PERSIST:     NO          INSRVMOD:    NA
          RANGE:       NA          DEFVALUE:    NA
          UNITS:       NA          SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 3 }

fsMIOspfType11LsdbSequence OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The sequence number field is a  signed  32-bit
         integer.   It  is used to detect old and dupli-
         cate link state advertisements.  The  space  of
         sequence  numbers  is  linearly  ordered.   The
         larger the sequence number the more recent  the
         advertisement.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 4 }

fsMIOspfType11LsdbAge OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This field is the age of the link state adver-
         tisement in seconds.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       0..3600     DEFVALUE:    0
         UNITS:       SECONDS     SPARSE:      NO
         DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 5 }

fsMIOspfType11LsdbChecksum OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This field is the  checksum  of  the  complete
         contents  of  the  advertisement, excepting the
         age field.  The age field is excepted  so  that
         an   advertisement's  age  can  be  incremented
         without updating the  checksum.   The  checksum
         used  is  the same that is used for ISO connec-
         tionless datagrams; it is commonly referred  to
         as the Fletcher checksum.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    0    
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 6 }

fsMIOspfType11LsdbAdvertisement OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(1..65535))
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The entire Link State Advertisement, including
         its header.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    '\0'
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
--     REFERENCE   
--         "The Opaque LSA Option ,Section 5.0 Management Considerations"
    ::= { fsMIOspfType11LsdbEntry 7 }

fsMIOspfAppInfoDbTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMIOspfAppInfoDbEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "This table maintains information related to application statistics.       
         ALCATEL NOTE: 
         	TABLE SIZE:   255 
         	DESCR:        NA"
    ::= { fsMIOspfOpaqueLSAGroup 5 }

fsMIOspfAppInfoDbEntry OBJECT-TYPE
    SYNTAX      FsMIOspfAppInfoDbEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A single Application's  statistical Information.
         Row creation : An entry shall be created dinamically. 
         Row deletion : An entry can be deleted when any dinamic infornmation lost.
         Index usage : the index is the Database Application Id.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on Database Application Id."
    INDEX       { fsMIStdOspfContextId, fsMIOspfAppInfoDbAppid }
    ::= { fsMIOspfAppInfoDbTable 1 }

FsMIOspfAppInfoDbEntry ::= SEQUENCE {
    fsMIOspfAppInfoDbAppid              INTEGER,
    fsMIOspfAppInfoDbOpaqueType         INTEGER,
    fsMIOspfAppInfoDbLsaTypesSupported  INTEGER,
    fsMIOspfAppInfoDbType9Gen           Counter,
    fsMIOspfAppInfoDbType9Rcvd          Counter,
    fsMIOspfAppInfoDbType10Gen          Counter,
    fsMIOspfAppInfoDbType10Rcvd         Counter,
    fsMIOspfAppInfoDbType11Gen          Counter,
    fsMIOspfAppInfoDbType11Rcvd         Counter
}

fsMIOspfAppInfoDbAppid OBJECT-TYPE
    SYNTAX      INTEGER (1..65535)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Application's ID. Each Application has a
         unique Appid associated with it.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    NA    
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 1 }

fsMIOspfAppInfoDbOpaqueType OBJECT-TYPE
    SYNTAX      INTEGER (0..255)
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The OpaqueType of the LSA the application can
         generate or receive. Each Application can register
         only for a single OpaqueType.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    NA    
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 2 }

fsMIOspfAppInfoDbLsaTypesSupported OBJECT-TYPE
    SYNTAX      INTEGER (0..7)
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The LSA Types the application can generate is indicated
         by a BITMASK.The BITMASK and the interpretation of the
         types supported is given as below:-
         
          BitMask    TypesSupported
          0 0 0   ==>  No types
          0 0 1   ==>  Only Type9
          0 1 0   ==>  Only Type10
          0 1 1   ==>  Types 9,10
          1 0 0   ==>  Only Type11
          1 0 1   ==>  Types 9,11
          1 1 0   ==>  Types 10,11
          1 1 1   ==>  Types 9,10,11
         The remaining values are left for future definition.
         ALCATEL NOTE:
          ACCESS:      NA          USAGE:       NA
          PERSIST:     NO          INSRVMOD:    NA
          RANGE:       NA          DEFVALUE:    NA    
          UNITS:       NA          SPARSE:      NO
          DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 3 }

fsMIOspfAppInfoDbType9Gen OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of Type 9 LSAs generated by the application.
         This field is invalid if the application does not
         support the generation of Type9 LSAs.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    0
         UNITS:       LSA         SPARSE:      NO
         DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 4 }

fsMIOspfAppInfoDbType9Rcvd OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of Type 9 LSAs received by the application.
         ALCATEL NOTE:
           ACCESS:      NA          USAGE:       NA
           PERSIST:     NO          INSRVMOD:    NA
           RANGE:       NA          DEFVALUE:    0
           UNITS:       LSA         SPARSE:      NO
           DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 5 }

fsMIOspfAppInfoDbType10Gen OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of Type 10 LSAs generated by the application.
         This field is invalid if the application does not
         support the generation of Type10 LSAs.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    0
         UNITS:       LSA         SPARSE:      NO
         DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 6 }

fsMIOspfAppInfoDbType10Rcvd OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of Type 10 LSAs received by the application.
         ALCATEL NOTE:
           ACCESS:      NA          USAGE:       NA
           PERSIST:     NO          INSRVMOD:    NA
           RANGE:       NA          DEFVALUE:    0
           UNITS:       LSA         SPARSE:      NO
           DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 7 }

fsMIOspfAppInfoDbType11Gen OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of Type 11 LSAs generated by the application.
         This field is invalid if the application does not support
         the generation of Type11 LSAs.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     NO          INSRVMOD:    NA
         RANGE:       NA          DEFVALUE:    0
         UNITS:       LSA         SPARSE:      NO
         DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 8 }

fsMIOspfAppInfoDbType11Rcvd OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of Type 11 LSAs received by the application.
         ALCATEL NOTE:
           ACCESS:      NA          USAGE:       NA
           PERSIST:     NO          INSRVMOD:    NA
           RANGE:       NA          DEFVALUE:    0
           UNITS:       LSA         SPARSE:      NO
           DESCR:       NA"
    ::= { fsMIOspfAppInfoDbEntry 9 }

fsMIOspfRRDGroup OBJECT IDENTIFIER
    ::= { fsMIOspf 13 }

fsMIOspfRRDGeneralGroup OBJECT IDENTIFIER
    ::= { fsMIOspfRRDGroup 1 }

fsMIOspfRRDRouteTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMIOspfRRDRouteEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        " Describes RRDRoute Entry 
         ALCATEL NOTE: 
         	TABLE SIZE:   64 
         	DESCR:        NA"
    ::= { fsMIOspfRRDGeneralGroup 1 }

fsMIOspfRRDRouteEntry OBJECT-TYPE
    SYNTAX      FsMIOspfRRDRouteEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        " Information describing the RRDRoute 
         Row creation : An entry will be created by the system when an entry
             		is created in the fsMIStdOspfTable.
         Row deletion : An entry will be deleted by the system when an entry
             		is deleted in the fsMIStdOspfTable.
         Index usage : the index is fsMIStdOspfEntry.
           Note that entries will be presented to the SNMP manager in full
           lexicographical order :  sorted on fsMIStdOspfEntry."
    INDEX       { fsMIStdOspfContextId }
--     AUGMENTS    { fsMIStdOspfEntry }
    ::= { fsMIOspfRRDRouteTable 1 }

FsMIOspfRRDRouteEntry ::= SEQUENCE {
    fsMIOspfRRDStatus               Status,
    fsMIOspfRRDSrcProtoMaskEnable   INTEGER,
    fsMIOspfRRDSrcProtoMaskDisable  INTEGER
}

fsMIOspfRRDStatus OBJECT-TYPE
    SYNTAX      Status
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This MIB object enables/disables OSPF to participate in Route 
         Redistribution. If set to enabled, OSPF will start advertising 
         the routes learned by other protocols. If set to disabled, OSPF 
         will stop redisbution of routes. However, OSPF will continue  
         updating the Common Routing Table using the queue interface.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       1,2         DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { disabled }
    ::= { fsMIOspfRRDRouteEntry 1 }

fsMIOspfRRDSrcProtoMaskEnable OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The bit mask of source protocols from which routes are 
         imported to OSPF.
         
         Bit 1, if set, indicates that local routes are
         redistributed into OSPF. (value = 2)
         
         Bit 2, if set, indicates that static routes are
         redistributed into OSPF. (value = 4)
         
         Bit 7, if set, indicates that routes from RIP are
         redistributed into OSPF. (value = 128)
         
         Bit 13, if set, indicates that routes from BGP are
         redistributed into OSPF. (value = 8192)
         
         Other bits are reserved for future use.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { 0 }
    ::= { fsMIOspfRRDRouteEntry 2 }

fsMIOspfRRDSrcProtoMaskDisable OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The bit mask of source protocols from which redistribution 
         to OSPF is disabled.
         
         Bit 1, if set, indicates that local routes are not
         redistributed into OSPF. (value = 2)
         
         Bit 2, if set, indicates that static routes are not
         redistributed into OSPF. (value = 4)
         
         Bit 7, if set, indicates that routes from RIP are not
         redistributed into OSPF. (value = 128)
         
         Bit 13, if set, indicates that routes from BGP are not
         redistributed into OSPF. (value = 8192) 
         
         Other bits are reserved for future use.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       NA
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { 8326 }
    ::= { fsMIOspfRRDRouteEntry 3 }

fsMIOspfRRDRouteConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMIOspfRRDRouteConfigEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A table describing the configuration of Metric Cost and Route
         Type information to be applied to the routes learnt from the RTM.
         ALCATEL NOTE:
         	TABLE SIZE:   200
         	DESCR:        NA"
    ::= { fsMIOspfRRDGroup 2 }

fsMIOspfRRDRouteConfigEntry OBJECT-TYPE
    SYNTAX      FsMIOspfRRDRouteConfigEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Information describing the configuration of single route entry.
         Row creation : An entry shall be created in one step by a 
          set-request with all necessary parameter values and  
          futOspfRRDRouteStatus set to createAndGo(4).
         Row deletion : An entry shall be deleted by setting
          the futOspfRRDRouteStatus of the specific entry to destroy(6).
         Index usage : the index is the Route Destination,Route Mask.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on Route Destination,Route Mask."
    INDEX       { fsMIStdOspfContextId, fsMIOspfRRDRouteDest, 
                  fsMIOspfRRDRouteMask }
    ::= { fsMIOspfRRDRouteConfigTable 1 }

FsMIOspfRRDRouteConfigEntry ::= SEQUENCE {
    fsMIOspfRRDRouteDest        IpAddress,
    fsMIOspfRRDRouteMask        IpAddress,
    fsMIOspfRRDRouteMetric      BigMetric,
    fsMIOspfRRDRouteMetricType  INTEGER,
    fsMIOspfRRDRouteTagType     INTEGER,
    fsMIOspfRRDRouteTag         INTEGER,
    fsMIOspfRRDRouteStatus      RowStatus
}

fsMIOspfRRDRouteDest OBJECT-TYPE
    SYNTAX      IpAddress
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Ip Address of the Destination route.
         ALCATEL NOTE:
           ACCESS:      NA          USAGE:       NA
           PERSIST:     YES         INSRVMOD:    NA
           RANGE:       NA          DEFVALUE:    NA
           UNITS:       NA          SPARSE:      NO
           DESCR:       NA"
    ::= { fsMIOspfRRDRouteConfigEntry 1 }

fsMIOspfRRDRouteMask OBJECT-TYPE
    SYNTAX      IpAddress
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Mask of the Destination route.
         ALCATEL NOTE:
           ACCESS:      NA          USAGE:       NA
           PERSIST:     YES         INSRVMOD:    NA
           RANGE:       NA          DEFVALUE:    NA
           UNITS:       NA          SPARSE:      NO
           DESCR:       NA"
    ::= { fsMIOspfRRDRouteConfigEntry 2 }

fsMIOspfRRDRouteMetric OBJECT-TYPE
    SYNTAX      BigMetric
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Metric value applied to the route before it is adv. into the
         OSPF Domain.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       OPT
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { 10 }
    ::= { fsMIOspfRRDRouteConfigEntry 3 }

fsMIOspfRRDRouteMetricType OBJECT-TYPE
    SYNTAX      INTEGER { asexttype1(1), asexttype2(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Metric Type applied to the route before it is adv. into the
         OSPF Domain
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       OPT
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { asexttype2 }
    ::= { fsMIOspfRRDRouteConfigEntry 4 }

fsMIOspfRRDRouteTagType OBJECT-TYPE
    SYNTAX      INTEGER { manual(1), automatic(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Tag Type MIB object describes whether Tags will be 
         automatically generated or will be manually configured. 
         If tags are manually configured , the fsMIOspfRRDRouteTag 
         MIB has to be set with the Tag value needed. 
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       OPT
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { manual }
    ::= { fsMIOspfRRDRouteConfigEntry 5 }

fsMIOspfRRDRouteTag OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Route Tag in case the user configures a Manual option for 
         the tags. 
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       OPT
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       NA          DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { 0 }
    ::= { fsMIOspfRRDRouteConfigEntry 6 }

fsMIOspfRRDRouteStatus OBJECT-TYPE
    SYNTAX      RowStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The variable dispalys the status of the entry.Setting it to
         'invalid' has the effect of rendering it inoperative.
         ALCATEL NOTE:
         ACCESS:      NA          USAGE:       MAND
         PERSIST:     YES         INSRVMOD:    YES
         RANGE:       1,2,4,5,6   DEFVALUE:    NA
         UNITS:       NA          SPARSE:      NO
         DESCR:       NA"
    ::= { fsMIOspfRRDRouteConfigEntry 7 }

fsMIOspfTestGroup OBJECT IDENTIFIER
    ::= { fsMIOspf 100 }

END -- end of module ARICENT-OSPFMI-MIB.
