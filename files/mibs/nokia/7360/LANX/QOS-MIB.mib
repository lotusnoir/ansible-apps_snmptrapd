    ::= { lanxQOSMIB 7 }

lanxQosPolicerEntry OBJECT-TYPE
    SYNTAX      LanxQosPolicerEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "QoS Policer Configuation entry
         Row creation : A QOS-POLICER entry shall be created in one step by a
         		      set-request with all necessary parameter values and
             		lanxQosPolicerRowStatus set to createAndGo(4).
         			One entry per port will be created by the System at the startup.
         Row deletion : A QOS-POLICER entry shall be deleted by setting
             		the lanxQosPolicerRowStatus of the specific entry to destroy(6).
         Index usage : the indexes are lanxQosPolicerPortIndex and lanxQosFlowIndex.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on lanxQosPolicerPortIndex, then on lanxQosFlowIndex."
    INDEX       { lanxQosPolicerPortIndex, lanxQosFlowIndex }
    ::= { lanxQosPolicerTable 1 }

LanxQosPolicerEntry ::= SEQUENCE {
    lanxQosPolicerPortIndex     INTEGER,
    lanxQosPolicerMeterIndex    INTEGER,
    lanxQosPolicerRowStatus     RowStatus
}

lanxQosPolicerPortIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..2147483647)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "LANX QoS port index
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     YES          INSRVMOD:    NA
             RANGE:       DEP           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NO
             DESCR:       DEP(RANGE) The range is the maximum number of physical ports in the system.
         	    			    This value can be got from issSystemNumberOfPorts in fsiss.mib."
    ::= { lanxQosPolicerEntry 1 }

lanxQosPolicerMeterIndex OBJECT-TYPE
    SYNTAX      INTEGER (0..64)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "An index to an already created Meter Entry. Same meter entry could be used 
         shared by multiple policers if needed.
            ALCATEL NOTE:
                ACCESS:      NA          USAGE:       MAND
                PERSIST:     YES         INSRVMOD:    YES
                RANGE:       1..64       DEFVALUE:    NA
                UNITS:       NA          SPARSE:      NO
                DESCR:       NA"
    ::= { lanxQosPolicerEntry 2 }

lanxQosPolicerRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Status of the Policer Entry. Only when the entry is made 'active' the corresponding 
         hardware resources are allocated.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       MAND
             PERSIST:     YES          INSRVMOD:    YES
             RANGE:       1..6         DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NO
             DESCR:       The Value 3 cannot be Set"
    ::= { lanxQosPolicerEntry 3 }

lanxQosMeterFreeIndex OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "A index to indicate the next free entry in the MeterTable. Before performing a 'set' operation, a get is performed on this object and this value is used in the set  routine.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     NO           INSRVMOD:    NA
             RANGE:       1..64        DEFVALUE:    3
             UNITS:       NA           SPARSE:      NA
             DESCR:       NA"
    ::= { lanxQOSMIB 8 }

lanxQosFlowFreeIndex OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The next free index to the flow table. Before any set operation, a get is performed on this object and this value is used in the set routine.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     NO           INSRVMOD:    NA
             RANGE:       1..64        DEFVALUE:    2
             UNITS:       NA           SPARSE:      NA
             DESCR:       NA"
    ::= { lanxQOSMIB 9 }

lanxQosDSCPtoPbitMapTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LanxQosDSCPtoPbitMapEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A table used to map DSCP values to Priority. Priority is a
         number in the range (0..(dot1dPortNumTrafficClasses-1)). There is only one
         DSCP to P-bits allignment table, common for all ports.
         ALCATEL NOTE:
             TABLESIZE:	 64
             DESCR:       NA"
    ::= { lanxQOSMIB 10 }

lanxQosDSCPtoPbitMapEntry OBJECT-TYPE
    SYNTAX      LanxQosDSCPtoPbitMapEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "DSCP to P-bit mapping.
         Row creation : All the Entries are created by the system at the startup.
         Row deletion : All the Entries are deleted by the system when it shutdowns.
         Index usage : the index is the lanxQosDSCPIndex.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on lanxQosDSCPIndex."
    INDEX       { lanxQosDSCPIndex }
    ::= { lanxQosDSCPtoPbitMapTable 1 }

LanxQosDSCPtoPbitMapEntry ::= SEQUENCE {
    lanxQosDSCPIndex        INTEGER,
    lanxQosDSCPPriority     INTEGER
}

lanxQosDSCPIndex OBJECT-TYPE
    SYNTAX      INTEGER (0..63)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The DSCP value in the IP frame. For all IP frames the 
         LANX looks up the DSCP field and alters the outgoing 
         P-bits as well as the internal priority based on the 
         DSCP_TABLE internal registers when DSCP to Pbit allignment 
         is enabled on the port.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     YES          INSRVMOD:    NA
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NO
             DESCR:       NA"
    ::= { lanxQosDSCPtoPbitMapEntry 1 }

lanxQosDSCPPriority OBJECT-TYPE
    SYNTAX      INTEGER (0..7)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Priority the received frame is mapped to.
         ALCATEL NOTE:
             ACCESS:      NA           		USAGE:       NA
             PERSIST:     YES          		INSRVMOD:    YES
             RANGE:       NA			DEFVALUE:    (lanxQosDSCPIndex/8)
             UNITS:       NA           		SPARSE:      NO
             DESCR:       NA"
    ::= { lanxQosDSCPtoPbitMapEntry 2 }

END -- end of module QOS-MIB.
