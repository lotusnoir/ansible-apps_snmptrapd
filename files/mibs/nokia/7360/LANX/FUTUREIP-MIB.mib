    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Indicate an inverse mask. An inverse mask sets
         a zero for each bit of the address that should be
         exactly matched and a one for each bit where     
         anything will match- the one bits are referred to
         as dont-care bits. 
         The mask is used to specify the range of IP 
         addresses.
         For Eg. Destination Net = 128.11.0.0
                 Net Mask = 0.0.255.255 specifies the range of
         routes starting from 128.11.0.0 to 121.11.255.255.
         For specifying all routes give 
                 Destination Net = 0.0.0.0
                 Net Mask = 255.255.255.255
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsVrfRrdControlEntry 2 }

fsVrfRrdControlSourceProto OBJECT-TYPE
    SYNTAX      INTEGER { any(0), other(1), local(2), netmgmt(3), 
                    icmp(4), egp(5), ggp(6), hello(7), rip(8), isIs(9), 
                    esIs(10), ciscoIgrp(11), bbnSpfIgp(12), ospf(13), 
                    bgp(14), idpr(15), ciscoEigrp(16) }
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Identifier of the source routing protocol.
         A value of zero will be interpreted as 'any',
         which may be used to configure interface based
         route redistribution.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsVrfRrdControlEntry 3 }

fsVrfRrdControlDestProto OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The identifier of the destination routing protocol.
         This field is a bit mask, a 1 indicates that the route
         has to be exported to the routing protocol corresponding
         to the bit-position.
         +===================================================+
         | ciscoEigrp |    idpr    |    bgp     |    ospf    |
         +===================================================+
         |     15     |     14     |    13      |     12     |
         +===================================================+
         |  bbnSpfIgp | ciscoIgrp  |    esIs    |    isIs    |
         +===================================================+
         |     11     |     10     |     9      |      8     |
         +===================================================+
         |     rip    |   hello    |    ggp     |    egp     |
         +===================================================+
         |      7     |      6     |     5      |      4     |
         +===================================================+
         |    icmp    |   netmgmt  |   local    |   other    |
         +===================================================+
         |      3     |      2     |     1      |      0     |
         +===================================================+
         ALCATEL NOTE:
         ACCESS:      NA           	USAGE:       OPT
         PERSIST:     YES          	INSRVMOD:    YES
         RANGE:       NA 			DEFVALUE:    NA
         UNITS:       NA           	SPARSE:      NO
         DESCR:       The value 0 or rip(bit-7) or ospf(bit-12) or bgp(bit-13) should be set"
    DEFVAL      { 0 }
    ::= { fsVrfRrdControlEntry 4 }

fsVrfRrdControlRouteExportFlag OBJECT-TYPE
    SYNTAX      INTEGER { permit(1), deny(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This flag specifies whether the route is allowed
         for export or denied.
         ALCATEL NOTE:
            ACCESS:      NA           	USAGE:       MAND
            PERSIST:     YES          	INSRVMOD:    YES
            RANGE:       NA             DEFVALUE:    NA
            UNITS:       NA           	SPARSE:      NO
            DESCR:       NA"
    ::= { fsVrfRrdControlEntry 5 }

fsVrfRrdControlRowStatus OBJECT-TYPE
