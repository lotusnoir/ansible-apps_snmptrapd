FsDot1qFutureLanxVlanConfigEntry ::= SEQUENCE {
    fsDot1qFutureVlanModeType               INTEGER,
    fsDot1qFutureVlanGratArpBCast           INTEGER,
    fsDot1qFutureUserToUserCommStatus       INTEGER,
    fsDot1qFutureVlanMACAddressLearning     INTEGER
}

fsDot1qFutureVlanModeType OBJECT-TYPE
    SYNTAX      INTEGER { residentialbridging(1), crossconnect(2), 
                    l2terminated(3), l2terminatedonnetworkports(4), 
                    vvlan(5), reserved(6), voicevlan(8), eponvlan(9) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This defines the type of the Vlan mode. By Default the vlan will be in 
         notclassified mode. All VLAN types excpet the corssconnect behave like 
         the residentialbridging VLAN. Following briefs on the various VLAN modes:
            L2 terminated on network ports - Used for VLANs for IP 
                                    forwarding (e.g. for RADIUS packet relay)
            V-VLAN -  This is used for communication between the LT and LANX and 
                      ideally has all the ASAM ports as member ports 
            reserved - This has the internal and external communicatieon VLAN
            notclassified - Default VLAN mode ( OBSOLETED)
            voicevlan - Enables Voice traffic through LANX in ISAM-V
         User has to change the VLAN mode from notclassified to any other valid 
         mode. Changing VLAN mode apart from notclassified is not allowed. 
            EPON VLAN -This is used for L2 communication between ASAM Ports and Subtending ports only.
         
         	ALCATEL NOTE:
         	      	ACCESS:      NA           USAGE:       MAND
              	PERSIST:     YES          INSRVMOD:    NO
              	RANGE:       NA           DEFVALUE:    NA
              	UNITS:       NA           SPARSE:      NO
              	DESCR:       NA"
    ::= { fsDot1qFutureLanxVlanConfigEntry 1 }

fsDot1qFutureVlanGratArpBCast OBJECT-TYPE
    SYNTAX      INTEGER { enable(1), disabled(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This defines wheather gratutious ARP broadcast is enabled or disabled  
         in a VLAN or not. 
         	ALCATEL NOTE:
         	      	ACCESS:      NA           USAGE:       NA
              	PERSIST:     YES          INSRVMOD:    YES
              	RANGE:       NA           DEFVALUE:    NA
              	UNITS:       NA           SPARSE:      NO
              	DESCR:       NA"
    DEFVAL      { disabled }
    ::= { fsDot1qFutureLanxVlanConfigEntry 2 }

fsDot1qFutureUserToUserCommStatus OBJECT-TYPE
    SYNTAX      INTEGER { enable(1), disable(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This object defines the the user-2-user communication status on a particular Vlan.
         This object is enabled by default for a voice vlan. The object status cannot be disabled 
         for a voice vlan. For all other kinds of vlan, this object is disabled by default. 
         At most there can be ten vlans including voice vlans with user-2-user communication 
         enabled.
         ALCATEL NOTE:
         	      	ACCESS:      NA           USAGE:       NA
               	PERSIST:     YES          INSRVMOD:    YES
               	RANGE:       NA           DEFVALUE:    NA
               	UNITS:       NA           SPARSE:      NO
               	DESCR:       NA"
    ::= { fsDot1qFutureLanxVlanConfigEntry 3 }

fsDot1qFutureVlanMACAddressLearning OBJECT-TYPE
    SYNTAX      INTEGER { enable(1), disable(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This MIB object determines the status of the MAC address learning feature per VLAN.
         If it is disable, then MAC learning will be disabled otherwise will be enabled. 	
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     YES          INSRVMOD:    YES
         RANGE:       NA           DEFVALUE:    NA
         UNITS:       NA           SPARSE:      NA
         DESCR:       NA"
    DEFVAL      { enable }
    ::= { fsDot1qFutureLanxVlanConfigEntry 4 }

