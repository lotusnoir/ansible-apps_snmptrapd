fsMIStdOspfExtLsdbTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMIStdOspfExtLsdbEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The OSPF Process's Links State Database.
         ALCATEL NOTE:
         	TABLE SIZE:  200                      
         	DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Section 12  Link  State  Adver-
--          tisements"
    ::= { fsMIStdOspf 11 }

fsMIStdOspfExtLsdbEntry OBJECT-TYPE
    SYNTAX      FsMIStdOspfExtLsdbEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A single Link State Advertisement.
         Row creation : An entry will be created when LSAs are received.
         Row deletion : An entry can not be deleted.
         Index usage : the index is the Ospf Context Id, Link State Type,Link State Id,Link State Router Id.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on Ospf Context Id then on Link State Type,Link State Id,Link State Router Id."
    INDEX       { fsMIStdOspfContextId, fsMIStdOspfExtLsdbType, 
                  fsMIStdOspfExtLsdbLsid, fsMIStdOspfExtLsdbRouterId }
    ::= { fsMIStdOspfExtLsdbTable 1 }

FsMIStdOspfExtLsdbEntry ::= SEQUENCE {
    fsMIStdOspfExtLsdbType              INTEGER,
    fsMIStdOspfExtLsdbLsid              IpAddress,
    fsMIStdOspfExtLsdbRouterId          RouterID,
    fsMIStdOspfExtLsdbSequence          INTEGER,
    fsMIStdOspfExtLsdbAge               INTEGER,
    fsMIStdOspfExtLsdbChecksum          INTEGER,
    fsMIStdOspfExtLsdbAdvertisement     OCTET STRING
}

fsMIStdOspfExtLsdbType OBJECT-TYPE
    SYNTAX      INTEGER { asExternalLink(5) }
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The type  of  the  link  state  advertisement.
         Each  link state type has a separate advertise-
         ment format.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA                          
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Appendix A.4.1 The  Link  State
--          Advertisement header"
    ::= { fsMIStdOspfExtLsdbEntry 1 }

fsMIStdOspfExtLsdbLsid OBJECT-TYPE
    SYNTAX      IpAddress
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Link State ID is an LS Type Specific field
         containing either a Router ID or an IP Address;
         it identifies the piece of the  routing  domain
         that is being described by the advertisement.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA                       
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Section 12.1.4 Link State ID"
    ::= { fsMIStdOspfExtLsdbEntry 2 }

fsMIStdOspfExtLsdbRouterId OBJECT-TYPE
    SYNTAX      RouterID
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The 32 bit number that uniquely identifies the
         originating router in the Autonomous System.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA                         
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Appendix C.1 Global parameters"
    ::= { fsMIStdOspfExtLsdbEntry 3 }

fsMIStdOspfExtLsdbSequence OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The sequence number field is a  signed  32-bit
         integer.   It  is used to detect old and dupli-
         cate link state advertisements.  The  space  of
         sequence  numbers  is  linearly  ordered.   The
         larger the sequence number the more recent  the
         advertisement.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version  2,  Section  12.1.6  LS  sequence
--          number"
    ::= { fsMIStdOspfExtLsdbEntry 4 }

fsMIStdOspfExtLsdbAge OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This field is the age of the link state adver-
         tisement in seconds.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       0..3600      DEFVALUE:    0
          UNITS:       SECONDS      SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Section 12.1.1 LS age"
    ::= { fsMIStdOspfExtLsdbEntry 5 }

fsMIStdOspfExtLsdbChecksum OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This field is the  checksum  of  the  complete
         contents  of  the  advertisement, excepting the
         age field.  The age field is excepted  so  that
         an   advertisement's  age  can  be  incremented
         without updating the  checksum.   The  checksum
         used  is  the same that is used for ISO connec-
         tionless datagrams; it is commonly referred  to
         as the Fletcher checksum.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    0
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Section 12.1.7 LS checksum"
    ::= { fsMIStdOspfExtLsdbEntry 6 }

fsMIStdOspfExtLsdbAdvertisement OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(36))
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The entire Link State Advertisement, including
         its header.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    '\0'
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
--     REFERENCE   
--         "OSPF Version 2, Section 12  Link  State  Adver-
--          tisements"
    ::= { fsMIStdOspfExtLsdbEntry 7 }

