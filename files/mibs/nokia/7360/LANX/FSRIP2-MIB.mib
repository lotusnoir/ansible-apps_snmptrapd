fsRipRouteMapSubIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..2147483647)
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The entry sub-index. Each route map entry may have rules for multiple IP addresses.
         This sub index identifies the rule for eacgh IP address.
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     YES          INSRVMOD:    NA
         RANGE:       1..128       DEFVALUE:    NA
         UNITS:       NA	      SPARSE:      NO
         DESCR:       NA"
    ::= { fsRipRouteMapEntry 2 }

fsRipRouteMapIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The IpAddress of the subnet that can be allowed or disallowed to be propagated by RIP over the interface
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       MAND
             PERSIST:     YES          INSRVMOD:    YES
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA	      SPARSE:      NO
             DESCR:       NA"
    ::= { fsRipRouteMapEntry 3 }

fsRipRouteMapDirection OBJECT-TYPE
    SYNTAX      INTEGER { transmit(1), receive(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This MIB object specifes the direction in which the RIP routes are advertised over an IP interface. Only direction TRANSMIT is supported. Direction receive is not supported.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       OPT
             PERSIST:     YES          INSRVMOD:    YES
             RANGE:       1            DEFVALUE:    NA
             UNITS:       NA	      SPARSE:      NO
             DESCR:       NA"
    DEFVAL      { transmit }
    ::= { fsRipRouteMapEntry 4 }

fsRipRouteMapAction OBJECT-TYPE
    SYNTAX      INTEGER { permit(1), deny(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "This MIB object specifes the action to be allowed.
         Permit - The route will be advertised by RIP over the interface in transmit direction. In direction receive this route will be accepted by RIP for advertisement.
         Deny - The route will not be advertised by RIP over the interface in transmit direction. In the direction receive this route will not be accepted by RIP for advertisement 
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       OPT
         PERSIST:     YES          INSRVMOD:    YES
         RANGE:       NA           DEFVALUE:    NA
         UNITS:       NA	      SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { permit }
    ::= { fsRipRouteMapEntry 5 }

fsRipRouteMapSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
