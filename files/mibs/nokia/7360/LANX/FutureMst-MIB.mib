fsMstErrTrapType OBJECT-TYPE
    SYNTAX      INTEGER { none(0), memfail(1), bufffail(2) }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The error event like
         none - none of the below values
         memfail - memory allocation failure
         bufffail - buffer allocation failure
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     NO           INSRVMOD:    NA
         RANGE:       NA           DEFVALUE:    NA
         UNITS:       NA           SPARSE:      NA
         DESCR:       NA"
    ::= { dot1sFsMstTrapsControl 3 }

fsMstPortTrapNotificationTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMstPortNotificationEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "This table is used to store the notification information
         occured in each of the port for protocol migration and 
         invalid packet received
          ALCATEL NOTE:
              TABLESIZE: 	DEP
         	  	DESCR: 	DEP(TABLESIZE): The size of table is obtained from Maximum number of physical interfaces
         			available in the system (obtained from issSystemNumberOfPorts in fsiss.mib)"
    ::= { dot1sFsMstTrapsControl 4 }

fsMstPortNotificationEntry OBJECT-TYPE
    SYNTAX      FsMstPortNotificationEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        " This entry is used to store the notification information
         Row creation : All entries will be created when a MSTP module is
         	   		enabled.
         Row deletion : All entries will be deleted when a MSTP module is
                     	   disabled.
         Index usage : the index is fsMstPortTrapIndex.
           Note that entries will be presented to the SNMP manager in full
           lexicographical order :  sorted on fsMstPortTrapIndex."
    INDEX       { fsMstPortTrapIndex }
    ::= { fsMstPortTrapNotificationTable 1 }

FsMstPortNotificationEntry ::= SEQUENCE {
    fsMstPortTrapIndex      INTEGER,
    fsMstPortMigrationType  INTEGER,
    fsMstPktErrType         INTEGER,
    fsMstPktErrVal          INTEGER
}

fsMstPortTrapIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..2147483647)
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "A unique value, greater than zero, indicating the Port number.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       DEP        DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NO
            DESCR:       DEP(RANGE):  The rnage is the maximum number of physical interfaces available in the system. 
         	Maximum number of physical interfaces is obtained from the object issSystemNumberOfPorts in fsiss.mib"
    ::= { fsMstPortNotificationEntry 1 }

fsMstPortMigrationType OBJECT-TYPE
    SYNTAX      INTEGER { sendstp(0), sendrstp(1) }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Port Protocol migration type occured in the port
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstPortNotificationEntry 2 }

fsMstPktErrType OBJECT-TYPE
    SYNTAX      INTEGER { protocolIdErr(0), invalidBpdu(1), 
                    configLengthErr(2), tcnLengthErr(3), 
                    rstpLengthErr(4), maxAgeErr(5), fwdDelayErr(6), 
                    helloTimeErr(7), mstpLengthErr(8) }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Type of invalid packet received in each of the port 
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstPortNotificationEntry 3 }

fsMstPktErrVal OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Packet error value corresponding to the above type 
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstPortNotificationEntry 4 }

fsMstTrapCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times MSTP Module has generated a TRAP.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sFsMstTrapsControl 5 }

dot1sFutureMstTraps OBJECT IDENTIFIER
