    ::= { dot1sFutureMst 35 }

fsMstCistTopChanges OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times that there have been atleast
         one non-zero TcWhile Timer on this Bridge for Common
         Spanning Tree context.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sFutureMst 36 }

fsMstCistNewRootBridgeCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times this Bridge has detected a Root
         Bridge change for Common Spanning Tree context.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    0
           UNITS:       NA           SPARSE:      NA
           DESCR:       NA"
    ::= { dot1sFutureMst 37 }

fsMstMstiBridgeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMstMstiBridgeEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Table containing Bridge Information specific to Spanning 
         Tree Instance.
          ALCATEL NOTE:
              TABLESIZE: 	16
         	  	DESCR: 	NA"
    ::= { dot1sFutureMst 38 }

fsMstMstiBridgeEntry OBJECT-TYPE
    SYNTAX      FsMstMstiBridgeEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Entry indicating the Bridge Information.
         Row creation : A fsMstMstiBridgeEntry entry shall be created in one 
         		        step by a set-request with fsMstMstiBridgeRowStatus 
         			set to createAndGo(4) or createAndWait(5).
         Row deletion : A fsMstMstiBridgeEntry entry shall be deleted by setting
             		the fsMstMstiBridgeRowStatus of the specific entry
         			to destroy(6).
         Index usage : the index is fsMstMstiInstanceIndex.
           Note that entries will be presented to the SNMP manager in full
           lexicographical order :  sorted on fsMstMstiInstanceIndex."
    INDEX       { fsMstMstiInstanceIndex }
    ::= { fsMstMstiBridgeTable 1 }

FsMstMstiBridgeEntry ::= SEQUENCE {
    fsMstMstiInstanceIndex                  INTEGER,
    fsMstMstiBridgeRegionalRoot             BridgeId,
    fsMstMstiBridgePriority                 INTEGER,
    fsMstMstiRootCost                       INTEGER,
    fsMstMstiRootPort                       INTEGER,
    fsMstMstiTimeSinceTopologyChange        TimeTicks,
    fsMstMstiTopChanges                     Counter,
    fsMstMstiNewRootBridgeCount             Counter,
    fsMstMstiBridgeRoleSelectionSemState    INTEGER,
    fsMstInstanceUpCount                    Counter,
    fsMstInstanceDownCount                  Counter,
    fsMstOldDesignatedRoot                  BridgeId,
    fsMstMstiBridgeRowStatus                RowStatus
}

fsMstMstiInstanceIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..16)
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Spanning Tree Instance to which the information belongs.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     YES          INSRVMOD:    NA
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NO
             DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 1 }

fsMstMstiBridgeRegionalRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "MSTI Regional Root Identifier value for the Instance. This value 
         is used as the MSTI Regional Root Identifier parameter in all
         Configuration Bridge PDUs originated by this node
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 2 }

fsMstMstiBridgePriority OBJECT-TYPE
    SYNTAX      INTEGER (0..61440)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The writable portion of the MSTI Bridge Identifier.
         comprising of the first two octets.
         The values that are set for Bridge Priority must be 
         in steps of 4096.
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       OPT
              PERSIST:     YES          INSRVMOD:    YES
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       NA"
    DEFVAL      { 32768 }
    ::= { fsMstMstiBridgeEntry 3 }

fsMstMstiRootCost OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Cost of the path to the MSTI Regional Root as seen 
         by this bridge.
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 4 }

fsMstMstiRootPort OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Port Number of the Port which offers the lowest    
         path cost from this bridge to the MSTI Region Root Bridge.
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:      DEP        DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       DEP(RANGE): This object will return 0 when it  is acting as a Root Bridge.
         		In all other cases it will return the Root Port ID, having a value in 
         		the range 1 to maximum number of physical interfaces available in the system. 
         		Maximum number of physical interfaces is obtained from the object issSystemNumberOfPorts in fsiss.mib"
    ::= { fsMstMstiBridgeEntry 5 }

fsMstMstiTimeSinceTopologyChange OBJECT-TYPE
    SYNTAX      TimeTicks
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The time (in hundredths of a second) since the
         TcWhile Timer for any port in this Bridge was 
         non-zero for this spanning tree instance.
         ALCATEL NOTE:
            ACCESS:      NA           		USAGE:       NA
            PERSIST:     NO           		INSRVMOD:    NA
            RANGE:       NA           		DEFVALUE:    0
            UNITS:       1/100th of Second      SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 6 }

fsMstMstiTopChanges OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times that there have been atleast
         one non-zero TcWhile Timer on this Bridge for this
         spanning tree instance.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 7 }

fsMstMstiNewRootBridgeCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times this Bridge has detected a Root
         Bridge change for this spanning tree instance.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    0
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 8 }

fsMstMstiBridgeRoleSelectionSemState OBJECT-TYPE
    SYNTAX      INTEGER { initbridge(0), roleselection(1) }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Current state of the Port Role Selection State Machine          
         for this spanning tree instance for this bridge.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 9 }

fsMstInstanceUpCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times a new spanning tree instance has
         been created.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 10 }

fsMstInstanceDownCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times a spanning tree instance has
         been deleted.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 11 }

fsMstOldDesignatedRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The bridge identifier of the old root of the spanning
         tree instance as determined by the Spanning Tree Protocol
         as executed by this node. 
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     NO           INSRVMOD:    NA
         RANGE:       NA           DEFVALUE:    NA
         UNITS:       NA           SPARSE:      NO
         DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 12 }

fsMstMstiBridgeRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Specifies the Row Status of the MSTI Bridge entry. 
         To create a new entry this has to be created with the
         CREAT-AND-WAIT or with CREATE-AND-GO. To delete the entry from this
         table, the row status should be set to Destroy.
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       MAND
         PERSIST:     YES          INSRVMOD:    YES
         RANGE:       1,4,5,6      DEFVALUE:    NA
         UNITS:       NA           SPARSE:      NO
         DESCR:       NA"
    ::= { fsMstMstiBridgeEntry 13 }

fsMstVlanInstanceMappingTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMstVlanInstanceMappingEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "This table contains one entry for each instance of MSTP.
         ALCATEL NOTE:
             TABLESIZE: 	16
         	  	DESCR: 	NA"
    ::= { dot1sFutureMst 39 }

fsMstVlanInstanceMappingEntry OBJECT-TYPE
    SYNTAX      FsMstVlanInstanceMappingEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A conceptual row containing the status of the MSTP instance.
         Row creation : A fsMstVlanInstanceMappingEntry shall be created in one 
         		        step by a set-request with fsMstMstiBridgeRowStatus 
         			set to createAndGo(4) or createAndWait(5).
         Row deletion : A fsMstVlanInstanceMappingEntry entry shall be deleted 
         			by setting the fsMstMstiBridgeRowStatus of the specific 
         			entry to destroy(6).
         Index usage : the index is fsMstInstanceIndex.
           Note that entries will be presented to the SNMP manager in full
           lexicographical order :  sorted on fsMstInstanceIndex."
    INDEX       { fsMstInstanceIndex }
    ::= { fsMstVlanInstanceMappingTable 1 }

FsMstVlanInstanceMappingEntry ::= SEQUENCE {
    fsMstInstanceIndex          INTEGER,
    fsMstSetVlanList            OCTET STRING,
    fsMstResetVlanList          OCTET STRING,
    fsMstInstanceVlanMapped     OCTET STRING
}

fsMstInstanceIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..16)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "An arbitrary integer within the range from 1 to the value of
         Max Instance Number that uniquely identifies an instance.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstVlanInstanceMappingEntry 1 }

fsMstSetVlanList OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..512))
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "A string of octets containing one bit per VLAN for
         the VlanIndex Ranges from 2 through 4094. The
          first octet corresponds to VLANs with VlanIndex values
          1 through 8; the second octet to VLANs 9 through
          16 etc.  The most significant bit of each octet
          corresponds to the lowest VlanIndex value in that octet.
          The set vlans configured by management to map for this 
          Instance. A vlans may not be mapped to this instance if 
          it is already a mapped to this Instance.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     YES          INSRVMOD:    YES
           RANGE:       NA           DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       Valid VlanIndex Range 2 through 4094"
    ::= { fsMstVlanInstanceMappingEntry 2 }

fsMstResetVlanList OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..512))
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "A string of octets containing one bit per VLAN for
         the VlanIndex Ranges from 2 through 4094. The
          first octet corresponds to VLANs with VlanIndex values
          1 through 8; the second octet to VLANs 9 through
          16 etc.  The most significant bit of each octet
          corresponds to the lowest VlanIndex value in that octet.
          The set vlans configured by management to unmap from this 
          Instance. A vlans may not be unmapped from this instance if 
          it is not mapped to this Instance.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     YES          INSRVMOD:    YES
           RANGE:       NA           DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       Valid VlanIndex Range 2 through 4094"
    ::= { fsMstVlanInstanceMappingEntry 3 }

fsMstInstanceVlanMapped OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..512))
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "A string of octets containing one bit per VLAN for
         the VlanIndex Ranges from 2 through 4094. The
         first octet corresponds to VLANs with VlanIndex values
         1 through 8; the second octet to VLANs 9 through
         16 etc.  The most significant bit of each octet
         corresponds to the lowest VlanIndex value in that octet.
         
         For each VLAN that is mapped to this MSTP instance,
         the bit corresponding to that VLAN is set to '1'.
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     NO           INSRVMOD:    NA
         RANGE:       NA           DEFVALUE:    NA
         UNITS:       NA           SPARSE:      NO
         DESCR:       Valid VlanIndex Range 2 through 4094"
    ::= { fsMstVlanInstanceMappingEntry 4 }

fsMstCistPortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsMstCistPortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "This table contains Common Spanning Tree Port
         Information.
         ALCATEL NOTE:
          TABLESIZE: DEP 
         DESCR: DEP (TABLESIZE)  The range of this table is the maximum number of ports in the
         		  system. This value can be got from the object issSystemNumberOfPorts
         		  in fsiss.mib.	"
    ::= { dot1sFutureMst 40 }

fsMstCistPortEntry OBJECT-TYPE
    SYNTAX      FsMstCistPortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A list of information maintained by every port for 
         Common Spanning tree.
         Row creation : All entries will be created when a MSTP module is
           		enabled.
         Row deletion : All entries will be deleted when a MSTP module is
                 	   disabled.
         Index usage : the index is fsMstCistPort.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on fsMstCistPort."
    INDEX       { fsMstCistPort }
    ::= { fsMstCistPortTable 1 }

FsMstCistPortEntry ::= SEQUENCE {
    fsMstCistPort                           INTEGER,
    fsMstCistPortPathCost                   INTEGER,
    fsMstCistPortPriority                   INTEGER,
    fsMstCistPortDesignatedRoot             BridgeId,
    fsMstCistPortDesignatedBridge           BridgeId,
    fsMstCistPortDesignatedPort             OCTET STRING,
    fsMstCistPortAdminP2P                   INTEGER,
    fsMstCistPortOperP2P                    TruthValue,
    fsMstCistPortAdminEdgeStatus            TruthValue,
    fsMstCistPortOperEdgeStatus             TruthValue,
    fsMstCistPortProtocolMigration          TruthValue,
    fsMstCistPortState                      INTEGER,
    fsMstCistForcePortState                 INTEGER,
    fsMstCistPortForwardTransitions         Counter,
    fsMstCistPortReceivedBPDUs              Counter,
    fsMstCistPortTransmittedBPDUs           Counter,
    fsMstCistPortInvalidBPDUsRcvd           Counter,
    fsMstCistPortTransmitSemState           INTEGER,
    fsMstCistPortReceiveSemState            INTEGER,
    fsMstCistPortProtMigrationSemState      INTEGER,
    fsMstCistProtocolMigrationCount         Counter,
    fsMstCistPortDesignatedCost             INTEGER,
    fsMstCistPortRegionalRoot               BridgeId,
    fsMstCistPortRegionalPathCost           INTEGER,
    fsMstCistSelectedPortRole               INTEGER,
    fsMstCistCurrentPortRole                INTEGER,
    fsMstCistPortInfoSemState               INTEGER,
    fsMstCistPortRoleTransitionSemState     INTEGER,
    fsMstCistPortStateTransitionSemState    INTEGER,
    fsMstCistPortTopologyChangeSemState     INTEGER,
    fsMstCistPortHelloTime                  Timeout
}

fsMstCistPort OBJECT-TYPE
    SYNTAX      INTEGER (1..24)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Port number of the port for which this entry contains   
         spanning tree information.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstCistPortEntry 1 }

fsMstCistPortPathCost OBJECT-TYPE
    SYNTAX      INTEGER (1..200000000)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The contribution of this port to the path cost of
         paths towards the CIST Root which include this port.
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     YES          INSRVMOD:    YES
         RANGE:       NA           DEFVALUE:    NA
         UNITS:       NA           SPARSE:      NO
         DESCR:       NA"
    ::= { fsMstCistPortEntry 2 }

fsMstCistPortPriority OBJECT-TYPE
    SYNTAX      INTEGER (0..240)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The four most significant bits of the Port Identifier 
         of the Spanning Tree instance can be modified by setting 
         the CistPortPriority value. The values that are set for Port 
         Priority must be in steps of 16.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     YES          INSRVMOD:    YES
           RANGE:       NA           DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    DEFVAL      { 128 }
    ::= { fsMstCistPortEntry 3 }

fsMstCistPortDesignatedRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The unique Bridge Identifier of the bridge recorded as the  
         CIST Root in the configuration BPDUs transmitted.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstCistPortEntry 4 }

fsMstCistPortDesignatedBridge OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The unique Bridge Identifier of the bridge which this port  
         considers to be the Designated Bridge for the port's segment.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstCistPortEntry 5 }

fsMstCistPortDesignatedPort OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(2))
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Port identifier of the port on the Designated Bridge    
         for this port's segment.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstCistPortEntry 6 }

fsMstCistPortAdminP2P OBJECT-TYPE
    SYNTAX      INTEGER { forceTrue(0), forceFalse(1), auto(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The administrative point-to-point status of the LAN segment
         attached to this port.  A value of forceTrue(0) indicates that
         this port should always be treated as if it is connected to
         a point-to-point link.  A value of forceFalse(1) indicates
         that this port should be treated as having a shared media
         connection.  A value of auto(2) indicates that this port is
         considered to have a point-to-point link if it is an Aggregator
         and all of its members are aggregatable, or if the MAC entity
         is configured for full duplex operation, either through
         auto-negotiation or by management means.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     YES          INSRVMOD:    YES
           RANGE:       NA           DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    DEFVAL      { auto }
    ::= { fsMstCistPortEntry 7 }

fsMstCistPortOperP2P OBJECT-TYPE
    SYNTAX      TruthValue
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The operational point-to-point status of the LAN segment
         attached to this port.  It indicates whether a port is
         considered to have a point-to-point connection or not.
         The value is determined by management or by auto-detection,
         as described in the fsMstCistPortAdminP2P object.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       1,2          DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    ::= { fsMstCistPortEntry 8 }

fsMstCistPortAdminEdgeStatus OBJECT-TYPE
    SYNTAX      TruthValue
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The administrative value of the Edge Port parameter.  A
         value of TRUE(1) indicates that this port should be
         assumed as an edge-port and a value of FALSE(2) indicates
         that this port should be assumed as a non-edge-port.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     YES          INSRVMOD:    YES
           RANGE:       1,2          DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    DEFVAL      { false }
    ::= { fsMstCistPortEntry 9 }

fsMstCistPortOperEdgeStatus OBJECT-TYPE
    SYNTAX      TruthValue
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The operational value of the Edge Port parameter.  The
         object is initialized to the value of
         fsMstCistPortAdminEdgeStatus and is set FALSE on reception 
         of a BPDU.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       1,2          DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    ::= { fsMstCistPortEntry 10 }

fsMstCistPortProtocolMigration OBJECT-TYPE
    SYNTAX      TruthValue
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The value of state machine variable mcheck for the port.         
         When operating in RSTP/MSTP (version >= 2) mode, writing 
         TRUE(1) to this object forces this port to transmit MSTP 
         BPDUs.
         Any other operation on this object has no effect and
         it always returns FALSE(2) when read.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    YES
          RANGE:       1,2          DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstCistPortEntry 11 }

fsMstCistPortState OBJECT-TYPE
    SYNTAX      INTEGER { disabled(1), discarding(2), learning(4), 
                    forwarding(5) }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Current state of the Port as defined by the Common    
         spanning tree protocol.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { fsMstCistPortEntry 12 }

fsMstCistForcePortState OBJECT-TYPE
    SYNTAX      INTEGER { disabled(0), enabled(1) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Current state of the Port which can be changed to either 
         Disabled or Enabled for the base spanning tree   
         instance.
