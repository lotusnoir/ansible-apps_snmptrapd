fsIgsEnhancedPerformance OBJECT IDENTIFIER
    ::= { fsigs 5 }

fsIgsVlanEnhancedPerformanceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FsIgsVlanEnhancedPerformanceEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "This table contains configuration to specific to the EnhancedPerformance IGMP snooping feature.
         ALCATEL NOTE:
         TABLESIZE: NA
         DESCR: No seperate tables are maintained for enhanced performance vlan."
    ::= { fsIgsEnhancedPerformance 1 }

fsIgsVlanEnhancedPerformanceEntry OBJECT-TYPE
    SYNTAX      FsIgsVlanEnhancedPerformanceEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Contains Vlan Id , Vlan Filter Status.
         Row creation : Entry is created when fsIgsVlanMode is set to enhancedperf(1).
         Row deletion : Entry is deleted when fsIgsVlanMode is set to proxyreporting(2).
         Index usage : The index is the fsIgsVlanEnhancedPerformanceVlanId.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on fsIgsVlanEnhancedPerformanceVlanId."
    INDEX       { fsIgsVlanEnhancedPerformanceVlanId }
    ::= { fsIgsVlanEnhancedPerformanceTable 1 }

FsIgsVlanEnhancedPerformanceEntry ::= SEQUENCE {
    fsIgsVlanEnhancedPerformanceVlanId          INTEGER,
    fsIgsVlanEnhancedPerformancePktFwdState     INTEGER
}

fsIgsVlanEnhancedPerformanceVlanId OBJECT-TYPE
    SYNTAX      INTEGER (1..4094)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        " Vlan Id for which IGMP snooping feature with the EnhancedPerformance is enabled.
         ALCATEL NOTE:
         ACCESS:      NA            USAGE:       NA
         PERSIST:     NO            INSRVMOD:    NA
         RANGE:       NA            DEFVALUE:    NA
         UNITS:       NA            SPARSE:      NO
         DESCR:       NA"
    ::= { fsIgsVlanEnhancedPerformanceEntry 1 }

fsIgsVlanEnhancedPerformancePktFwdState OBJECT-TYPE
    SYNTAX      INTEGER { enabled(1), disabled(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        " If it configured as enabled, the coloured the report messages will be forward in the upstream.
         If it is configured as disabled the coloured report messages will not be forwarded in the upstream.
         This object can be set if IGMP mode of the VLAN is enhanced performance mode.
          ALCATEL NOTE:
          ACCESS:      NA            USAGE:       OPT
         PERSIST:     YES           INSRVMOD:    YES
         RANGE:       NA            DEFVALUE:    NA
         UNITS:       NA            SPARSE:      NO
         DESCR:       NA"
    DEFVAL      { disabled }
    ::= { fsIgsVlanEnhancedPerformanceEntry 2 }

