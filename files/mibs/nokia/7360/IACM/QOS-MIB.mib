    ::=  {qosLimQueueStatsP15mTable 1 }


QosLimQueueStatsP15mEntry ::= SEQUENCE
   {
      qosLimQueueStatsP15mLineLinkIndex               InterfaceIndex,
      qosLimQueueStatsP15mLineQueueIndex           INTEGER,
      qosLimQueueStatsP15mIntervalNumber           INTEGER,
