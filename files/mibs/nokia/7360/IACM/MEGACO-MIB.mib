   -- *****************************************************************
   -- accessTerminationTable
   -- *****************************************************************
   accessTerminationTable OBJECT-TYPE
      SYNTAX SEQUENCE OF AccessTerminationEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION 
            "The accessTerminationTable contains the list of configured user ports.
            An entry MUST be configured for a user port when the voice service is subscribed for this user  port 
            i.e. the voice service is enabled at the MGC for this user port.
            An entry MAY be configured for a user port  without voice service subscription 
            i.e. the voice service for this port is disabled at the MGC. 
            For this latter configuration option, the HW and Media Gateway behaviour shall depend on 
            the setting of the administrative state for this port as described for the accTermVoiceServAtMgc object."

   ::= {accessTerminationConfig 1}
   
   -- *****************************************************************
   -- accessTerminationEntry
   -- *****************************************************************
   accessTerminationEntry OBJECT-TYPE
      SYNTAX AccessTerminationEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION "Entry holding configuration information for a configured user port."
      INDEX {accTerminationHldrEquipmentId, accTerminationUserPortId}
   ::= {accessTerminationTable 1}
   
   AccessTerminationEntry ::= SEQUENCE
   {
      accTerminationHldrEquipmentId AccessEquipmentId, -- na
      accTerminationUserPortId TerminationUserPortId, -- na
      accTerminationRowStatus RowStatus, -- rw
      accTerminationtId AccessTerminationId, -- rw
      accTerminationAssocMedGwyId MediaGatewayId, -- rw
      accTerminationOperStatus INTEGER, -- ro
      accTerminationAdminStatus INTEGER, -- rw
      accTerminationLineCharact INTEGER, -- rw
      accTerminationUserPortType INTEGER, -- rw
      accTerminationPacketSwitchType INTEGER, -- rw
      accTerminationL1ActivateType  INTEGER, --rw  
      accTerminationImpedance       INTEGER, --rw          
--      accTerminationGain       INTEGER, 
      accTerminationIdStr  DisplayString, -- ro
      accTerminationLockType		INTEGER, --wo
      accTerminationLockDelay	INTEGER, --wo
      accTerminationVoiceDscp INTEGER, -- rw
      accTerminationVoiceDot1P INTEGER, -- rw
      accTerminationETSIClipDataMode INTEGER, -- rw
      accTerminationMeteringPulseType INTEGER, -- rw
      accTerminationDirectoryNumber DisplayString, --rw
      accTerminationCallStatus  INTEGER, -- ro
      accTerminationOpStatDownReason  INTEGER, -- ro
      accTerminationTestStatus  INTEGER, -- ro
      accTerminationChannelStatus  INTEGER, -- ro
      accTerminationRxGain      INTEGER, --rw                
