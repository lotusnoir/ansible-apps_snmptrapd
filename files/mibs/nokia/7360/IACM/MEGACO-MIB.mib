-- *********************************************************************
-- MEGACO-MIB
-- Media Gateway Control MIB
-- *********************************************************************
-- ===============================================================================

MEGACO-MIB DEFINITIONS ::= BEGIN

   IMPORTS
      OBJECT-TYPE
      FROM RFC-1212

      ifIndex,
      InterfaceIndex
                FROM ITF-MIB


      IpAddress, Counter, Gauge
      FROM RFC1155-SMI

      DisplayString, RowStatus, TestAndIncr, TruthValue
      FROM SNMPv2-TC

      asam, alcatel
      FROM SYSTEM-MIB;

   -- *****************************************************************
   -- OID for the MIB
   -- *****************************************************************
   megacoMib OBJECT IDENTIFIER ::= {asam 55}

   -- *****************************************************************
   -- Object Groups
   -- *****************************************************************
   mediaGatewayConfig OBJECT IDENTIFIER      ::= {megacoMib 1}
   accessEquipmentConfig OBJECT IDENTIFIER   ::= {megacoMib 2}
   accessTerminationConfig OBJECT IDENTIFIER ::= {megacoMib 3}
   ivpsConfig OBJECT IDENTIFIER              ::= {megacoMib 4}
   signallingGatewayInterfaceConfig OBJECT IDENTIFIER      ::= {megacoMib 5}   
   ltLineTest OBJECT IDENTIFIER              ::= {megacoMib 6}
   -- *****************************************************************
   -- Textual conventions for the Media Gateway MIB
   -- *****************************************************************
   -- MediaGatewayId ::= TEXTUAL-CONVENTION
   -- DISPLAY-HINT "d"
   -- DESCRIPTION "Internal media gateway Id that can be used to identify
   -- any media gateway uniquely within this IVPS."
   -- SYNTAX INTEGER (1..2147483647)
   MediaGatewayId ::= INTEGER (1..2147483647)

   -- AccessEquipmentId ::= TEXTUAL-CONVENTION
   -- DISPLAY-HINT "d"
   -- DESCRIPTION "Internal ISAM-V Id that can be used to identify
   -- any access NE uniquely within this IVPS."
   -- SYNTAX INTEGER (1..2147483647)
   AccessEquipmentId ::= INTEGER (1..255)
   
   -- AccessBoardId ::= TEXTUAL-CONVENTION
   -- DISPLAY-HINT "d"
   -- DESCRIPTION "Slot identifier composed of the rack number (4 bits),
   -- subrack number (4 bits) and slot number (8 bits)."
   -- SYNTAX INTEGER (1..65535)
   AccessBoardId ::= INTEGER (1..65535)
   
   -- TerminationUserPortId ::= TEXTUAL-CONVENTION
   -- DISPLAY-HINT "d"
   -- DESCRIPTION "User Port identifier composed of the rack number
   -- (4 bits), subrack number (4 bits), slot number
   -- (8 bits) and port number (16 bits)."
   -- SYNTAX INTEGER (1..2147483647)
   TerminationUserPortId ::= INTEGER (1..2147483647)
   
   -- AccessTerminationId ::= TEXTUAL-CONVENTION
   -- DISPLAY-HINT "d"
   -- DESCRIPTION "Possible termination Id that can be used to identify
   -- any access termination uniquely."
   -- SYNTAX INTEGER (1..2147483647)
   AccessTerminationId ::= INTEGER (0..2147483647)

   -- SignallingGatewayInterfaceId ::= TEXTUAL-CONVENTION
   -- DISPLAY-HINT "d"
   -- DESCRIPTION "Internal signalling gateway Id that can be used to identify
   -- any signalling gateway uniquely within this MGI."
   -- SYNTAX INTEGER (1..2147483647)
   SignallingGatewayInterfaceId ::= INTEGER (1..2147483647)   
   
   MedGwyEventListType ::= OCTET STRING (SIZE (8))
   
   -- *****************************************************************
   -- mediaGatewayTable
   -- *****************************************************************
   mediaGatewayTable OBJECT-TYPE
      SYNTAX SEQUENCE OF MediaGatewayEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION "List of gateways configured for this IVPS."
   ::= {mediaGatewayConfig 1}

   -- *****************************************************************
   -- mediaGatewayEntry
   -- *****************************************************************
   mediaGatewayEntry OBJECT-TYPE
      SYNTAX MediaGatewayEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION "Entry holding configuration information for a gateway."
      INDEX {mediaGatewayId}
   ::= {mediaGatewayTable 1}
   
   MediaGatewayEntry ::= SEQUENCE
   {
      mediaGatewayId MediaGatewayId, -- na
      mediaGatewayRowStatus RowStatus, -- rw
      mediaGatewayName DisplayString, -- rw
      mediaGatewayOperStatus INTEGER, -- ro
      mediaGatewayAdminStatus INTEGER, -- rw
      mediaGatewayIPAddress IpAddress, -- rw
      mediaGatewayUDPPort INTEGER, -- rw
      mediaGatewaySubnetMask IpAddress, -- rw
      mediaGatewayNextHop IpAddress, -- rw
      mediaGatewayVLAN INTEGER, -- rw
      medGwyCtrlrCallServerId INTEGER, -- rw
      medGwyCtrlrPrimaryIPAddress IpAddress, -- rw
      medGwyCtrlrPrimaryUDPPort INTEGER, -- rw
      medGwyCtrlrSecondaryIPAddress IpAddress, -- rw
      medGwyCtrlrSecondaryUDPPort INTEGER, -- rw
      medGwyCtrlrActiveAddress INTEGER, -- ro
      mediaGatewaySGI INTEGER, -- ro      
      medGwyCtrlrCallServerType INTEGER, --rw
      mediaGatewayLockType INTEGER, --wo
      mediaGatewayLockDelay INTEGER, --wo
      mediaGatewaySignDscp INTEGER, -- rw
      mediaGatewaySignDot1P INTEGER, -- rw
      mediaGatewayVoiceDscp INTEGER, -- rw
      mediaGatewayVoiceDot1P INTEGER, -- rw
      mediaGatewayMidType INTEGER, -- rw
      mediaGatewayDomainName DisplayString, -- rw
      mediaGatewaySVCReasonFormat INTEGER, -- rw
      mediaGatewayProfileName DisplayString, -- rw
      medGwyCtrlrTertiaryIPAddress IpAddress, -- rw
      medGwyCtrlrTertiaryUDPPort INTEGER, -- rw      
