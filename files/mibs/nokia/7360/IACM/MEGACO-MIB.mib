--  Data Define
--  LTU8::= INTEGER(0..255) 
--  LTU16::= INTEGER(0..65535)  
--  LTU32::= Gauge
--  LTS32::= INTEGER   
     
  LTSessionCmd::= INTEGER
  {
    create(1),      	--create a test session, ready for test data transfering
    starttest(2),    	--transfer data done, start a test
    sendinfo(3),  	    --start to transfer info data for an underway testing, to cancel 1 or some lines' test task
    startinfo(4),      	--transfer info data done, start info operation
    destroy(5),         --destroy a test session
    receiveSubTest(6),  --start to transfer the sub test's data
    startSubTest(7),    --transfer sub test's data done, start the sub test
    repeat(8),          --repeat the last test, just for continuous mode test.
    endtest(9)          --end current test session.
  }           
                      
  LTSessionState::= INTEGER
  {
	  idle(1),               --indicate the session is not initialized, you can create it.
	  revcmd(2),         --indicate the session is recieving TEST cmd.
	  runcmd(3),         --indicate the session is running TEST cmd.
	  revinfo(4),          --indicate the session is recieving INFO cmd.
	  testover(5),        --indicate the session test is finished, you can get test result.
	  stateerror(6),      --something is wrong. 
	  testhold(7)         --indicate the session is held for sub test of talking test or 1 round is finished in continuous test. 
  }
  --NOTICE::OPERATOR COULD SEND DESTROY COMMAND @ TESTOVER STATUS, WHILE @ TESTHOLD STATUS, OPERATOR NEED ONLY GET TEST RESULTS!
                      
  LTLineIdStatus::= INTEGER
  {
  	intest(1),         -- put the line in the test. this is for write to put the lineId to test.
  	testover(2),       -- this line is tested, and the result is ready for polling.  this is read-only
  	rstpartlyready(3), -- it's defined for DP/DTMF test test, in this state, results partly are ready for tested rounds, network manager shall get these results but need re-read from first result(ltExtRpRstIndex=1) every time.
  	testrejected(4)    -- line test not supported for shared line, line test rejected.
  }

   LTLineIdHookChangeStatus::= INTEGER
  {
  	fromOffhooktoOnhook(1),   -- the tested line has hook state change, it from off hook to on hook
  	fromOnhooktoOffhook(2)    -- the tested line has hook state change, it from on hook to off hook
  }

  LTTestMode::= INTEGER
  {
    single(1),       -- default value, means the test needn't repeat in program, it will end after one time.
    block(2),        -- 20 rounds test will be done automatically by NBLT after test start, all results would be report together.
    continuous(3),   -- operator's program will send repeat command to NBLT to start a new round after receive last round's result.
    interactive(4),  -- the interactive test.
    cablepair(5)     -- the cable pair test.
  }
  -- !!! about test mode, please notice ::
  -- the block test would do 20 rounds automatically after recieve test start command, 
  -- and 20 rounds' results would be reported one time after 20 rounds done.
  -- while continuous test would do 1 round after start command, and would be re-started 
  -- by the repeat command sent out by Operator after get last round's result.
  -- !!!please be noticed, the repeat command is sent out automatically by operator's program(network manager..), 
  -- the test would be repeat and repeat untill the operator cancel or end the test. 
  -- Continuous does not has the limition of 20 rounds.
  -- Moreover:
  -- Block/Continuous test only support these test types:
  --   Foreign Voltage (AC/DC)
  --   Capacitance
  --   Insulation Resistance
  -- And only 1 of these test types could be set in 1 test requirement, that means u could not combine AC current of TR with
  -- any other test type, no matter it's AC current RG or Capacity of TR.

  -- for block test, the 20 rounds result are all available but for continuous, the next result would overwritten the last one.
  --    Finally, xDSL line test  only support the following values of test mode for NALT-M/NVLT-M lines:
  --    single(1),
  --    cablepair(5)
                        

LTSessionInputErrCode::= INTEGER
{
	noErr(0),				 -- 0  no error.
	ltTestParamNumInconsistent(1),           -- 1  the actual input param number in ltTestParamTable is not equal to ltTestParamNum.
 	ltTestParamNumOutOfRange(2),             -- 2  the input param number is out of range, from 0 to 4 is allowed.  
	testTypeNeedNoParam(3),                  -- 3  the input test param is meaningless, because those test types not need any test param.
