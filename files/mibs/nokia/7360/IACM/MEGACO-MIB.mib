    testBeyondConfigedThreshold(18),  -- test result beyond the threshold that operator configed
    pPErrVinDegradedAccuracy(19),     -- the accuracy of the results are degraded because of low leakage resistance
                                      -- For impendence test, the resistance part is too low of an impedance compared with the capacitance part. 
                                      -- Repeat the test with a higher frequency to improve the accuracy.
		pPErrVinThermalFaultDetected(20),			-- a thermal fault was detected during the test, the results of the test are indeterminate
																					-- For impendence test, a very large capacitor is present. 
		pPErrVinExcessiveAcInduction(21),			-- excessive AC induction was detected during the test, the results of the test are indeterminate
																					-- For impendence test, the test cannot be executed due to an external AC fault.	
		pPErrVinCmSaturationDetected(22),			-- common mode saturation was detected during the test, the results of capacitance measurements could not be calculated.	
																					-- For impendence test, can repeat the test with a reduced common mode voltage value.	
		pPErrVinDiffSaturationDetected(23),		-- differential saturation was detected during the test, the results of capacitance measurements could not be calculated
																					-- For impendence test, can repeat the test with a reduced differential voltage value.
		pPErrVinExcessiveForeignVoltage(24),	-- a large foreign voltage prevented the respective tests from being executed
		pPErrVinCapacitorOutOfRange(25),			-- capacitor were too large to be measured		
		pPErrVinExcessiveLowResistance(26),	  -- the 3-element capacitance test has not been executed due to the presence of a very low fault resistance
		pPErrVinReceiverOffHook(27),					-- the 4-Element Resistance test has measured a low metallic resistance and the ROH test recognized it as a receiver off-hook, as opposed to a metallic fault.
		pPErrVinTestNotPassed(28),						-- test failed because the measured voltage never settled to within tolerancelimits. 
		pPErrVinGndReported(29),		          -- a single resistance to ground
		pPErrVinResistanceOutOfRange(30),	    -- Resistance were too large to be measured
		testErrLineStatusNotSupported(31),	  -- the related test item can not be executed in current line status.
		testItemNotApplicable(32),		        -- the related test item was not applicable
		passed(33),                           -- GR909 sub test passed
		failed(34),                            -- GR909 sub test failed
		voiceDialnosisDiallingRelease(35),
        voiceDialnosisAfterDialRelease(36),
        voiceDialnosisNoIncomingCall(37),
        voiceDialnosisOffHookRelease(38),
        voiceDialnosisNoAnswer(39),
        voiceDialnosisDTMFCheckFail(40),
        voiceDiagnosisNoDialTone(41),
    testErrLoopbackHWsetFailed(42),       -- ISDN PRA loopback setting failed
    testErrPatternCheckFailed(43)         -- ISDN PRA loopback pattern check failed 
  } 


