accSharedLineTerminationCallStatus OBJECT-TYPE
    SYNTAX INTEGER
        {
        none(0),
        onhook(1),
        offhook(2),
        dialling(3),
        calling(4),
        idleoffhook(6),
        busyTone(7),
        howlerTone(8),
        ringing(9),
        releasing(10),
        ringback(11),
        callerConnected(12),
        calleeConnected(13),
        sharedCutoff(14)
        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The status of call at the secondary termination. The meaning of the status is list as following:
        none(0): there is no meaning when oper status is down and admin-status is down
        onhook(1): hook on without any call
        offhook(2): hookoff but user no dialling
        dialling(3): dial digit
        calling(4):setting up connection with remote (notify msg)
        idleoffhook(6): parking
        busyTone(7): this status unused after R41
        howlerTone(8): this status unused after R41
        ringing(9): ringing
        releasing(10): call has been terminated. but subscriber does not hook on. Some tone maybe play in this state. The line is in releasing state
        ringback(11): ringback tone to caller
        callerConnected(12): call has been setup (connected as caller)
        calleeConnected(13): call has been setup (connected as callee)
        sharedCutoff(14):The one termination configured at a shared line is cut off by the system
                                     when the other termination configured at that same share line is found to be in one of
                                     the following states : offhook, dialling, ringing, releasing, ringback, callerConnected,
                                     calleeConnected.
                                 The one termination that is in sharedCutoff state will hear silence upon going off-hook
                                     (no dial-tone is put at the line); Also incoming call attempts are rejected for the one
                                     termination in internal blocked state.

        ALCATEL-LUCENT NOTE:
            ACCESS:     NA
            USAGE:      NA
            PERSIST:    NO
            INSRVMOD:   NA
            RANGE:      0..14
            DEFVALUE    NA
            UNITS:      NA
            SPARSE:     NO
        "
    ::= { accessSharedLineTerminationEntry 11 }

accSharedLineTerminationOpStatDownReason OBJECT-TYPE
    SYNTAX INTEGER
        {
        none(0),
        registering(1),
        registrationFailure(2),
        physicalFailure(3)
        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The operation status down reason of the secondary termination.
               none(0) - there is no meaning when term is up
               registering(1) - The secondary termination is registering.
               registrationFailure(2) - The secondary termination registration has failed.
               physicalFailure(3) - The secondary termination is out of service.

        ALCATEL-LUCENT NOTE:
            ACCESS:     NA
            USAGE:      NA
            PERSIST:    NO
            INSRVMOD:   NA
            RANGE:      0 | 1 | 2 | 3
            DEFVALUE    NA
            UNITS:      NA
            SPARSE:     NO
        "
    ::= { accessSharedLineTerminationEntry 12 }

accSharedLineTermVoiceServAtMgc   OBJECT-TYPE
    SYNTAX  INTEGER
    {
        enabled (1),
        disabled (2)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "This MIB object indicates whether the voice service is enabled/disabled at the MGC for the secondary termination.
        Enabled (1) : The secondary termination is known to the MGC. The secondary termination can be registered to the MGC.
        Disabled (2) : The secondary termination is NOT known to the MGC. The secondary termination CANNOT be registered to the MGC.
        When the secondary termination entry has this object set to 'disabled (2)' while the accSharedLineTerminationAdminStatus = locked, the secondary termination is not feeded; Should the secondary termination go off-hook, no tone is generated.
        When the secondary termination entry has this object set to 'disabled(2)' while the accSharedLineTerminationAdminStatus = unlocked, the secondary termination is feeded; Should the secondary termination go off-hook, a special busy tone with tone name = nnatone (number_not_assigned tone ) and with tone ID = 237, is played.
ALCATEL-LUCENT NOTE:
            ACCESS:    READ-WRITE
            USAGE:     OPT
            PERSIST:   YES
            INSRVMOD:  YES
            RANGE:     1 | 2
            DEFVALUE:  enabled (1) (for backward compatibility reason)
            UNITS:     NA
            SPARSE:    NO"
            DEFVAL  {enabled}
   ::= {accessSharedLineTerminationEntry 13}

