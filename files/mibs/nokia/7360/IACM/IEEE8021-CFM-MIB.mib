--                   { notInService(2), createAndGo(4), destroy(6) }
--         DESCRIPTION   
--               "Support for createAndWait is not required."

--         OBJECT  dot1agCfmMaRowStatus
--         SYNTAX  RowStatus
--                   { active(1), notInService(2) }
--         WRITE-SYNTAX    RowStatus
--                   { notInService(2), createAndGo(4), destroy(6) }
--         DESCRIPTION   
--               "Support for createAndWait is not required."

--         OBJECT  dot1agCfmVlanRowStatus
--         SYNTAX  RowStatus
--                   { active(1), notInService(2) }
--         WRITE-SYNTAX    RowStatus
--                   { notInService(2), createAndGo(4), destroy(6) }
