            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "An entry in the sipIsdnCasTerminationTable."
            INDEX { ifIndex }
        ::= { sipIsdnCasTerminationTable  1 }

        SipIsdnCasTerminationEntry ::=
        SEQUENCE {
            sipIsdnCasTerminationGroupDirectoryNumber  OCTET STRING,
