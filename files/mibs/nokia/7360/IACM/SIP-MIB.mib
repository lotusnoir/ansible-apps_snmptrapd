			netwRedunFGMonitorIntv			INTEGER,
			netwRedunBGMonitorMethod		INTEGER,
			netwRedunBGMonitorIntv			INTEGER,
			netwRedunStableObsPeriod		INTEGER,
			netwRedunFOHysteresis			INTEGER,
