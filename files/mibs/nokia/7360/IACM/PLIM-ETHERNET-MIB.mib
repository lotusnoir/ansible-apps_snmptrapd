-- =====================
-- A table to list all unicast MAC address from a user port, which could be
--dynamically learned or statically configured. This table is also used to
--manually delete a dynamically learned MAC address.
-- =====================

	extendUserPortFdbTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF ExtendUserPortFdbEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains unicast MAC address, static and dynamic,
			per user forwarder interface, which could be translated to a vMAC 				address to network.
			And it also allows to remove  MAC addresses, which were obtained
			through the self learning mechanism, from the forwarding database
			on a specified port.
			Note that this table can not be used to delete a static MAc address.             			ALCATEL NOTE:
                 TABLESIZE:  DEP [nbr of unicast addresses/board * nbr of
                                 boards]"
		::= { extendForwardingInfo 10 }

	extendUserPortFdbEntry OBJECT-TYPE
		SYNTAX	ExtendUserPortFdbEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"An entry of extendUserPortFdbTable, learned or configured on a user forwarder port."
		INDEX	{ ifIndex, extendUserPortFdbUserAddress }
		::= { extendUserPortFdbTable 1 }

	 ExtendUserPortFdbEntry ::=
      SEQUENCE {
         extendUserPortFdbUserAddress
            MacAddress,
         extendUserPortFdbId
            Gauge,
         extendUserPortFdbStatus
            INTEGER,
         extendUserPortFdbvMacAddress
            MacAddress,
         extendUserPortFdbRowStatus
            RowStatus
      }


	  extendUserPortFdbUserAddress OBJECT-TYPE
		SYNTAX	MacAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The source MAC address contained in the frame coming from a user forwarder interface.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NA
                 RANGE:      NA       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      the user forwarder interface identifies a vlan.port"
		::= { extendUserPortFdbEntry 1 }

   	extendUserPortFdbId OBJECT-TYPE
		SYNTAX	Gauge
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Identifies a L2 forwarder which the user forwarder interface attaches to.
	             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NO
                 RANGE:      NA       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
		::= { extendUserPortFdbEntry 2 }

    extendUserPortFdbStatus OBJECT-TYPE
      SYNTAX	INTEGER {
         other(1),
         invalid(2),
         learned(3),
         self(4),
         mgmt(5)
      }
      ACCESS	read-only
      STATUS	mandatory
      DESCRIPTION
         "The status of this entry.  The meanings of the values are:
         other(1) - none of the following.  This may include	the case where
            some other MIB object is being used to determine if and how
            frames addressed to the value of the corresponding instance of
            extendUserPortFdbUserAddress are being forwarded.
         invalid(2) - this entry is no longer valid (e.g., it was learned
            but has aged out), but has not yet been flushed from the
            table.
         learned(3) - the value of the corresponding instance of
            extendUserPortFdbUserAddress was learned and is being used.
         self(4) - the value of the corresponding instance of
            extendUserPortFdbUserAddress represents one of the device's
            addresses.
         mgmt(5) - the value of the corresponding instance of
            extendUserPortFdbUserAddress is configured manually.
         ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NA       INSRVMOD:   NA
         RANGE:      3,5    DEFVALUE:   NA
         UNITS:      NA       SPARSE:     NA
         DESCR:      NA"
      ::= { extendUserPortFdbEntry 3 }

	extendUserPortFdbvMacAddress OBJECT-TYPE
		SYNTAX  MacAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
		"A unicast virtual MAC address to which the extendUserPortFdbUserAddress was translated and the device has forwarding and/or filtering information.
		             ALCATEL NOTE:
		                 ACCESS:     NA       USAGE:      NA
		                 PERSIST:    NO       INSRVMOD:   NO
		                 RANGE:      NA       DEFVALUE:   NA
		                 UNITS:      NA       SPARSE:     NA
		                 DESCR:      NA"
	::= { extendUserPortFdbEntry 4 }


      extendUserPortFdbRowStatus OBJECT-TYPE
		SYNTAX	RowStatus
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The row status of the entry.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   YES
                 RANGE: active(1),destroy(6)  DEFVALUE:   active(1)
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
        DEFVAL	{ active }
		::= { extendUserPortFdbEntry 5 }


