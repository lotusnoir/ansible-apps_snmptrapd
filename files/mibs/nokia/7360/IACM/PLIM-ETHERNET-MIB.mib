--		DEFVAL { disabled }
--		::= { extendVlanPortEntry 12 }

   extendVlanPortMirroringMode OBJECT-TYPE 
      SYNTAX INTEGER 
      { 
         disabled (1), 
         insertNewEthernetHeader (2),
         insertVlanTagOnly (3),
         overwriteOuterVlanTag (4)
      } 
      ACCESS read-write 
      STATUS mandatory 
      DESCRIPTION 
      "Indicates the flow mirroring mode of the bridgeport. It is disabled by default. Flow mirroring can be enabled in 3 modes: In mode 2, a completely new Ethernet Header is added to the mirrored packets. The VLAN-ID and Dest MAC address are configured in the objects given below this object. In mode 3, only a new VLAN tag is inserted, which is configured in extendVlanPortMirroringVlanId. In mode 4, a the outer VLAN tag is overwitten with the mirror VLAN tag.  Only values 1 and 4 are supported on GLTs.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      OPT
                 PERSIST:    YES      INSRVMOD:   YES
                 RANGE:      1..4       DEFVALUE:   1
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA
" 
      DEFVAL { disabled } 
      ::= { extendVlanPortEntry 13 }


   extendVlanPortMirroringVlanId OBJECT-TYPE 
      SYNTAX INTEGER (0..4093) 
      ACCESS read-write 
      STATUS mandatory 
      DESCRIPTION 
      "Indicates the VLAN ID to be inserted into  mirrored packets if bridgeport mirroring is enabled on this bridgeport. 
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      OPT
                 PERSIST:    YES      INSRVMOD:   YES
                 RANGE:      0..4093  DEFVALUE:   0
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA " 
      DEFVAL { 0 } 
      ::= { extendVlanPortEntry 14 }

        extendVlanPortCommittedMaxNumMacAddresses OBJECT-TYPE
                SYNTAX   Gauge
                ACCESS   read-write
                STATUS   mandatory
                DESCRIPTION
                       "The committed maximum number of unicast MAC addresses on the 
                        port, i.e. the committed maximum for the sum of the dynamic 
                        learned and the static configured unicast MAC addresses. This 
                        value should be equal to or less than the value of 
                        extendVlanPortMaxNumMacAddresses.Default value is 65535, which 
                        means it has the same value as extendVlanPortMaxNumMacAddresses.
                ALCATEL NOTE:
                        ACCESS:     NA       USAGE:      OPT
                        PERSIST:    YES      INSRVMOD:   YES
                        RANGE:      0..128,65535.[For BCM LT, only value between 1 
                                    and 16 with step 1 and value between 20 and 64 
                                    with step 4, or the default value 65535 can be 
                                    set.]    
                        DEFVALUE:   65535    UNITS:      NA       
                        SPARSE:     NA       DESCR:      NA"
                   ::= { extendVlanPortEntry 38 }
                                      
        extendVlanPortPVIDTaggingFlag OBJECT-TYPE
                SYNTAX INTEGER 
                { 
                   onu (1), 
                   olt (2)
                }                 
                ACCESS   read-write
                STATUS   mandatory
                DESCRIPTION
                        "	Indicates the tagging behavior of untagged frames in GPON:
                         When ONU (1)
                             upstream untag frame: PVID is tagged in ONU; downstream: transmission mode of PVID corresponding vlan.port is handled in ONU.
                         When OLT (2)
                             onu forwards the untag frame transparently, the PVID tagging and corresponding transmission mode will be handled in OLT.
                         Default = 1 = ONU.
                        
                ALCATEL NOTE:
                        ACCESS:     NA       USAGE:      OPT
                        PERSIST:    YES      INSRVMOD:   YES
                        RANGE:      1..2     DEFVALUE:   1
                        UNITS:      NA       SPARSE:     NA
                        DESCR:      NA"
                   ::= { extendVlanPortEntry 51 }
        extendVlanPortTPIDList OBJECT-TYPE 
                SYNTAX OCTET STRING (SIZE (8)) 
                ACCESS read-write 
                STATUS mandatory 
                DESCRIPTION 
                        "Indicates the TPID list on this bridgeport.The mapping of the
			                   TPID list is as follows(must be Hex) :
                         Byte 0 - 1:  TPID1 (default value is hex 8100, byte0:00, byte1:81) 
                         Byte 2 - 3:  TPID2 (default value is 0)		 
                         Byte 4 - 7:  not used and set to 0 
                ALCATEL NOTE:
                        ACCESS:     NA       USAGE:      OPT
                        PERSIST:    YES      INSRVMOD:   YES
                        RANGE:      600...ffff    DEFVALUE:   8100
                        UNITS:      NA       SPARSE:     NA
                        DESCR:      NA " 
                DEFVAL { "" } 
                ::= { extendVlanPortEntry 53 }

        extendVlanPortDSPbitMode  OBJECT-TYPE
                SYNTAX INTEGER
                {
                  auto (1),
                  translated (2),
                  transparency (3)
                }
                ACCESS    read-write
                STATUS    mandatory
                DESCRIPTION
                        " This object specifies if the P-bit should be translated or not in downstream.
                        auto : legacy behavior, for DSL, auto means transparency while for GPON, auto means translated.
                        translated : for matched p-bits the inverse translation is performed in downstream; unmatched p-bits are forwarded unchanged in downstream.
                        transparency : all p-bits are forwarded unchanged in downstream.
                        Attribute was first introduced as of ISAM5.5 MIB, with targeted implementation in ISAM5.5.01.
                ALCATEL NOTE:
                        ACCESS:     NA       USAGE:      OPT
                        PERSIST:    YES      INSRVMOD:   YES
                        RANGE:      1,2,3    DEFVALUE:   1 [auto] 
                        UNITS:      NA       SPARSE:     NA
                        DESCR:      NA"
                DEFVAL { auto }
                ::= {extendVlanPortEntry 54 }

