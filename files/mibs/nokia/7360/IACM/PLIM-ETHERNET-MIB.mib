--	extendVlanSetMaskPortList OBJECT-TYPE
--		SYNTAX	PortList
--		ACCESS	write-only
--		STATUS	mandatory
--		DESCRIPTION
--			"This object has only a meaning in case the object
--			extendVlanStaticSystemMode has the value residentialBridge:
--			it represents the set of ports that should be set as EgressPorts 
--			for this VLAN.
--			A 1 indicates it shall be added, a 0 means no action.
--			Those systems for which the number of ports is small, will support 
--			the object dot1qVlanStaticEgressPorts.
--			Those systems for which the number of ports is large, will split 
--			this EgressPorts info into a set of bitlists supported by the 
--			object extendVlanStaticBrEgressPorts in the table
--			extendVlanStaticBrPortsTable. In that case this
-- 			extendVlanSetMaskPortList is to be used in combination with
--			the object extendVlanStaticPortListId. The last object identifies
--			in combination with the object dot1qVlanIndex the entry in the
--			table extendVlanStaticBrPortsTable on which the -SetMask- applies."
--		::= { extendVlanStaticEntry 1 }

--	extendVlanClearMaskPortList OBJECT-TYPE
--		SYNTAX	PortList
--		ACCESS	write-only
--		STATUS	mandatory
--		DESCRIPTION
--			"This object has only a meaning in case the object
--			extendVlanStaticSystemMode  has the value residentialBridge:
--			it represents the set of ports that should be cleared as EgressPorts 
--			for this VLAN.
--			A 1 indicates it shall be cleared, a 0 means no action.
--			Those systems for which the number of ports is small, will support 
--			the object dot1qVlanStaticEgressPorts.
--			Those systems for which the number of ports is large, will split 
--			this EgressPorts info into a set of bitlists supported by the 
--			object extendVlanStaticBrEgressPorts in the table
--			extendVlanStaticBrPortsTable. In that case this
-- 			extendVlanSetMaskPortList is to be used in combination with
--			the object extendVlanStaticPortListId. The last object identifies
--			in combination with the object dot1qVlanIndex the entry in the
--			table extendVlanStaticBrPortsTable on which the -ClearMask- applies."
--		::= { extendVlanStaticEntry 2 }

	extendVlanStaticSystemMode OBJECT-TYPE
		SYNTAX	ExtendVlanSystemMode
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The Ethernet-ATM interworking function of this VLAN can work 
         in either cross-connect mode, residential bridging mode, QOS aware 
         or 'layer2Terminated' mode. Only cross-connect and residential
         bridging modes are supported in the 7367 ISAM SX / 7363 ISAM MX systems.
             ALCATEL NOTE:
                 ACCESS:     RO       USAGE:      MAND
                 PERSIST:    YES      INSRVMOD:   NO
                 RANGE:   1,2,3,4,7   DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"

--		DEFVAL  { crossconnect}		in R5.0.10 for GE-LSM
--		DEFVAL  { residentialBridge }	in NBE
-- 		Conclusion: default value is system specific and not part of the MIB
		::= { extendVlanStaticEntry 3 }
--
--	extendVlanStaticCcEgressPort	OBJECT-TYPE
--		SYNTAX	INTEGER (0..65535)
--		ACCESS	read-write
--		STATUS	mandatory
--		DESCRIPTION
--			"This object has only a meaning in case the object
--			extendVlanStaticSystemMode has the value crossconnect:
--			It is the port number of the ATM PVC port associated to this VLAN.
--			The value 0 is used to indicate that no port is associated
--			to the VLAN."
--		::= { extendVlanStaticEntry 4}
--
--	extendVlanStaticPortListId	OBJECT-TYPE
--		SYNTAX	INTEGER
--		ACCESS	write-only
--		STATUS	mandatory
--		DESCRIPTION
--			"This object has only a meaning in case the object
--			extendVlanStaticSystemMode  has the value residentialBridge:
--			together with the object dot1qVlanIndex it identifies the 
--			entry in the extendVlanStaticBrPortsTable on which the manager
--			wants to perform a configuration action. The configuration 
--			action is determined by the use of the object 
--			extendVlanSetMaskPortList or extendVlanClearMaskPortList in 
--			the same PDU."
--		::= { extendVlanStaticEntry 5}
--
   extendVlanStaticName OBJECT-TYPE
