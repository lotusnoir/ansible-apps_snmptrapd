-- ========================================================
--     extend VLAN TPID table.
-- ========================================================
extendVlanTpidTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF ExtendVlanTpidEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains system TPID value"
		::= { vlanStack 16 }

extendVlanTpidEntry OBJECT-TYPE
		SYNTAX	ExtendVlanTpidEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"An entry of systemTpidTable."
		INDEX	{ extendVlanTpidIndex }
		::= { extendVlanTpidTable 1 }

ExtendVlanTpidEntry ::=
      SEQUENCE {
         extendVlanTpidIndex
            INTEGER,
         extendVlanTpidValue
            INTEGER,
         extendVlanTpidRowStatus
            RowStatus
      }
      
extendVlanTpidIndex OBJECT-TYPE
		SYNTAX	INTEGER(1..4)
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"The system TPID index."
		::= { extendVlanTpidEntry 1 }
		
extendVlanTpidValue OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The system TPID value, must be Hex.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NA
                 RANGE:      1536..65535       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      system TPID value"
		::= { extendVlanTpidEntry 2 }

extendVlanTpidRowStatus OBJECT-TYPE
		SYNTAX	RowStatus
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The row status of the entry.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   YES
                 RANGE: active(1),createAndGo(4), destroy(6)  DEFVALUE:   active(1)
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
        DEFVAL	{ active }
		::= { extendVlanTpidEntry 3 }

END 
