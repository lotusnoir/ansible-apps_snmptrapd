-- =================================================================================================================
--
-- The table contains OLT-side rolling frame counters per PBit value AND per VLAN.
--
-- Rows in this table are created/deleted as a result of the creation/deletion of associated
-- rows in troubleShootingPbitObservationTable in the TROUBLESHOOTING-MIB.
-- As of ISAM5.0.01, the ifIndex value in this table only represents a VLAN (which is associated to the
-- bridge port ifindex from the above mentioned row in troubleShootingPbitObservationTable).
-- 
-- This table also contains an index for individulal PBit value (i.e. extendPortVlanPbitTroubleshootPriority),
-- so that manager can retrieve counters per PBit valuesper per VLAN port.
--
-- This table is SUPPORTED as of ISAM5.0.01
--
-- =================================================================================================================

   extendPortVlanPbitTroubleshootTable    OBJECT-TYPE
      SYNTAX   SEQUENCE OF ExtendPortVlanPbitTroubleshootEntry
      ACCESS   not-accessible
      STATUS   mandatory
      DESCRIPTION
         " OLT-side PBit troubleshoot counters for a vlan port.
             ALCATEL NOTE:
                 TABLESIZE:  DEP [8 per LT card]
                 DESCR:      NA"
      ::= { extendStatistics 18  }

   extendPortVlanPbitTroubleshootEntry OBJECT-TYPE
      SYNTAX   ExtendPortVlanPbitTroubleshootEntry
      ACCESS   not-accessible
      STATUS   mandatory
      DESCRIPTION
         "Table entry for OLT-side PBit troubleshoot counters for vlan port."
      INDEX   { ifIndex, extendPortVlanPbitTroubleshootPriority  }
      ::= { extendPortVlanPbitTroubleshootTable    1 }

    ExtendPortVlanPbitTroubleshootEntry ::=
      SEQUENCE {

            extendPortVlanPbitTroubleshootPriority
               ExtendVlanSystemPriority, 
            extendPortVlanPbitTroubleshootUpFwdFrameCounter
               OCTET STRING,
            extendPortVlanPbitTroubleshootDownFwdFrameCounter
               OCTET STRING,
            extendPortVlanPbitTroubleshootUpDropFrameCounter
               OCTET STRING,
            extendPortVlanPbitTroubleshootDownDropFrameCounter
               OCTET STRING }

    extendPortVlanPbitTroubleshootPriority OBJECT-TYPE
      SYNTAX   ExtendVlanSystemPriority
      ACCESS     read-only
      STATUS   mandatory
      DESCRIPTION
         " This index represents the PBit value associated with the counter
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NO
                 RANGE:      0..7       DEFVALUE:  NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA "
      ::= { extendPortVlanPbitTroubleshootEntry 1 }
   
   extendPortVlanPbitTroubleshootUpFwdFrameCounter OBJECT-TYPE
      SYNTAX   OCTET STRING (SIZE (8))
      ACCESS     read-only
      STATUS   mandatory
      DESCRIPTION
         " 64-bit counter equivalent for number of upstream forwarded frames.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NO
                 RANGE:      NA     DEFVALUE:   0
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
      ::= { extendPortVlanPbitTroubleshootEntry 2 }


   extendPortVlanPbitTroubleshootDownFwdFrameCounter OBJECT-TYPE
      SYNTAX   OCTET STRING (SIZE (8))
      ACCESS     read-only
      STATUS   mandatory
      DESCRIPTION
         " 64-bit counter equivalentfor number of downstream forwarded frames.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NO
                 RANGE:      NA     DEFVALUE:   0
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
      ::= { extendPortVlanPbitTroubleshootEntry 3 }


  extendPortVlanPbitTroubleshootUpDropFrameCounter OBJECT-TYPE
      SYNTAX   OCTET STRING (SIZE (8))
      ACCESS     read-only
      STATUS   mandatory
      DESCRIPTION
         " 64-bit counter equivalent for number of upstream dropped frames.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NO
                 RANGE:      NA     DEFVALUE:   0
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
      ::= { extendPortVlanPbitTroubleshootEntry 4 }


   extendPortVlanPbitTroubleshootDownDropFrameCounter OBJECT-TYPE
      SYNTAX   OCTET STRING (SIZE (8))
      ACCESS     read-only
      STATUS   mandatory
      DESCRIPTION
         " 64-bit counter equivalent for number of downstream dropped frames.
             ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    NO       INSRVMOD:   NO
                 RANGE:      NA     DEFVALUE:   0
                 UNITS:      NA       SPARSE:     NA
                 DESCR:      NA"
      ::= { extendPortVlanPbitTroubleshootEntry 5 }


-- ========================================================
--     vMAC Address Format. The 48 bit MAC address is defined 
--     in accordance with the VLAN MIB of type MacAddress.
-- ========================================================

systemVMacAddressFormat        OBJECT IDENTIFIER ::= { plimEthernet 11 }

--   extendVMacSizeofvMacId OBJECT-TYPE
--   ::= { systemVMacAddressFormat 1 }

--   extendVMacSizeofPortId OBJECT-TYPE
--   ::= { systemVMacAddressFormat 2 }

--   extendVMacSizeofSlotId OBJECT-TYPE
--   ::= { systemVMacAddressFormat 3 }

--   extendVMacSizeofRackId OBJECT-TYPE
--   ::= { systemVMacAddressFormat 4 }

--   extendVMacSizeofDSLAMId OBJECT-TYPE
--   ::= { systemVMacAddressFormat 5 }

extendVMacValueofDSLAMId OBJECT-TYPE
SYNTAX  INTEGER
ACCESS  read-write
STATUS  mandatory
DESCRIPTION
      "The object specifies the value of the DSLAM Id. This value 
      must be unique DSLAM Id within an EMAN connected to the same IP edges. 
      When the value is 0, it implies that the object has not been 
      set yet and vMAC could not be enabled on any VLAN. The value
      0 cannot be configured; it is a factory default value only.
      For CFM, this field must be configured to ensure the uniqueness 
      of the MP MAC address across the network with multiple DSLAMs.
ALCATEL NOTE:
ACCESS:     RW       USAGE:      OPT
PERSIST:    YES      INSRVMOD:   YES
            RANGE:   0..524287   DEFVALUE:   0
UNITS:      NA       SPARSE:     NA
DESCR:      NA"
             DEFVAL { 0 }
::= { systemVMacAddressFormat 6 }

