            gxdslRtxMode                   GRtxMode,
            gxdslRtxMinExpThrpt            INTEGER,
            gxdslRtxPlanExpThrpt           INTEGER,
            gxdslRtxMaxExpThrpt            INTEGER,
            gxdslRtxMaxNetDataRate         INTEGER,
            gxdslRtxMinDelay               INTEGER,
            gxdslRtxMaxDelay               INTEGER,
            gxdslRtxMinInpShine            INTEGER,
            gxdslRtxMinInpRein             INTEGER,
            gxdslRtxReinInterArrTime       INTEGER,
            gxdslRtxShineRatio             INTEGER,
            gxdslRtxLeftrThreshold         INTEGER,
            gxdslRtxProfileRowStatus       RowStatus
        }

gxdslRtxProfileIndex           OBJECT-TYPE
        SYNTAX          AsamProfileIndex
        ACCESS          not-accessible
        STATUS          mandatory
        DESCRIPTION     
        "This object is used to uniquely identify an entry in this table.
         It is used by the line configuration table to identify a row of 
         the table. The max value of this object is limited to 
         gxdslRtxProfileMaxIndex value.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    1..128              DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
::= {gxdslRtxProfileEntry 1}

gxdslRtxProfileName            OBJECT-TYPE
        SYNTAX          AsamProfileName
        ACCESS          read-write
        STATUS          mandatory
        DESCRIPTION     
        "A textual name identifying this profile. This name has to be unique.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    MAND
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    NA                  DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
::= {gxdslRtxProfileEntry 2}

gxdslRtxProfileVersionNbr      OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
        DESCRIPTION     
        "The manager can associate a version number to the profile. It is set
         to 1 when the profile is first created and will be incremented by the
         manager when the profile is modified.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    OPT
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    NA                  DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
        DEFVAL          {1}
::= {gxdslRtxProfileEntry 3}

gxdslRtxProfileUsedStatus      OBJECT-TYPE
        SYNTAX          AsamProfileRefCount
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
        "Indicates the number of entities using this profile. If the reference
         count is greater than 0, then this profile is 'in use', and may NOT be
         deleted. If the value is 0, then this profile is 'not in use', and can
         be deleted.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  NO                  INSRVMOD: NO
             RANGE:    NA                  DEFVALUE: 0
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
::= {gxdslRtxProfileEntry 4}

gxdslRtxMode OBJECT-TYPE
        SYNTAX      GRtxMode 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This parameter allows the manager to enable the RTX(Retransmission)
	 feature on lines (only VDSL2) supporting RTX.  As long as the feature 
	 has not been enabled, the other RTX parameters will have no effect on this line.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    1,2,3,4             DEFVALUE: rtxPreferred
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 5}

gxdslRtxMinExpThrpt OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object contains the minimum expected throughput (bitrate)
         when gxdslRtxMode is different from rtxForbidden.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..2147483647       DEFVALUE: 64
             UNITS:    kbits/sec           SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 6}

gxdslRtxPlanExpThrpt OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object contains the planned expected throughput (bitrate)
         when gxdslRtxMode is different from rtxForbidden.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..2147483647       DEFVALUE: 128
             UNITS:    kbits/sec           SPARSE:   NA
             DESCR:    NA

        Introduced in ISAM5.3.01 MIB for MIB readiness
        For GPON C NOT SUPPORTED as of ISAM5.3.02
        For XG-PON1 - NOT SUPPORTED as of ISAM5.3.02
        For NG-PON2 - NOT SUPPORTED as of ISAM5.3.02
        "
::= { gxdslRtxProfileEntry 7}

gxdslRtxMaxExpThrpt OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object contains the maximum expected throughput (bitrate)
         when gxdslRtxMode is different from rtxForbidden.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..2147483647       DEFVALUE: 262143
             UNITS:    kbits/sec           SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 8}

gxdslRtxMaxNetDataRate OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object contains the maximum net data rate 
	 when gxdslRtxMode is different from rtxForbidden.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..2147483647       DEFVALUE: 262143
             UNITS:    kbits/sec           SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 9}

gxdslRtxMinDelay OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object contains the minimum instantaneous delay allowed (only due 
         to the effect of the retransmission) when gxdslRtxMode is different from rtxForbidden.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..63               DEFVALUE: 0
             UNITS:    milliseconds        SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 10}

gxdslRtxMaxDelay OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object contains the maximum instantaneous delay allowed (only due 
         to the effect of the retransmission) when gxdslRtxMode is different from rtxForbidden. 
         When RTX is used in upstream or downstream direction, it invalidates the corresponding
	 gxdslMaxInterleavingDelayUpstream or gxdslMaxInterleavingDelayDownstream parameter of the Service profile.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    2..63               DEFVALUE: 12
             UNITS:    milliseconds        SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 11}

gxdslRtxMinInpShine OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object defines the minimum impulse noise protection against 
         SHINE (single high impulse noise event) when gxdslRtxMode is different from rtxForbidden.
         It is expressed in DMT symbols with a subcarrier spacing of 4.3125kHz.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..63               DEFVALUE: 8
             UNITS:    symbols             SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 12}

gxdslRtxMinInpRein OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object defines the minimum impulse noise protection against REIN
         (repetitive electrical impulse noise) when gxdslRtxMode is different from rtxForbidden.
         It is expressed in DMT symbols with a subcarrier spacing of 4.3125kHz.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..7                DEFVALUE: 0
             UNITS:    symbols             SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 13}

gxdslRtxReinInterArrTime OBJECT-TYPE
        SYNTAX      INTEGER 
                    {
                      derivedFrom100Hz (1),
                      derivedFrom120Hz (2)
                    }
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        "This object specifies the inter-arrival time that shall be assumed for 
         REIN (repetitive electrical impulse noise)protection when gxdslRtxMode 
	       is different from rtxForbidden.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    1,2                 DEFVALUE: 1
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 14}

gxdslRtxShineRatio OBJECT-TYPE
        SYNTAX      INTEGER 
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
        " This object specifies the SHINE ratio when gxdslRtxMode is different from rtxForbidden.
         The value ranges from 0 to 0.100 with increments of 0.001.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..100              DEFVALUE: 10
             UNITS:    1/1000              SPARSE:   NA
             DESCR:    NA
         "
::= { gxdslRtxProfileEntry 15}

gxdslRtxLeftrThreshold        OBJECT-TYPE
        SYNTAX          INTEGER
        ACCESS          read-write
        STATUS          mandatory
        DESCRIPTION     
        "This parameter specifies the threshold for declaring a near-end 'leftr' 
         defect when gxdslRtxMode is different from rtxForbidden.
         The value ranges from 0.01 to 0.99 with increments of 0.01.
         The special value 0 means the ETR shall be used as the default threshold
         for declaring a 'leftr' defect.    

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    OPT
             PERSIST:  YES                 INSRVMOD: NO
             RANGE:    0..99               DEFVALUE: 0
             UNITS:    1/100               SPARSE:   NA
             DESCR:    NA
        "
::= { gxdslRtxProfileEntry 16}

gxdslRtxProfileRowStatus       OBJECT-TYPE
        SYNTAX          RowStatus
        ACCESS          read-write
        STATUS          mandatory
        DESCRIPTION 
        "This object is used to create a new row or delete an existing row in
         this table. The usage is as per standard semantics of 'RowStatus'
         defined in SNMPv2-TC.
            o Both 'createAndGo' and 'createAndWait' options of row creation
              are supported.
            o The entry associated with a Profile that is being used may
              not be deleted.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    MAND
             PERSIST:  YES                 INSRVMOD: YES
             RANGE:    NA                  DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
::= { gxdslRtxProfileEntry 17}

gxdslRtxProfileMappingTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF GxdslRtxProfileMappingEntry
        ACCESS          not-accessible
        STATUS          mandatory
        DESCRIPTION
        "The (conceptual) listing the profile name to profile Index
         Mapping.

         ALCATEL NOTE:
             TABLESIZE: 128                    
             DESCR:     NA                          
        " 
        ::= {gxdslRtxProfile 4}

gxdslRtxProfileMappingEntry    OBJECT-TYPE
      SYNTAX            GxdslRtxProfileMappingEntry
      ACCESS            not-accessible
      STATUS            mandatory
      DESCRIPTION
        "An entry (conceptual row) representing the profile Name to Index
         Mapping.

         Row creation : When an entry is created in the 
                        gxdslRtxProfileTable, an entry is created in
                        this table.
         Row deletion : When an entry is deleted from the
                        gxdslRtxProfileTable, the entry is deleted from
                        this table.
        "
        INDEX            {gxdslRtxProfileMappingName}
        ::= { gxdslRtxProfileMappingTable 1 }

GxdslRtxProfileMappingEntry ::=
        SEQUENCE {
            gxdslRtxProfileMappingName        AsamProfileName,
            gxdslRtxProfileMappingIndex       AsamProfileIndex
        }

gxdslRtxProfileMappingName     OBJECT-TYPE
        SYNTAX          AsamProfileName
        ACCESS          not-accessible
        STATUS          mandatory
        DESCRIPTION
        "The RTX(Retransmission) profile name.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  NO                  INSRVMOD: NA
             RANGE:    NA                  DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
        ::= { gxdslRtxProfileMappingEntry 1 }

gxdslRtxProfileMappingIndex    OBJECT-TYPE
        SYNTAX          AsamProfileIndex
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
        "The Index of the corresponding RTX(Retransmission) profile.

         ALCATEL NOTE:
             ACCESS:   NA                  USAGE:    NA
             PERSIST:  NO                  INSRVMOD: NA
             RANGE:    1..128              DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        "
        ::= { gxdslRtxProfileMappingEntry 2 }




END

