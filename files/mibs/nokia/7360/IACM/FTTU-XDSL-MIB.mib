FTTU-XDSL-MIB DEFINITIONS ::= BEGIN


IMPORTS
        
        Counter, 
        TimeTicks, 
        Gauge
                FROM RFC1155-SMI

        OBJECT-TYPE
                FROM RFC-1212
                
        ifIndex
                FROM ITF-MIB

        RowStatus
                FROM SNMPv2-TC

        asam
                FROM SYSTEM-MIB

        AsamNextProfileIndex, 
        AsamMaxProfileIndex, 
        AsamProfileIndex,
        AsamProfilePointer, 
        AsamProfileName, 
        AsamProfileScope,
        AsamProfileRefCount
                FROM ASAM-TC-MIB

        eqptSlotId
                FROM ASAM-EQUIP-MIB;

--FGU4.4: REPOSITIONING OF THE XDSL MIB UNDER AN APON BRANCH
apon                             OBJECT IDENTIFIER ::= { asam 35 }
fttuXdslMIB                      OBJECT IDENTIFIER ::= { apon 25 }
-- xdslMIB                          OBJECT IDENTIFIER ::= { asam 39 }
gxdslMIB                          OBJECT IDENTIFIER ::= { fttuXdslMIB 1}

-- groups in XDSL-MIB

gxdslLine                         OBJECT IDENTIFIER ::= { gxdslMIB 3 }

gxdslFarEndLine                   OBJECT IDENTIFIER ::= { gxdslMIB 4 }

gxdslChannel                      OBJECT IDENTIFIER ::= { gxdslMIB 5 }

gxdslFarEndChannel                OBJECT IDENTIFIER ::= { gxdslMIB 6 }

gxdslTest                         OBJECT IDENTIFIER ::= { gxdslMIB 7 }

gxdslXturInventory                OBJECT IDENTIFIER ::= { gxdslMIB 8 }

gxdslXtucInventory                OBJECT IDENTIFIER ::= { gxdslMIB 9 }

gxdslLineTcaThreshold             OBJECT IDENTIFIER ::= { gxdslMIB 10 }

gxdslChannelTcaThreshold          OBJECT IDENTIFIER ::= { gxdslMIB 11 }

gxdslLinkUp                       OBJECT IDENTIFIER ::= { gxdslMIB 12 }

gxdslDpboProfile					OBJECT IDENTIFIER ::= { gxdslMIB 13 }
gxdslGponOnt					    OBJECT IDENTIFIER ::= { gxdslMIB 14 }



