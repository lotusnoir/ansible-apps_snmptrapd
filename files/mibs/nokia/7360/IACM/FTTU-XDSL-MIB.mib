             ACCESS:   NA                  USAGE:    OPT
             PERSIST:  YES                 INSRVMOD: YES
             RANGE:    NA                  DEFVALUE: NA
             UNITS:    NA                  SPARSE:   NA
             DESCR:    NA
        
