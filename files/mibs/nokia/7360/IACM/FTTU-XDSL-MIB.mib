           SEQUENCE {
--             adslModemFeatures                         INTEGER,
               gadslMaxAggregatePowerLevelDownstream      INTEGER,
               gadslMaxAggregatePowerLevelUpstream        INTEGER,
               gadslMaxPsdLevelDownstream                 INTEGER,
               gadslMaxPsdLevelUpstream                   INTEGER
