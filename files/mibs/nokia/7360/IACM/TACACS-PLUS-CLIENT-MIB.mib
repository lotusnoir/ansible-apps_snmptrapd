-- Start of tacAuthenServerTable

tacAuthenServerTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF TacAuthenServerEntry
      ACCESS     not-accessible
      STATUS     mandatory
      DESCRIPTION
         "The (conceptual) table listing the TACACS+ authentication
         servers with its configurable parameters.
         If a entry (server) has to be deleted, then it must not be
         associated with any tacacs+ Policy. If so, the deletion is
         not allowed.
         Introduced in ISR5.2, to be supported in ISR5.2.01.
         ALCATEL NOTE :
            TABLESIZE : 256 
            DESCR     : NA"
      ::= { tacacsPlusClientMIB 5 }

tacAuthenServerEntry OBJECT-TYPE
      SYNTAX     TacAuthenServerEntry
      ACCESS	 not-accessible
      STATUS     mandatory
      DESCRIPTION
         "An entry (conceptual row) representing the configuration 
         of a TACACS+ authentication server."
      INDEX { tacAuthenServerIndex }
      ::= { tacAuthenServerTable 1 }

TacAuthenServerEntry ::= SEQUENCE {
      tacAuthenServerIndex          INTEGER,
      tacAuthenServerName           DisplayString,
      tacAuthenServerVrfIndex	    TacacsRouterIndex,
      tacAuthenServerAddrType       InetAddressType,
      tacAuthenServerAddress	    InetAddress,
      tacAuthenServerPort  	    InetPortNumber,
      tacAuthenServerMode           INTEGER,
      tacAuthenServerSecret	    DisplayString,
      tacAuthenServerTimeoutLimit   TacacsPlusTimeout,
      tacAuthenServerAdminState	    INTEGER,
      tacAuthenServerOperState	    INTEGER,
      tacAuthenServerDeadInterval   TacacsPlusDeadInterval,
      tacAuthenServerRowStatus	    RowStatus
      }

tacAuthenServerIndex	 OBJECT-TYPE
      SYNTAX		INTEGER
      ACCESS		not-accessible
      STATUS		mandatory
      DESCRIPTION
         "Index used to uniquely identify an entry in this table.
         ALCATEL NOTE :
            ACCESS:     NA / INDEX
            USAGE:      NA
            PERSIST:  	YES
            INSRVMOD: 	NA
            RANGE:	RANGE(1..256)
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 1 }

tacAuthenServerName	 OBJECT-TYPE
      SYNTAX		DisplayString (SIZE (1..64))
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
         "The Name associated with this Tacacs Authentication Server Entry. It should be  
         unique among the different Authentication Servers configured.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      MAND
            PERSIST:  	YES
            INSRVMOD: 	NO
            RANGE:	NA
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 2 }

tacAuthenServerVrfIndex	 OBJECT-TYPE
      SYNTAX		TacacsRouterIndex
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
	 "The tacAuthenServerVrfIndex to which this TACACS+ Authentication server belongs.
         In case of NE with SHUB, this is the Vrf. 
         In case of NE with IHUB, this is the Service Id of the VPRN or the base router when 0.
         Only base router supported in ISR5.2. 
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      MAND
            PERSIST:  	YES
            INSRVMOD: 	NO
            RANGE:      DEP (1..127) in case of NE with SHUB,
                        (0..2147483647) in case of NE with IHUB
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 3 }

tacAuthenServerAddrType  OBJECT-TYPE
      SYNTAX		InetAddressType
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
         " A value representing the type of Internet Address.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      OPT
            PERSIST:  	YES
            INSRVMOD: 	NO
            RANGE:	NA
            DEFVALUE: 	1(ipv4)
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 4 }

tacAuthenServerAddress          OBJECT-TYPE
      SYNTAX		InetAddress
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
         "The IP address of this TACACS+ Authentication server. The Type of 
         this Ip Address (IPv4/Ipv6/...) is determined by the value of the 
         object tacAuthenServerAddrType.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      MAND
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      NA
            DEFVALUE: 	NA
            UNITS:	NA
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 5 }

tacAuthenServerPort OBJECT-TYPE
      SYNTAX            InetPortNumber
      ACCESS            read-write
      STATUS            mandatory
      DESCRIPTION
	 "The TCP port number to/from server as TACACS+ authentication messages is sent/received 
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      OPT
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      RANGE(0..65535)
            DEFVALUE: 	49
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      DEFVAL  {49}
      ::= { tacAuthenServerEntry 6 }

tacAuthenServerMode OBJECT-TYPE
      SYNTAX            INTEGER {
                           ascii (1),
                           pap (2),
                           chap (3)
                        }  
      ACCESS            read-write
      STATUS            mandatory
      DESCRIPTION
         "The TACACS+ authentication mode.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      OPT
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      NA
            DEFVALUE: 	ascii (1)
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      DEFVAL  {1}
      ::= { tacAuthenServerEntry 7 }
	  
tacAuthenServerSecret OBJECT-TYPE
      SYNTAX		DisplayString (SIZE (6..64))
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
	 "The secret used in encrypting the packets passed between the TACACS+ 
	 authentication server and the client.
         For security reasons, the secret wont be displayed to the users.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      MAND
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      NA
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 8 }

tacAuthenServerTimeoutLimit OBJECT-TYPE
      SYNTAX		TacacsPlusTimeout
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
	 "This is the time of waiting for response from server after TACACS+ 
         authentication message is sent.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      OPT
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      RANGE(1..60)
            DEFVALUE: 	5
            UNITS:	time in seconds 
            SPARSE:	NO 
            DESCR:	NA"
      DEFVAL  {5}
      ::= { tacAuthenServerEntry 9 }

tacAuthenServerAdminState OBJECT-TYPE
      SYNTAX		INTEGER {
                           up(1),
                           down(2)
                        }  
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
	 "The Administrative State of the TACACS+ Authentication Server.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      OPT
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      NA
            DEFVALUE:   up
            UNITS:	NA
            SPARSE:	NO 
            DESCR:	NA"
      DEFVAL  { up }
      ::= { tacAuthenServerEntry 10 }

tacAuthenServerOperState  OBJECT-TYPE
      SYNTAX		INTEGER {
                           up(1),
                           down(2),
  	                   unknown(3)
                        }
      ACCESS		read-only
      STATUS		mandatory
      DESCRIPTION
	 "The Operational State of the TACACS+ Authentication Server.
         Initially the oper state of a server is in unknown. When
         the first user authenticates using this server and if 
         a reply is received, then the state of this server is
         moved to up state. When the server doesnt get a response 
         and goes to down state then the deadinterval timer is 
         started and when it expires, the state again moves to
         unknown. When a user has to be authenticated using this
         server, then the state goes to up state after receiving
         a reply from this server.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      NA
            PERSIST:  	NO
            INSRVMOD: 	NA
            RANGE:      NA
            DEFVALUE: 	NA
            UNITS:	NA
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 11 }

tacAuthenServerDeadInterval     OBJECT-TYPE
      SYNTAX		TacacsPlusDeadInterval
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
	 "Indicates the number of seconds before setting the operational state
	 of this authentication server to unknown from down state. A write to this 
         object will immediately trigger the above mentioned action. If this Interval 
         is set to zero, then the dead interval timer won't be started.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      OPT
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      RANGE(0..3600)
            DEFVALUE: 	600
            UNITS:	in seconds 
            SPARSE:	NO 
            DESCR:	NA"
      DEFVAL  {600}
      ::= { tacAuthenServerEntry 12 }

tacAuthenServerRowStatus       OBJECT-TYPE
      SYNTAX		RowStatus
      ACCESS		read-write
      STATUS		mandatory
      DESCRIPTION
         "This is used for Row creation/Deletion.
         ALCATEL NOTE:
            ACCESS:     NA
            USAGE:      MAND
            PERSIST:  	YES
            INSRVMOD: 	YES
            RANGE:      1,4,6
            DEFVALUE: 	NA
            UNITS:	NA
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerEntry 13 }

-- End of tacAuthenServerTable


-- Start of tacAuthenServerMappingTable

tacAuthenServerMappingTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF TacAuthenServerMappingEntry
      ACCESS     not-accessible
      STATUS     mandatory
      DESCRIPTION
         "The (conceptual) table is a Mapping Table listing the TACACS+
         Authentication Server Names within a Vrf with their equivalent TACACS+ 
         Authentication Server Index used in the TACACS+ Authentication Server Table.
	 This is a Read-Only Table and for each entry in the TACACS+ Authentication
         Server Table, an corresponding entry is created in this table.
         Introduced in ISR5.2, to be supported in ISR5.2.01
         ALCATEL NOTE :
            TABLESIZE : DEP on the number of rows in the tacAuthenServerTable 
            DESCR : NA"     
      ::= { tacacsPlusClientMIB 6 }

tacAuthenServerMappingEntry OBJECT-TYPE
      SYNTAX     TacAuthenServerMappingEntry
      ACCESS 	 not-accessible
      STATUS     mandatory
      DESCRIPTION
         "An entry (conceptual row) representing the TACACS+ Authentication Server Name
         (Within a Vrf) to Index Mapping."
      INDEX      { tacAuthenServerMappingVrf, tacAuthenServerMappingName }
      ::= { tacAuthenServerMappingTable 1 }

TacAuthenServerMappingEntry ::= SEQUENCE {
      tacAuthenServerMappingVrf				TacacsRouterIndex,
      tacAuthenServerMappingName			DisplayString,
      tacAuthenServerMappingIndex			INTEGER
      }

tacAuthenServerMappingVrf OBJECT-TYPE
      SYNTAX     TacacsRouterIndex
      ACCESS 	 not-accessible
      STATUS     mandatory
      DESCRIPTION
         "The tacAuthenServerMappingVrf to which the TACACS+ Authentication Server belongs.
         In case of NE with SHUB, this is the Vrf. 
         In case of NE with IHUB, this is the Service Id of the VPRN or the base router when 0.
         Only base router supported in ISR5.2. 
         ALCATEL NOTE :
            ACCESS:     NA / INDEX
            USAGE:      NA
            PERSIST:  	YES
            INSRVMOD: 	NA
            RANGE:	DEP (1..127) in case of NE with SHUB,
                        (0..2147483647) in case of NE with IHUB
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerMappingEntry 1 }

tacAuthenServerMappingName OBJECT-TYPE
      SYNTAX     DisplayString (SIZE (1..64))
      ACCESS 	 not-accessible
      STATUS     mandatory
      DESCRIPTION
         "A name uniquely identifying a TACACS+ Authentication Server.
         ALCATEL NOTE :
            ACCESS:     NA / INDEX
            USAGE:      NA
            PERSIST:  	YES
            INSRVMOD: 	NA
            RANGE:	NA
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerMappingEntry 2 }

tacAuthenServerMappingIndex OBJECT-TYPE
      SYNTAX     INTEGER
      ACCESS 	 read-only
      STATUS     mandatory
      DESCRIPTION
         "The corresponding Index associated with this Name.
          ALCATEL NOTE :
            ACCESS:     NA
            USAGE:      NA
            PERSIST:  	NO
            INSRVMOD: 	NA
            RANGE:	RANGE(1..256)
            DEFVALUE: 	NA
            UNITS:	NA 
            SPARSE:	NO 
            DESCR:	NA"
      ::= { tacAuthenServerMappingEntry 3 }

-- End of tacAuthenServerMappingTable

