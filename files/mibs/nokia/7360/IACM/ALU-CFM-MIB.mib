--
-- MEP Loss Measurement Test Config Table
--
extendAluCfmLmInitiatorOnDemandTestTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF ExtendAluCfmLmInitiatorOnDemandTestEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
        "This table is for initating and viewing results of on demand  Loss Measurement tests. 
         Steps to initiate on demand Lm test 
         ====================================
         Pre-requisites 
         ============== 
         1)extendAluCfmPmMepLmSingleEndedInitiator must be set to 'enablePerVlan' 
           or 'enablePerVlanPerPbit' for the MEP from which the on-demand Lm test 
           has to be initiated. 

	     2)LT should be reachable when initiating the Lm on-demand test.
         
         Steps
         =====
         1) Create an entry in this table with a valid unicast MAC address as the 
            remote MEP MAC address.
         2) Optionally specify interval,priority and number of packets to send
         3) The Test status will be in-progress if the test has been successfully initiated.             
		    The duration of the test depends on the sendcount and interval
         4) Wait till the test completion time ( ((sendcount-1) *interval) + 5sec).
         5) Then read the test status 
              if status is done read the near-end loss/far-end loss
              else the test has not succeeded.
            
         Note:
         1) If the test status is 'failed' it means that the test was not successfully completed 
         2) The values of interval ,sendcount and priority cannot be modified after row creation
         ALCATEL-NOTE - TABLESIZE 16 "
    REFERENCE "ITU-T Y.1731 Ethernet  Frame Loss Measurement (ETH-Lm)"
    ::= { extendAluCfmPMObjects 3 }

extendAluCfmLmInitiatorOnDemandTestEntry   OBJECT-TYPE
    SYNTAX      ExtendAluCfmLmInitiatorOnDemandTestEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
        "An entry in this table contains the configuration and results
         for on demand single-ended two-way  Loss Measurement (Lm) Tests. "
    INDEX {
              dot1agCfmMdIndex,
              dot1agCfmMaIndex,
              dot1agCfmMepIdentifier,
              extendAluCfmLmInitiatorOnDemandRemoteMepMacAddress
          }

    ::= { extendAluCfmLmInitiatorOnDemandTestTable 1 }

ExtendAluCfmLmInitiatorOnDemandTestEntry   ::= SEQUENCE {
--Test Input
    extendAluCfmLmInitiatorOnDemandRemoteMepMacAddress MacAddress,
    extendAluCfmLmInitiatorOnDemandPriority            INTEGER,
    extendAluCfmLmInitiatorOnDemandInterval            INTEGER,
    extendAluCfmLmInitiatorOnDemandSendCount           INTEGER,
--Test Result
    extendAluCfmLmInitiatorOnDemandTestStatus          INTEGER,
    extendAluCfmLmInitiatorOnDemandPduSent             INTEGER,
    extendAluCfmLmInitiatorOnDemandPduReceived         INTEGER,
    extendAluCfmLmInitiatorOnDemandFramesRxNearEnd     OCTET STRING,
    extendAluCfmLmInitiatorOnDemandFramesRxFarEnd      OCTET STRING,  
    extendAluCfmLmInitiatorOnDemandFramesTxNearEnd     OCTET STRING,
    extendAluCfmLmInitiatorOnDemandFramesTxFarEnd      OCTET STRING,
    extendAluCfmLmInitiatorOnDemandFramesLossNearEnd   OCTET STRING,
    extendAluCfmLmInitiatorOnDemandFramesLossFarEnd    OCTET STRING, 
    extendAluCfmLmInitiatorOnDemandLossRatioNearEnd    ExtendAluCfmHundrethsOfPercentage, 
    extendAluCfmLmInitiatorOnDemandLossRatioFarEnd     ExtendAluCfmHundrethsOfPercentage,
    extendAluCfmLmInitiatorOnDemandPrevTxNearEnd       Counter,
    extendAluCfmLmInitiatorOnDemandPrevRxNearEnd       Counter,
    extendAluCfmLmInitiatorOnDemandPrevTxFarEnd        Counter,
    extendAluCfmLmInitiatorOnDemandPrevRxFarEnd        Counter,
    extendAluCfmLmInitiatorOnDemandCurrTxNearEnd       Counter,
    extendAluCfmLmInitiatorOnDemandCurrRxNearEnd       Counter,
    extendAluCfmLmInitiatorOnDemandCurrTxFarEnd        Counter,
    extendAluCfmLmInitiatorOnDemandCurrRxFarEnd        Counter,	
--Row creation/Test initiation
    extendAluCfmLmInitiatorOnDemandRowStatus            RowStatus
}

