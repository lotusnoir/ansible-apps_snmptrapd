--
-- MEP Synthetic Loss Measurement Test Config Table
--
extendAluCfmSlmInitiatorOnDemandTestTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF ExtendAluCfmSlmInitiatorOnDemandTestEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
        "This table is for initating and viewing results of on demand Synthetic Loss Measurement tests.
         Steps to initiate on demand SLM test
         ====================================
         Pre-requisites
         ==============
         1)extendAluCfmPmMepSlmSingleEndedInitiator must be set to 'true' for the MEP from which
         the on-demand SLM test has to be initiated.
	 2)LT should be reachable when initiating the SLM on-demand test.
         
         Steps
         =====
         1) Create an entry in this table with a valid unicast MAC address as the 
            remote MEP MAC address.
         2) Optionally specify interval,timeout,data size and number of packets to send
         3) The Test status will be in-progress if the test has been successfully initiated. 
            The time taken for the test depends on the timeout,sendcount and the interval.          
            Wait till the test completion time ( ((sendcount-1) *interval) + timeout + 3 sec).
         4) Wait for the test status to change to 'done'. Then read the values of in loss,out loss
            and unacknowledged packets.
         Note:
         1) If the test status is 'failed' it means that the test was not successfully completed 
         2) The values of interval,timeout,datasize and sendcount cannot be modified after row creation
         ALCATEL-NOTE - TABLESIZE 16 "
    REFERENCE "ITU-T Y.1731 Ethernet Synthetic Frame Loss Measurement (ETH-SLM)"
    ::= { extendAluCfmPMObjects 2 }

extendAluCfmSlmInitiatorOnDemandTestEntry   OBJECT-TYPE
    SYNTAX      ExtendAluCfmSlmInitiatorOnDemandTestEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
        "An entry in this table contains the configuration and results
         for on demand single-ended two-way Synthetic Loss Measurement (SLM) Tests. "
    INDEX {
              dot1agCfmMdIndex,
              dot1agCfmMaIndex,
              dot1agCfmMepIdentifier,
              extendAluCfmSlmInitiatorOnDemandRemoteMepMacAddress
          }

    ::= { extendAluCfmSlmInitiatorOnDemandTestTable 1 }

ExtendAluCfmSlmInitiatorOnDemandTestEntry   ::= SEQUENCE {
--Test Input
    extendAluCfmSlmInitiatorOnDemandRemoteMepMacAddress  MacAddress,
    extendAluCfmSlmInitiatorOnDemandPriority             INTEGER,
    extendAluCfmSlmInitiatorOnDemandInterval             INTEGER,
    extendAluCfmSlmInitiatorOnDemandTimeout              INTEGER,
    extendAluCfmSlmInitiatorOnDemandDataSize             INTEGER,
    extendAluCfmSlmInitiatorOnDemandSendCount            INTEGER,
--Test Result
    extendAluCfmSlmInitiatorOnDemandTestStatus           INTEGER,
    extendAluCfmSlmInitiatorOnDemandTestId               Gauge,
    extendAluCfmSlmInitiatorOnDemandRemoteMepId          Dot1agCfmMepIdOrNA,
    extendAluCfmSlmInitiatorOnDemandSlmPacketOut         Counter,
    extendAluCfmSlmInitiatorOnDemandSlrPacketIn          Counter, 
    extendAluCfmSlmInitiatorOnDemandLossIn  	         INTEGER, 
    extendAluCfmSlmInitiatorOnDemandLossOut 	         INTEGER,
    extendAluCfmSlmInitiatorOnDemandUnack   	         INTEGER,
    extendAluCfmSlmInitiatorOnDemandLastTxSeqF           Counter,    
--Row creation/Test initiation
    extendAluCfmSlmInitiatorOnDemandRowStatus            RowStatus
}

extendAluCfmSlmInitiatorOnDemandRemoteMepMacAddress  OBJECT-TYPE
    SYNTAX      MacAddress
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "The value of this object specifies the MAC address  
         used as a target for the two-way Synthetic Loss Measurement (SLM) test.

         This object MUST be set to a valid unicast MAC Address for proper execution of
         the two-way SLM test.
		ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NO       INSRVMOD:   NA
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      NA       SPARSE:     NA
         DESCR:      NA"
    ::= { extendAluCfmSlmInitiatorOnDemandTestEntry 1 }

extendAluCfmSlmInitiatorOnDemandPriority   OBJECT-TYPE
    SYNTAX     INTEGER (0..7)
    ACCESS     read-write
    STATUS     mandatory
    DESCRIPTION
        "The value of this object specifies the priority used
         in the generated test frame for the two-way Synthetic Loss Measurement
         (SLM) test.
		ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NO       INSRVMOD:   NA
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      NA       SPARSE:     NA
         DESCR:      NA"
    DEFVAL { 7 }
	::= { extendAluCfmSlmInitiatorOnDemandTestEntry 2 }

extendAluCfmSlmInitiatorOnDemandInterval OBJECT-TYPE
    SYNTAX      INTEGER (1..10)
    ACCESS  read-write
    STATUS      mandatory
    DESCRIPTION
        "The value of this object specifies 
	 the interval in seconds between Synthetic Loss Measurement (SLM) messages sent 
	 by the two-way SLM test.

	ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NO       INSRVMOD:   NA
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      seconds  SPARSE:     NA
         DESCR:      NA"
    DEFVAL { 5 }
    ::= { extendAluCfmSlmInitiatorOnDemandTestEntry 3 }

extendAluCfmSlmInitiatorOnDemandTimeout     OBJECT-TYPE
    SYNTAX      INTEGER (5..10)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "The value of this object specifies the time-out value,
         in seconds, to wait for a Synthetic Loss Measurement Reply (SLR) message 
         for each SLM message in the two-way SLM test. Upon expiration of the timeout period,
         the MEP assumes that the message response will not be received. Any
         response received after the timeout period has expired is silently
         discarded.
		ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NO       INSRVMOD:   NA
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      seconds  SPARSE:     NA
         DESCR:      NA"
    DEFVAL { 5 }
    ::= { extendAluCfmSlmInitiatorOnDemandTestEntry 4 }

extendAluCfmSlmInitiatorOnDemandDataSize    OBJECT-TYPE
    SYNTAX      INTEGER (0..1500)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "The value of this object specifies the data size,
         in bytes, contained in the padding TLV for the two-way Synthetic Loss
         Measurement (SLM) test. A value of zero (0) specifies that no padding
         TLV is inserted in the SLM packet. Any non-zero value will increase
         the packet size by the specified data size plus 3 bytes for the TLV
         header.
		ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NO       INSRVMOD:   NA
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      bytes    SPARSE:     NA
         DESCR:      NA"
    DEFVAL { 0 }
    ::= { extendAluCfmSlmInitiatorOnDemandTestEntry 5 }

extendAluCfmSlmInitiatorOnDemandSendCount   OBJECT-TYPE
    SYNTAX      INTEGER(1..100)
    ACCESS  read-write
    STATUS      mandatory
    DESCRIPTION
        "The value of this object specifies the number of
         Synthetic Loss Measurement (SLM) packets to send during the two-way
         SLM test.
		ALCATEL NOTE:
         ACCESS:     NA       USAGE:      NA
         PERSIST:    NO       INSRVMOD:   NA
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      packets  SPARSE:     NA
         DESCR:      NA"
    DEFVAL { 1 }
    ::= { extendAluCfmSlmInitiatorOnDemandTestEntry 6 }

extendAluCfmSlmInitiatorOnDemandTestStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                      done(1),
                      inProgress(2),
		      failed(3)
    }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This object can be used to monitor the progress of the SLM test.

         done - The test is complete and the result is available in the result fields such as
                in loss, out loss , unacknowledged packets etc.
         in-progress - The test has been successfully initiated.
         failed -  The test could not be initiated and hence the results will not 
                    be available in the result table
     
         ALCATEL NOTE:
            ACCESS:      NA            USAGE:       NA
            PERSIST:     NO            INSRVMOD:    NA
            RANGE:       NA            DEFVALUE:    NA
            UNITS:       NA            SPARSE:      NA"
   ::= { extendAluCfmSlmInitiatorOnDemandTestEntry 7 }

extendAluCfmSlmInitiatorOnDemandTestId      OBJECT-TYPE
    SYNTAX      Gauge 
    ACCESS      read-only
