extendAluCfmStackTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF ExtendAluCfmStackEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Extend dot1agCfmStackTable:
        There is one CFM Stack table per bridge. It permits
         the retrieval of information about the Maintenance Points
         configured on any given interface.
         Add support to mep lt bridgeport, add equipment as index.
         ALCATEL NOTE:
             TABLESIZE: NA
             DESCR: The stack table lists all the MEPs and MIPs of the 
                   operational LTs in the system. There is no fixed limits;                 
                   it is dependent on the number of MEPs configured and the 
                   number of MIPs applied."
--     REFERENCE   
--         "extend 802.1ag clauses 12.14.2"
    ::= { extendAluCfmMIB 12 }

extendAluCfmStackEntry OBJECT-TYPE
    SYNTAX      ExtendAluCfmStackEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
     "Extend dot1agCfmStackTable:
		The Stack table entry.
       ALCATEL NOTE:
       Row creation : Auto-instantiated as a result of an MP creation.
       Row deletion : Auto-deleted as a result of an MP deletion. 
       Index usage  : See INDEX below.
       lexicographical order : Sorted on StackifIndex,then equipment, then StackVlanIndex,
                               then StackMdLevel, and then StackDirection.
            dot1agCfmStackVlanIdOrNone has been replaced with 
            dot1agCfmStackVlanIndex in order to support S+C notation."
    INDEX { extendAluCfmStackifIndex,extendAluCfmStackEquipment, extendAluCfmStackVlanIndex, 
                  extendAluCfmStackMdLevel, extendAluCfmStackDirection }
    ::= { extendAluCfmStackTable 1 }

    ExtendAluCfmStackEntry ::= SEQUENCE {
    extendAluCfmStackifIndex          InterfaceIndex,
    extendAluCfmStackEquipment		   AluCfmMepEquipmentType,
    extendAluCfmStackVlanIndex        VlanIndex,
    extendAluCfmStackMdLevel          Dot1agCfmMDLevel,
    extendAluCfmStackDirection        Dot1agCfmMpDirection,
    extendAluCfmStackMdIndex          Gauge,
    extendAluCfmStackMaIndex          Gauge,
    extendAluCfmStackMepId            Dot1agCfmMepIdOrZero,
    extendAluCfmStackMacAddress       MacAddress
}

extendAluCfmStackifIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Extend dot1agCfmStackTable:
		This object represents the  Bridge Port or aggregated port
         on which MEPs or MHFs might be configured.
         ALCATEL NOTE:
                ACCESS:     NA       USAGE:      NA
                PERSIST:    NO       INSRVMOD:   NO
                RANGE:      NA       DEFVALUE:   NA
                UNITS:      NA       SPARSE:     NA
                DESCR:      NA"
--     REFERENCE   
--         "802.1ag clause 12.14.2.1.2:a"
    ::= { extendAluCfmStackEntry 1 }

extendAluCfmStackEquipment OBJECT-TYPE
   SYNTAX AluCfmMepEquipmentType
   ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
         " Identifies the equpment type of mep ;
         	0 is for Gpon onu and Dsl ;
         	1 is for Gpon Lt ;
			2 is for Gpon onu;
         	
 		 ALCATEL NOTE:
                            ACCESS:     RW       USAGE:      OPT
                            PERSIST:    YES      INSRVMOD:   NA
                            RANGE:      NA       DEFVALUE:   NA 
                            UNITS:      NA       SPARSE:     NA
                 "
  DEFVAL { default}
