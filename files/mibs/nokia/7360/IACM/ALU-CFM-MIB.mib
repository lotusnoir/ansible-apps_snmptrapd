-- ************************************************************************
-- GENERAL NOTES:
-- (1) There are NO traps in this MIB.
--
-- (2) There are NO alarms in this MIB.
-- 
-- ************************************************************************
-- .iso.org.dod.internet.private.enterprises.alcatel.asd.asam.cfmMIB
-- (.1.3.6.1.4.1.637.61.1.76)

extendAluCfmMIB OBJECT IDENTIFIER ::= {asam 76}

AluCfmMaCcmAdminState ::=
    INTEGER { unlock(1), lock(2) }


AluCfmMepEquipmentType ::=
		INTEGER { default(0), olt(1),onu(2)}
		
AluCfmMaMhfLocationType ::=
		INTEGER { default(1), ltuplink(2), vbp(3), onu(4), ltuplink-vbp(5), ltuplink-onu(6), vbp-onu(7), ltuplink-vbp-onu(8)}		
		
-- Based upon LldpChassisIdSubtype definition FROM IEEE8021-CFM-MIB
AluLldpChassisIdSubtype ::=
	INTEGER {notApplicable(0), chassisComponent(1), interfaceAlias(2), portComponent(3),
             macAddress(4), networkAddress(5), interfaceName(6),
             local(7)}

-- Based upon Dot1agCfmIngressActionFieldValue definition FROM IEEE8021-CFM-MIB
AluDot1agCfmIngressActionFieldValue ::=
    INTEGER { ingNoTlv(0), ingOk(1), ingDown(2), ingBlocked(3), ingVid(4) }

-- Based upon LldpPortIdSubtype definition FROM IEEE8021-CFM-MIB
AluLldpPortIdSubtype ::=
	INTEGER {none(0), interfaceAlias(1), portComponent(2), macAddress(3),
             networkAddress(4), interfaceName(5), agentCircuitId(6),
             local(7)}

-- Based upon Dot1agCfmEgressActionFieldValue definition FROM IEEE8021-CFM-MIB
AluDot1agCfmEgressActionFieldValue ::=
    INTEGER { egrNoTlv(0), egrOK(1), egrDown(2), egrBlocked(3), egrVid(4) }


--A value expressed in Hundreths of Percentage defined to two decimal places.
--For eg:where 1 = 0.01 , 2=0.02 ..100=1 10000=100
--If value is 65535, it means that the percentage is invalid (greater than 100 or less than 0)
ExtendAluCfmHundrethsOfPercentage ::=
    INTEGER (0..10000 | 65535)    

--Type of the Y1731 test.
ExtendAluCfmY1731TestType ::=
    INTEGER {
             single-ended-slm (1),
		     single-ended-lm  (2)                    
            }
--A valid mep ID or 65535 if it is not applicable.
-- 0 if Unavailable 
Dot1agCfmMepIdOrNA ::=
    Gauge (0..8191 | 65535)
-- ***********************************************************************
--    Extend CFM MEP Table
-- ***********************************************************************
extendAluCfmMepTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ExtendAluCfmMepEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
                "This table contains the proprietary MEP objects"
        ::= { extendAluCfmMIB 2 }

extendAluCfmMepEntry OBJECT-TYPE
        SYNTAX ExtendAluCfmMepEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "This list contains the proprietary objects for a MEP."
--      AUGMENTS { dot1agCfmMepEntry }
        INDEX { dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier }
        ::= { extendAluCfmMepTable 1}

ExtendAluCfmMepEntry ::=
        SEQUENCE {
                extendAluCfmMepSvlanId INTEGER,
                extendAluCfmMepPbits INTEGER,
                extendAluCfmMepCvlanId INTEGER,
                extendAluCfmPmMepSlmSingleEndedResponder TruthValue,
                extendAluCfmPmMepDmSingleEndedResponder TruthValue,
	        			extendAluCfmPmMepLmSingleEndedResponder INTEGER,
	        			extendAluCfmPmMepSlmSingleEndedInitiator TruthValue,
	      			  extendAluCfmPmMepLmSingleEndedInitiator INTEGER,
	      			  extendAluCfmMepAISEnable	TruthValue,
	      			  extendAluCfmMepAISMegLevel INTEGER,
	      			  extendAluCfmMepAISPeriod	 INTEGER,
	      			  extendAluCfmMepAISPriority	INTEGER,
	      			  extendAluCfmMepEquipment	AluCfmMepEquipmentType
	      	  
        }

extendAluCfmMepSvlanId OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
                "This is the S-VLAN ID to be used in LBM or LTM packets
                 from the MEP. If the S-VLAN ID is 4095, untagged packets
                 will be transmitted.
         ALCATEL NOTE:
                ACCESS:     NA       USAGE:      NA
                PERSIST:    NA       INSRVMOD:   NA
                RANGE:      NA       DEFVALUE:   NA
                UNITS:      NA       SPARSE:     NA
                DESCR:      This object is not supported by ISAM"
        DEFVAL {4095}
        ::={ extendAluCfmMepEntry 1}

extendAluCfmMepPbits OBJECT-TYPE
        SYNTAX INTEGER (0..7)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
                "This is the pbits to be used in S-VLAN and C-VLAN tags.
         ALCATEL NOTE:
                ACCESS:     NA       USAGE:      NA
                PERSIST:    NA       INSRVMOD:   NA
                RANGE:      NA       DEFVALUE:   NA
                UNITS:      NA       SPARSE:     NA
                DESCR:      This object is not supported by ISAM"

        DEFVAL {0}
        ::={ extendAluCfmMepEntry 2}

extendAluCfmMepCvlanId OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
              "This is the C-VLAN ID to be used in LBM or LTM packets from
               the MEP. The p-bits will be the same as in the outer VLAN 
               tag. If the C-VLAN ID is 4095, the inner VLAN tag is not
               transmitted.
         ALCATEL NOTE:
                ACCESS:     NA       USAGE:      NA
                PERSIST:    NA       INSRVMOD:   NA
                RANGE:      NA       DEFVALUE:   NA
                UNITS:      NA       SPARSE:     NA
                DESCR:      This object is not supported by ISAM"
        DEFVAL {4095}
        ::={ extendAluCfmMepEntry 3}

extendAluCfmPmMepSlmSingleEndedResponder OBJECT-TYPE	
    SYNTAX TruthValue
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		
         "This object specifies whether the Synthetic Loss Measurement (SLM)
		 single-ended Responder is enabled.

		 The value 'true' indicates the single-ended SLM Responder is enabled and
		 if a SLM message is received a SLR will be sent in reply.
		 The value 'false' indicates the single-ended SLM Responder is disabled.
		 If a SLM message is received no response will be sent and the message
		 will be discarded.

		 A MEP can be both a single-ended Responder and Controller simultaneously.
		 
                 ALCATEL NOTE:
                            ACCESS:     RW       USAGE:      OPT
                            PERSIST:    YES      INSRVMOD:   NA
                            RANGE:      NA       DEFVALUE:   NA
                            UNITS:      NA       SPARSE:     NA
                "
	 DEFVAL { false }
  ::={ extendAluCfmMepEntry 4}

extendAluCfmPmMepDmSingleEndedResponder OBJECT-TYPE
	 SYNTAX TruthValue
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
		 "This object specifies whether the Delay Measurement (DMM) single
		 ended Responder for a two-way delay measurement is enabled.

		 The value 'true' indicates the single-ended Delay Measurement Responder
		 is enabled and if a DMM message is received a DMR will be sent in reply.

		 The value 'false' indicates the single-ended Delay Measurement Responder
		 is disabled. If a DMM message is received no response will be sent and
		 the message will be discarded.

		 A MEP can be both a single-ended Responder and Controller simultaneously.

 		  ALCATEL NOTE:
                            ACCESS:     RW       USAGE:      OPT
                            PERSIST:    YES      INSRVMOD:   NA
                            RANGE:      NA       DEFVALUE:   NA
                            UNITS:      NA       SPARSE:     NA
                 "
	 DEFVAL { false }
::= { extendAluCfmMepEntry 5 }

extendAluCfmPmMepLmSingleEndedResponder OBJECT-TYPE
         SYNTAX  INTEGER 
    		{
                 disable (0),
                 enablePerVlan (1),
                 enablePerVlanPerPbit (2)
                }
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
                "This object specifies whether the Loss Measurement single
		 ended Responder for a two-way loss measurement is 
		 enabled per vlan (or) enabled per vlan per priority bit (or)
                 disabled.

		 The value 'disable' indicates the single-ended Loss Measurement Responder
		 is disabled. If a LMM message is received no response will be sent and
		 the message will be discarded.
		 A MEP can be both a single-ended Responder and Initiator 
		 simultaneously.
                 
		 1)This object can be set to either enablePerVlan or 
		    enablePerVlanPerPbit, only when the mep is associated to a
		    cross-connect vlan, which is already created

		 2)This object can be set to either enablePerVlan or
		    enablePerVlanPerPbit, only when the mep is associated to CATAN
		    based LTs

		 3)The limit for the maximum number of MEPs with this object set
		   to either enablePerVlan or enablePerVlanPerPbit in a specified
		   LT is the value of the capability key CFM_MAX_LMRESPONDER_ENABLED_MEPS  
           	   for that LT.

		 4)MEPs in all the domain levels associated to the same vlan in a
		   specified LT, should be set to the same value for this object,
		   when this object is enabled

         	 5) The value of this object and extendAluCfmPmMepLmSingleEndedInitiator 
            	    must be same for all the MEPs in the same vlan.
                 
		 Error mib will be updated with appropriate error codes, when any
		 of the above conditions is not satisfied.

 		 ALCATEL NOTE:
                            ACCESS:     RW       USAGE:      OPT
                            PERSIST:    YES      INSRVMOD:   NA
                            RANGE:      NA       DEFVALUE:   NA 
                            UNITS:      NA       SPARSE:     NA
                 "
	 DEFVAL { disable }
::= { extendAluCfmMepEntry 6 }
extendAluCfmPmMepSlmSingleEndedInitiator OBJECT-TYPE	
    	SYNTAX TruthValue
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		
         	"This object specifies whether the Synthetic Loss Measurement (SLM)
		 single-ended Initiator is enabled.

		 The value 'true' indicates that the single-ended SLM Initiator is enabled 
         and an SLM message can be sent from this MEP
		 
         The value 'false' indicates that the single-ended SLM Initiator is 
         disabled and an SLM message cannot be sent from this MEP.
		  
		 A MEP can be both a single-ended initiator and responder simultaneously.
                 ALCATEL NOTE:
                            ACCESS:     RW       USAGE:      OPT
                            PERSIST:    YES      INSRVMOD:   NA
                            RANGE:      NA       DEFVALUE:   NA
                            UNITS:      NA       SPARSE:     NA
                "
	 DEFVAL { false }
  ::={ extendAluCfmMepEntry 7}

extendAluCfmPmMepLmSingleEndedInitiator OBJECT-TYPE
         SYNTAX  INTEGER 
    		{
                 disable (0),
                 enablePerVlan (1),
                 enablePerVlanPerPbit (2)
                }
	 ACCESS read-write
	 STATUS mandatory
	 DESCRIPTION
         "This object specifies whether the Loss Measurement single
		 ended initiator for a two-way loss measurement is 
		 enabled per vlan or per vlan per priority bit or disabled.

		 The value 'disable' indicates the single-ended Loss Measurement Initiator
		 is disabled and an LMM message cannot be sent from this mep.

		 A MEP can be both a single-ended Responder and Initiator 
		 simultaneously.
		 1)This object can be set to either enablePerVlan or 
		    enablePerVlanPerPbit, only when the mep is associated to a
		    cross-connect vlan, which is already created

		 2)This object can be set to either enablePerVlan or
		    enablePerVlanPerPbit, only when the mep is associated to CATAN
		    based  LTs

		 3)The limit for the maximum number of MEPs with this object set
		   to either enablePerVlan or enablePerVlanPerPbit in a specified
		   LT is the value of the capability key CFM_MAX_LMINITIATOR_ENABLED_MEPS  
           	   for that LT

		 4)MEPs in all the domain levels associated to the same vlan in a
		   specified LT, should be set to the same value for this object,
		   when this object is enabled

         	5) The value of this object and extendAluCfmPmMepLmSingleEndedResponder 
                   Must be same for MEPs in the same vlan.
                 
		 Error mib will be updated with appropriate error codes, when any
		 of the above conditions is not satisfied.

 		 ALCATEL NOTE:
                            ACCESS:     RW       USAGE:      OPT
                            PERSIST:    YES      INSRVMOD:   NA
                            RANGE:      NA       DEFVALUE:   NA 
                            UNITS:      NA       SPARSE:     NA
                 "
	 DEFVAL { disable }
::= { extendAluCfmMepEntry 8 }

      	  
	      	  
