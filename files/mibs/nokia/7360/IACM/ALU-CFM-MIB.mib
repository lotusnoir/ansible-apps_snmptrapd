-- ***********************************************************************
--     Extend CFM MA Table
-- ***********************************************************************

extendAluCfmMaTable OBJECT-TYPE
    	SYNTAX      SEQUENCE OF ExtendAluCfmMaEntry
    	ACCESS      not-accessible
    	STATUS      mandatory
    	DESCRIPTION 
        	"The ALU exension to the standard Maintenance Association 
		 table.  Each row in the table represents an MA. 
		 An MA is a set of MEPs, each configured with a single 
		 service instance. A row in this table is automatically 
		 created/ deleted when a corresponding row in 
		 dot1agCfmMaTable is created/ deleted. 
         ALCATEL NOTE:
        	TABLESIZE: 20000
             	DESCR: ISAM supports at most 20000 MAs, except for SHUB 
             	based NTs, where the table size is 2700. Active CCM would 
                be supported from ISR4.5.0.2 onwards."
 	REFERENCE   
 		"802.1ag clause 18.2"
	::={ extendAluCfmMIB 5}

extendAluCfmMaEntry OBJECT-TYPE
    	SYNTAX      ExtendAluCfmMaEntry
    	ACCESS      not-accessible
    	STATUS      mandatory
    	DESCRIPTION 
        	"The MA table entry.
         ALCATEL NOTE:
            	Row creation : Instantiated as a result of an MA creation.
            	Row deletion : Deleted as a result of an MA deletion. 
            	Index usage  : See INDEX below.
            	lexicographical order : Sorted on MdIndex and then MaIndex."
    	INDEX { dot1agCfmMdIndex, dot1agCfmMaIndex }
    	::={ extendAluCfmMaTable 1}

ExtendAluCfmMaEntry ::= 
	SEQUENCE {
    		extendAluCfmMaBridgeport    InterfaceIndexOrZero,
	        extendAluCfmMaCcmAware      TruthValue,
	        extendAluCfmMaCcmAdminState AluCfmMaCcmAdminState,
            extendAluCfmMaLtmFilter     TruthValue,
            extendAluCfmMaDualTagAware  TruthValue,
            extendAluCfmMaMhfLocation   AluCfmMaMhfLocationType
}

extendAluCfmMaBridgeport OBJECT-TYPE
    	SYNTAX      InterfaceIndexOrZero
    	ACCESS      read-write
    	STATUS      mandatory
    	DESCRIPTION 
        	"The Bridgeport corresponding the UNI where only an outward 
	  	 facing MEP may be created. If the value of this object 
		 is 0 (default), it is an MA where a MIP or inward facing MEP 
		 at the UNI can be created, or where an outward facing MEP 
		 at an LT's NT interface can be created. If an non-zero 
		 IfIndex is provided, then it must be the ifIndex of a 
		 BridgePort on a UNI and it means this is an access link MA 
		 and only an outward facing MEP can be created on one 
		 particular UNI whose IfIndex is provided. If the value of 
		 this object is 0, it means it is a generic MA for which 
		 multiple MEPs on multiple ONTs can be created or where 
		 outward facing MEPs at an LT-NT interface can be created. 
		 The value of this object cannot be modified after the row 
		 is created. 
         ALCATEL NOTE:
                ACCESS:     NA       USAGE:      OPT 
                PERSIST:    YES      INSRVMOD:   NO 
                RANGE:      NA       DEFVALUE:   0
                UNITS:      NA       SPARSE:     NA
                DESCR:      NA"
        DEFVAL {0}
    	::={ extendAluCfmMaEntry 1}

extendAluCfmMaCcmAware OBJECT-TYPE
    	SYNTAX      TruthValue
    	ACCESS      read-write
    	STATUS      mandatory
    	DESCRIPTION 
          "This object is used in order to enable CCM configuration
           within the related MA.
          ALCATEL NOTE:
            ACCESS:     NA       USAGE:      OPT 
            PERSIST:    YES      INSRVMOD:   YES 
            RANGE:      NA       DEFVALUE:   NA
            UNITS:      NA       SPARSE:     NA
            DESCR:
              A CCM aware MA is one with:
              extendAluCfmMaCcmAware equal to true.
              Any change in this object is only possible for a 
              CCM locked MA (see extendAluCfmMaCcmAdminState).
              A transition of this object from true to false 
              will stop processing the received CCM frames. 
              A transition of this object from false to true 
              will start processing of the received CCM frames."
        DEFVAL {false}
    	::={ extendAluCfmMaEntry 2}

extendAluCfmMaCcmAdminState OBJECT-TYPE
    	SYNTAX      AluCfmMaCcmAdminState
    	ACCESS      read-write
    	STATUS      mandatory
    	DESCRIPTION 
          "This object is used in order to activate CCM
           for the related MA (transmitting and receiving CCM).
          ALCATEL NOTE:
            ACCESS:     NA       USAGE:      OPT 
            PERSIST:    YES      INSRVMOD:   YES 
            RANGE:      NA       DEFVALUE:   NA
            UNITS:      NA       SPARSE:     NA
            DESCR:
              A locked CCM MA is one with:
              extendAluCfmMaCcmAdminState equal to lock.
              In the lock state, configuration settings for CCM
              can be done inside :
              dot1agCfmMdTable, dot1agCfmMaTable, dot1agCfmMepTable, 
              dot1agCfmMaMepListTable.
              A transition of this object from unlock to lock
              will
              0/freeze all CCM processing within the MA
              (transmitting, monitoring, alarm generation)
              1/clear outstanding CCM alarms
              For all CCI enabled local MEPS, it will cause:
              2/clearing all available CCM transmit information, i.e. 
              dot1agCfmMepCciSentCcms, etc.
              3/clearing all available CCM receive information, i.e. 
              dot1agCfmMepErrorCcmLastFailure, dot1agCfmMepXconCcmLastFailure, 
              dot1agCfmMepFngState, dot1agCfmMepHighestPrDefect, dot1agCfmMepDefects, 
              dot1agCfmMepCcmSequenceErrors, etc.
              4/clearing entries inside dot1agCfmMepDbTable.
              A transition of this object from lock to unlock
              will start all CCM processing and result reporting within the MA."
        DEFVAL {lock}
    	::={ extendAluCfmMaEntry 3}

extendAluCfmMaLtmFilter OBJECT-TYPE
         SYNTAX      TruthValue
         ACCESS      read-write
         STATUS      mandatory
         DESCRIPTION 
            "This object is used in order to filter out upstream LTM messages
             within the related MA.
         ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      OPT 
                 PERSIST:    YES      INSRVMOD:   YES 
                 RANGE:      NA       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:
                   Upstream LTM messages at the same level as a MIP 
                   are filtered (dropped) by default 
                   within the ISAM as the CPE is generally considered a 
                   non-trusted device. When a customer uses authenticated 
                   CPEs,upstream LTM message filtering can be disabled within 
                   the related MA.This object is not applicable for GPON EPON
                   NELT-B NNI HC-UNI."
         DEFVAL {true}
         ::= { extendAluCfmMaEntry 4}

extendAluCfmMaDualTagAware OBJECT-TYPE
         SYNTAX      TruthValue
         ACCESS      read-write
         STATUS      mandatory
         DESCRIPTION 
            "This object is used in order to enable the processing of
             dual tagged CFM messages on single tag vlan cross connect 
             model within the related MA.
         ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      OPT 
                 PERSIST:    YES      INSRVMOD:   YES 
                 RANGE:      NA       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                 DESCR:
                   When enabled, CFM processing will occur for messages whose 
                   outer (Svlan) tag has a related MA, regardless of the 
                   presence of an inner Cvlan tag. CFM message processing 
                   will occur against the combination of the Svlan and any 
                   Cvlan tag.
                   When disabled, CFM packets with two vlan tags in a C-VLAN CC
                   will not be processed.
		   Configurable on all forwarding models, but applicable only  
		   for C-Vlan CC.
                   Not applicable to GPON EPON NELT-B NNI HC-UNI"
         DEFVAL {false}
         ::= { extendAluCfmMaEntry 5}
         
extendAluCfmMaMhfLocation OBJECT-TYPE
	    SYNTAX AluCfmMaMhfLocationType
	    ACCESS read-write
		STATUS mandatory
		DESCRIPTION
	         " Identifies the mhf location type of ma ;
	         	1 : default value, for Gpon, MIP is set on onu; for DSL, MIP is set on lt uplink;
	         	2 : MIP is set on lt uplink;
				3 : MIP is set on lt virtual bridge port;
				4 : MIP is set on gpon onu
				5 : MIP is set on both Lt uplink and virtual bridgeport
				6 : MIP is set on both Lt uplink and ONU
				7 : MIP is set on both lt virtual bridgeport and ONU
				8 : MIP is set on lt uplink, virtual bridgeport and ONU
	         	
	 		 ALCATEL NOTE:
	                            ACCESS:     RW       USAGE:      OPT
	                            PERSIST:    YES      INSRVMOD:   NA
	                            RANGE:      NA       DEFVALUE:   NA 
	                            UNITS:      NA       SPARSE:     NA
	                 "
	    DEFVAL { default}
		::= { extendAluCfmMaEntry 6} 
         

-- ***********************************************************************
--     CFM MEP OamCtlTable
-- ***********************************************************************
extendAluCfmMepOamCtlTableNextIndex OBJECT-TYPE
    SYNTAX      Dot1afCfmIndexIntegerNextFree
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
      "This object contains an unused value for extendAluCfmMepOamCtlTestIndex 
       in the extendAluCfmMepOamCtlTable,
       or a zero to indicate that none exist.
       ALCATEL NOTE:
          ACCESS:      NA            USAGE:       NA
          PERSIST:     NO            INSRVMOD:    NA
          RANGE:       NA            DEFVALUE:    NA
          UNITS:       NA            SPARSE:      NA
          DESCR:       NA" 
      ::= { extendAluCfmMIB 6 }

extendAluCfmMepOamCtlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ExtendAluCfmMepOamCtlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
          "Primary controls and status for the OAM capabilities of a MEP.
          It replaces managed objects of dot1agCfmMepTable for
          configuring, activating, result monitoring of following 
          oam protocol operations:
            uc LBM: unicast Loopback Message
            mc LBM: multicast Loopback Message
            LTM: Linktrace Message
          The results of a completed uc LBM and LTM (partially) test are
	  also contained in this table.
          Entries are created to initiate transmission of either uc LBM,
          mc LBM or of LTM with particular test settings.
          Availability of test results can be monitored via the
          extendAluCfmMepOamCtlResult. If this variable indicates
          test completion:
          * uc LBM results can be retrieved in this table
          * mc LBM results can be retrieved in extendAluCfmMepMcLbrTable
          * LTM results can be retrieved in extendAluCfmMepLtrTable and
	    in this table
          Modifying settings for an ongoing test is not possible.
          Deleting an entry for a test in this table will clear
          associated results. 
		  This table is supported only from ISR4.4 onwards.
          ALCATEL NOTE:
           TABLESIZE:     1..16(simultaneous number of OAM sessions)"
        ::= { extendAluCfmMIB 7 }

extendAluCfmMepOamCtlEntry OBJECT-TYPE
        SYNTAX ExtendAluCfmMepOamCtlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION 
              "An entry in the table models a test activated on a
               particular MEP.
               Only one oam test can be active on a MEP.
	       Only two oam tests can be active on an LT."
        INDEX { extendAluCfmMepOamCtlTestIndex } 
        ::= { extendAluCfmMepOamCtlTable 1}

ExtendAluCfmMepOamCtlEntry ::= SEQUENCE {
    extendAluCfmMepOamCtlTestIndex                    Gauge,
    extendAluCfmMepOamCtlMdIndex                      Gauge, 
    extendAluCfmMepOamCtlMaIndex                      Gauge, 
    extendAluCfmMepOamCtlMepIdentifier                Dot1agCfmMepId, 
    extendAluCfmMepOamCtlTestType                     INTEGER,
    extendAluCfmMepOamCtlTransmitTargetMacAddress     MacAddress,
    extendAluCfmMepOamCtlTransmitLbmMessages          INTEGER,
    extendAluCfmMepOamCtlTransmitLbmTlvType           INTEGER,
    extendAluCfmMepOamCtlTransmitLbmDataLen           INTEGER,
    extendAluCfmMepOamCtlTransmitLbmDataBitPattern    OCTET STRING,
    extendAluCfmMepOamCtlTransmitLbmTestBitPattern    INTEGER,
    extendAluCfmMepOamCtlTransmitVlanPriority         INTEGER,
    extendAluCfmMepOamCtlTransmitLbmVlanDropEnable    INTEGER,
    extendAluCfmMepOamCtlTransmitLbmVlanDropPcp       INTEGER,
    extendAluCfmMepOamCtlTransmitLtmTtl               Gauge,
    extendAluCfmMepOamCtlTransmitLtmUseFdbOnly	      TruthValue,
    extendAluCfmMepOamCtlTransmitLtmEgressIdentifier  OCTET STRING,
    extendAluCfmMepOamCtlTransmitLtmSeqNumber	      Counter,
    extendAluCfmMepOamCtlResult                       INTEGER,
    extendAluCfmMepOamCtlLbrIn                        Counter,
    extendAluCfmMepOamCtlLbrInOutOfOrder              Counter,
    extendAluCfmMepOamCtlLbrBadMsdu                   Counter,
    extendAluCfmMepOamCtlLbrBitErrorRatio             INTEGER,
    extendAluCfmMepOamCtlLbrCrcError                  Counter,
    extendAluCfmMepOamCtlUnexpLtrIn		      Counter,
    extendAluCfmMepOamCtlValidResults                 Counter,
    extendAluCfmMepOamCtlRowStatus                    RowStatus,
    extendAluCfmMepOamCtlLbrBitError                  Counter
}

extendAluCfmMepOamCtlTestIndex OBJECT-TYPE
    SYNTAX      Gauge (1..4294967295)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
      "This is a unique test index.
      extendAluCfmMepOamCtlTableNextIndex can be inspected to find an
      available index for row-creation.
      
      ALCATEL NOTE:
        ACCESS:     NA       USAGE:      MAND
        PERSIST:    NO       INSRVMOD:   NO
        RANGE:      (1..16)  DEFVALUE:   NA
        UNITS:      NA       SPARSE:     NA
        DESCR:      NA"
    ::= { extendAluCfmMepOamCtlEntry  1 }

extendAluCfmMepOamCtlMdIndex OBJECT-TYPE
    SYNTAX      Gauge (1..4294967295)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "This is the Maintenance Domain index.
       
       ALCATEL NOTE:
         ACCESS:     NA       USAGE:      MAND
         PERSIST:    NO       INSRVMOD:   NO
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      NA       SPARSE:     NA
         DESCR:      NA"
    ::= { extendAluCfmMepOamCtlEntry  2 }

extendAluCfmMepOamCtlMaIndex OBJECT-TYPE
    SYNTAX      Gauge (1..4294967295)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "This is the Maintenance Association index.
      
       ALCATEL NOTE:
         ACCESS:     NA       USAGE:      MAND
         PERSIST:    NO       INSRVMOD:   NO
         RANGE:      NA       DEFVALUE:   NA
         UNITS:      NA       SPARSE:     NA
         DESCR:      NA"
    ::= { extendAluCfmMepOamCtlEntry 3 }

extendAluCfmMepOamCtlMepIdentifier OBJECT-TYPE
    SYNTAX      Dot1agCfmMepId
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "This is the MEP Identifier.
      ALCATEL NOTE:
        ACCESS:     NA       USAGE:      MAND
        PERSIST:    NO       INSRVMOD:   NO
        RANGE:      NA       DEFVALUE:   NA
        UNITS:      NA       SPARSE:     NA
        DESCR:      NA"
    ::= { extendAluCfmMepOamCtlEntry 4 }

extendAluCfmMepOamCtlTestType  OBJECT-TYPE
    SYNTAX      INTEGER {
                      ucLbm(1),
                      mcLbm(2),
                      ltm(3)
    }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "ALCATEL NOTE:
         ACCESS:      NA            USAGE:       MAND
         PERSIST:     NO            INSRVMOD:    NO
         RANGE:       NA            DEFVALUE:    NA
         UNITS:       NA            SPARSE:      NA
         DESCR:       
           Specifies the type of OAM test:
             ucLbm: unicast Loopback Message
             mcLbm: multicast Loopback Message
             ltm:   Linktrace Message"
    ::= { extendAluCfmMepOamCtlEntry 5 }

extendAluCfmMepOamCtlTransmitTargetMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 

      "The MAC Address for the OAM target: a unicast destination
      MAC address.
      ALCATEL NOTE:
        ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       NA            DEFVALUE:    NA
        UNITS:       NA            SPARSE:      NO
        DESCR:
          For extendAluCfmMepOamCtlTestType mcLbm(see ITU-T Y.1731 Table 10-1),  
          as a predefined Multicast address is always used as destination
          MAC address(calculated by the system), this object is NA
          (and default value is filled out).
          For extendAluCfmMepOamCtlTestType ucLbm,
          (see 802.1ag clause 12.14.7.3.2:b)it is used as destination 
          MAC address in the LBM.
          For extendAluCfmMepOamCtlTestType ltm,
          (see 802.1ag clause 12.14.7.4.2:c) it is used for target
          MAC address within the payload of the LTM. The destination
          MAC address is calculated by the system."
     REFERENCE   
       "802.1ag clause 12.14.7.3.2:b
       802.1ag clause 12.14.7.4.2:c"
    DEFVAL { '000000000000'H }
    ::= { extendAluCfmMepOamCtlEntry 6 }

extendAluCfmMepOamCtlTransmitLbmMessages  OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "The number of Loopback messages to be transmitted.
      ALCATEL NOTE:
        ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       (1..5)        DEFVALUE:    NA
        UNITS:       NA            SPARSE:      NO
        DESCR:       
          Applicable only for uc LBM operation. 
	  Applicable for all extendAluCfmMepOamCtlTransmitLbmTlvType.
          Default value is filled out if NA.
          The interval between subsequent transmitted LBMs
          is fixed: 250ms."

      REFERENCE   
          "802.1ag clause 12.14.7.3.2:c"
    DEFVAL      { 1 }
    ::= { extendAluCfmMepOamCtlEntry 7 }

extendAluCfmMepOamCtlTransmitLbmTlvType	  OBJECT-TYPE
    SYNTAX      INTEGER {
		      none(1),
		      dataTlv(2),
		      testTlv(3)
    }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
      "Specifies which TLV type is to be used for LBM.
      ALCATEL NOTE:
        ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       NA            DEFVALUE:    NA
        UNITS:       NA            SPARSE:      NO
        DESCR:
          Only applicable for extendAluCfmMepOamCtlTestType ucLbm and mcLbm.
	  ucLbm supports all TLV types. mcLbm supports only none(1) and dataTlv(2).
          To be interpreted w.r.t. extendAluCfmMepOamCtlTransmitLbmMessages.
	  The value testTlv is not supported in ISAM."
	REFERENCE
	    "ITU-T Y.1731 clause 9.3.2"
    DEFVAL      { 1 }
    ::= { extendAluCfmMepOamCtlEntry 8 }

extendAluCfmMepOamCtlTransmitLbmDataLen OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "For dataTlv, the length specifies the size in octets of the 
       value field. For testTlv, the length specifies the size in octets 
       of the Test pattern and CRC-32 field.
       If CRC-32 is used with the testTlv, the length must be at least 4.
       Default value is filled out if NA.
      ALCATEL NOTE:
        ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       (1..1488)     DEFVALUE:    NA
        UNITS:       bytes         SPARSE:      NO
        DESCR: 
	  Only applicable for extendAluCfmMepOamCtlTestType ucLbm, mcLbm and
          extendAluCfmMepOamCtlTransmitLbmTlvType dataTlv, testTlv."
     REFERENCE   
         "802.1ag clause 12.14.7.3.2:d"
    DEFVAL      { 64 }
    ::= { extendAluCfmMepOamCtlEntry 9 }

extendAluCfmMepOamCtlTransmitLbmDataBitPattern OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(8))
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "This 64 bit pattern is used repetitively to fillup Data TLV,
       if the Data TLV is selected to be sent.
       Most significant bits are used first.
       Applicable only for LBM operation for non zero value of
       extendAluCfmMepOamCtlTransmitLbmDataLen. Applicable only
       for extendAluCfmMepOamCtlTransmitLbmTlvType dataTlv(2).
       Default value is filled out if NA.
      ALCATEL NOTE:
        ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       NA            DEFVALUE:    NA
        UNITS:       bytes         SPARSE:      NO
        DESCR: "      
     REFERENCE   
         "802.1ag clause 12.14.7.3.2:d"
    DEFVAL          {'0000000000000000'H }
    ::= { extendAluCfmMepOamCtlEntry 10 }

extendAluCfmMepOamCtlTransmitLbmTestBitPattern	OBJECT-TYPE
    SYNTAX	INTEGER {
		      allZerosNoCrc(0),
		      allZerosCrc(1),
		      prbsNoCrc(2),
		      prbsCrc(3),
		      allOnesNoCrc(4),
		      allOnesCrc(5)
    }
    ACCESS	read-write
    STATUS	mandatory
    DESCRIPTION
      "Range of accepted bit pattern types for the Test TLV. 
       prbsNoCrc and prbsCrc make use of 
       ITU-T O.150 PRBS (2^31)-1.
      ALCATEL NOTE:
   	ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       3             DEFVALUE:    3
        UNITS:       NA            SPARSE:      NO
        DESCR:
           Only applicable for extendAluCfmMepOamCtlTestType ucLbm and 
	       extendAluCfmMepOamCtlLbmTlvType testTlv. Default value is
	       filled out if NA.Only prbsCrc is supported.
           This object is not supported."
      REFERENCE
	  "ITU-T Y.1731 clause 9.3.2"
    DEFVAL      { 3 }
    ::= { extendAluCfmMepOamCtlEntry 11 }

extendAluCfmMepOamCtlTransmitVlanPriority OBJECT-TYPE
    SYNTAX      INTEGER (0..7)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Priority. 3 bit value to be used in the VLAN tag, if present
         in the transmitted frame.
         
--         The default value is CCM priority.
         ALCATEL NOTE:
            ACCESS:      NA            USAGE:       OPT
            PERSIST:     NO            INSRVMOD:    NO
            RANGE:       NA            DEFVALUE:    DEP (see DESCR)
            UNITS:       bytes         SPARSE:      NO
            DESCR:       
              Applicable for both LBM and LTM operation.
              Default value is filled out if NA, i.e. highest priority { 7 }
              (from ISR4.4).
              For LTM, dot1agCfmMepCcmLtmPriority setting is never used.
	      For ucLBM, to be configured with respect to
	      extendAluCfmMepOamCtlTransmitLbmVlanDropEnable and
              extendAluCfmMepOamCtlTransmitLbmVlanDropPcp."
     REFERENCE   
         "802.1ag clause 12.14.7.3.2:e"
    ::= { extendAluCfmMepOamCtlEntry 12 }

extendAluCfmMepOamCtlTransmitLbmVlanDropEnable OBJECT-TYPE
    SYNTAX      INTEGER {
	              noDrop(1),
                      pcp(2),
                      dei(3)
    }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
      "Parameter indicating whether the packet must be marked as drop
       eligible. pcp points out that Priority Code Point encoding must
       be used. dei points out that the drop eligible indicator must be
       used. dei can only be used in the context of an S-vlan.
       Default value is used if NA.
      ALCATEL NOTE:
          ACCESS:      NA            USAGE:       OPT
          PERSIST:     NO            INSRVMOD:    NO
          RANGE:       NA            DEFVALUE:    NA
          UNITS:       NA            SPARSE:      NO
          DESCR:       
	      Applicable for ucLbm. To be configured with respect to
              extendAluCfmMepOamCtlTransmitVlanPriority and
              extendAluCfmMepOamCtlTransmitLbmVlanDropPcp.
              This object is not supported"
    REFERENCE
        "802.1ad clause 6.7.3 and 802.1ag clause 12.14.7.3.2:e"
    DEFVAL	{ 1 }
    ::= { extendAluCfmMepOamCtlEntry 13 }

extendAluCfmMepOamCtlTransmitLbmVlanDropPcp	OBJECT-TYPE
    SYNTAX	INTEGER {
                      pcp8P0D(1),
                      pcp7P1D(2),
                      pcp6P2D(3),
                      pcp5P3D(4)
    }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
      "Defines the Priority Code Point encoding rule used. 
       Applicable only for extendAluCfmMepOamCtlTransmitLbmVlanDropEnable
       noDrop(1) and pcp(2).
       Default value is used if NA.
      ALCATEL NOTE:
        ACCESS:      NA            USAGE:       OPT
        PERSIST:     NO            INSRVMOD:    NO
        RANGE:       NA            DEFVALUE:    NA
        UNITS:       NA            SPARSE:      NO
        DESCR:       
              Applicable only for ucLbm operation.
	      Table 6-3 of 802.1ad (2005) explains the difference 
              between the different encoding rules.
              This object is not supported "
    REFERENCE
        "802.1ad clause 6.7.3"
    DEFVAL       { 1 }
    ::={ extendAluCfmMepOamCtlEntry 14 }

extendAluCfmMepOamCtlTransmitLtmTtl OBJECT-TYPE
    SYNTAX      Gauge (0..255)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The LTM TTL field. Default value, if not specified, is 64.
         The TTL field indicates the number of hops remaining to the
         LTM.  Decremented by 1 by each Linktrace Responder that
         handles the LTM.  The value received in the LTM is returned
         in the LTR.  If 0 on input, the LTM is not transmitted to the
         next hop.
         ALCATEL NOTE:
            ACCESS:      NA            USAGE:       OPT
            PERSIST:     NO            INSRVMOD:    NO
            RANGE:       NA            DEFVALUE:    NA
            UNITS:       NA            SPARSE:      NO
            DESCR:       Only applicable for extendAluCfmMepOamCtlTestType 
			 ltm. Default value is filled out if NA."
     REFERENCE   
         "802.1ag clause 12.14.7.4.2:d and 21.8.4"
    DEFVAL      { 64 }
    ::= { extendAluCfmMepOamCtlEntry 15 }

extendAluCfmMepOamCtlTransmitLtmUseFdbOnly	OBJECT-TYPE
    SYNTAX	TruthValue
    ACCESS	read-write
    STATUS	mandatory
    DESCRIPTION
	"Only the bit 'UseFDBonly' can be configured in the flags fields for
         LTMs transmitted by the MEP. The ISAM operates independent
	 of this bit value. The bridge filtering database is used to
	 determine the egress port (which corresponds with the bit value
	 true). Configuring this bit value as false will not change the 
	 behaviour of the ISAM.
	 This field was made configurable since it is copied from
	 LTM to LTM during the Linktrace protocol. If a device along the
	 route supports the use of this bit value, it can act accordingly.
	ALCATEL NOTE:
            ACCESS:      NA            USAGE:       OPT
            PERSIST:     NO            INSRVMOD:    NO
            RANGE:       NA            DEFVALUE:    NA
            UNITS:       NA            SPARSE:      NO
            DESCR:       Only applicable for extendAluCfmMepOamCtlTestType 
			 ltm. Default value is filled out if NA."
    REFERENCE
	"802.1ag clause 12.14.7.4.2:b and 20.37.1"
    DEFVAL	{ true }
    ::= { extendAluCfmMepOamCtlEntry 16 }

extendAluCfmMepOamCtlTransmitLtmEgressIdentifier  OBJECT-TYPE
    SYNTAX	OCTET STRING (SIZE(8))
    ACCESS	read-only
    STATUS	mandatory
    DESCRIPTION
	"Identifies the MEP that is originating this LTM. The low-order 
	 (highest numbered) octets contain a 48-bit IEEE MAC address unique 
	 to the system in which the MEP resides. The high-order (lowest
	 numbered) two octets are set to zero. (Only one Linktrace Initiator 
	 and Linktrace Responder exist).
	 The value returned is undefined if the test was not sent out.
        ALCATEL NOTE:
	   ACCESS:	NA		USAGE:	    NA
	   PERSIST:	NO		INSRVMOD:   NO
	   RANGE:	NA		DEFVALUE:   NA
	   UNITS:	NA		SPARSE:	    NO
	   DESCR:       Only applicable for extendAluCfmMepOamCtlTestType 
			ltm, otherwise value 0."
    REFERENCE
	"802.1q clause 12.14.7.4.3:c"
    ::= { extendAluCfmMepOamCtlEntry 17 }

extendAluCfmMepOamCtlTransmitLtmSeqNumber  OBJECT-TYPE
    SYNTAX 	Counter
    ACCESS	read-only
    STATUS	mandatory
    DESCRIPTION
	"The LTM Transaction Identifier of the LTM sent.
	ALCATEL NOTE:
	   ACCESS:	NA		USAGE:	    NA
	   PERSIST:	NO		INSRVMOD:   NA
	   RANGE:	NA		DEFVALUE:   NA
	   UNITS:	NA		SPARSE:	    NA
	   DESCR:       Only applicable for extendAluCfmMepOamCtlTestType 
			ltm, otherwise value 0."
    REFERENCE
	"802.1q clause 12.14.7.4.3:b and clause 20.41.1"
    ::= { extendAluCfmMepOamCtlEntry 18 }

extendAluCfmMepOamCtlResult  OBJECT-TYPE
    SYNTAX  INTEGER {
                      done(1),
                      inProgress(2),
		      failed(3)
    }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Can be used to monitor the progress of the OAM operation.
         
         ALCATEL NOTE:
            ACCESS:      NA            USAGE:       NA
            PERSIST:     NO            INSRVMOD:    NA
            RANGE:       NA            DEFVALUE:    NA
            UNITS:       NA            SPARSE:      NA
            DESCR:
              Indicates availability of OAM operation results.
              For LBM/LTM, it replaces monitoring on
              dot1agCfmMepTransmitLbmStatus/dot1agCfmMepTransmitLtmStatus
              (in combination with 802.1ag clause 20.5.5 timeout/5s timeout).
              done(1) - For uc LBM :results are available in this table.
                      - For mc LBM :results are available in
                        extendAluCfmMepMcLbrTable.
                      - For LTM :results are available in extendAluCfmLtrTable
	                         and this table.
              inProgress(2) - Operation successfully started, not yet completed.
	      failed(3) - Operation succesfully started, but unable to finish.
              Following table indicates time boundaries in seconds within which 
              this object will reach value done or failed for each test type:
              TestType Max (in seconds)
              'ucLbm'  (extendAluCfmMepOamCtlTransmitLbmMessages)*(0,25)+5.75 
              'mcLbm'  6   
              'ltm'    6
              A worst case (TestType independent) value of 7s can be taken."
     REFERENCE   
         "802.1ag clause 12.14.7.3.3:a,
          802.1ag clause 12.14.7.4.3:a"
    ::= { extendAluCfmMepOamCtlEntry 19 }

