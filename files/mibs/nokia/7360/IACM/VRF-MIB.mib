             INSRVMOD: 	NO
             RANGE:	NA
             DEFVALUE: 	NA
             UNITS:	NA
         "
      ::= { vrfInetCidrRouteEntry 11}


vrfInetCidrRouteNextHopAS OBJECT-TYPE
