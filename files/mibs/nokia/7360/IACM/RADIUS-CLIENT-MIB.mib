             PERSIST:  	NO
             INSRVMOD: 	NA
             RANGE:	NA
             DEFVALUE: 	NA
             UNITS:	NA 
             SPARSE:	NO 
             DESCR:	NA "
      ::= { radAccServerStatsEntry 1 }

