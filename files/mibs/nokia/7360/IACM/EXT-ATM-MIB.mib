-- Extensions to the ATM interface configuration Table
-- ===================================================

asamAtmInterfaceConfigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AsamAtmInterfaceConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains ATM interface configuration parameters,
            one entry per ATM interface. This table is an extended part
            of 'atmInterfaceConfTable' in the ATM_MIB."
    ::= { asamExtAtmMIBObjects 38 }

asamAtmInterfaceConfigEntry OBJECT-TYPE
    SYNTAX  AsamAtmInterfaceConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This list contains ATM interface configuration information,
            Each ATM interface automatically has an entry in this table
            associated with it."
    INDEX   { ifIndex }
    ::= { asamAtmInterfaceConfigTable 1 }

AsamAtmInterfaceConfigEntry ::=
    SEQUENCE {
        asamAtmInterfaceType
            INTEGER,
 
--        asamAtmCacProfileIndexIncoming
--            AsamProfilePointer,
--             
--         asamAtmCacProfileIndexOutgoing
--             AsamProfilePointer,
-- 
--         asamAtmAccessProfileIndex
--             AsamProfilePointer,
-- 
--         asamAtmCdvtProfileIndex
--             AsamProfilePointer,
-- 
--         asamAtmNgcrAdminWeight
--             INTEGER,
-- 
-- 	       asamAtmSignVpi
--             AtmVpIdentifier,
-- 
-- 	       asamAtmSignVci
--             AtmVcIdentifier,
-- 
--         asamAtmGlobalLoopbackConfigured
--             TruthValue,
-- 
--         asamAtmGlobalLoopbackDirection
--             AsamAtmLoopbackDirectionType,
--             
--         asamAtmIlmiVpi
--             AtmVpIdentifier,
-- 
-- 	       asamAtmIlmiVci
--             AtmVcIdentifier,
--          asamAtmInterfaceCrossConnectedPVpcs
--	     INTEGER,
--
--         asamAtmInterfaceCrossConnectedPVccs
--             INTEGER,
--
--         asamAtmInterfaceNonCrossConnectedPVpcs
--             INTEGER,

           asamAtmInterfaceNonCrossConnectedPVccs
               INTEGER

         }

asamAtmInterfaceType OBJECT-TYPE
    SYNTAX  INTEGER {
              ns (1),  -- Not Specified
              nt (2),  -- Network Termination
              aai (3), -- Access to Access Interface
              uai (4), -- User to Access Interface
              isi (5), -- Internal Server Interface
              asi (6)  -- External Server Interface
            }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The value of this object determines the type of ATM interface.
Only uai (4) is supported."
    ::= { asamAtmInterfaceConfigEntry 1 }

