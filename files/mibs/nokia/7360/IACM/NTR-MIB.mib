     ::= { asamSysTodPriorityTable 1}

   AsamSysTodPriorityEntry ::=
      SEQUENCE {
               asamSysTodPriorityIndex          INTEGER,
               asamSysTodPriorityReference      INTEGER,
               asamSysTodPrioritySyntonized     INTEGER
               }

   asamSysTodPriorityIndex          OBJECT-TYPE
      SYNTAX INTEGER (1..3)
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION     
        " This object is used by the asamSysTodPriorityTable to identify 
          the priority of the tod source; the first index corresponds 
          to the highest priority. The last index  corresponds to the 
          preset entry for the tod <local> and cannot be changed as
          the tod assigned the lowest priority.

      ALCATEL NOTE: Applicable to NTs supporting the Enhanced clock mgmt
             SUPPORT: YES    INSRVMOD: YES          RANGE: 1..3
             PERSIST: YES    DEFVALUE: NA           UNITS: NA"

      ::= { asamSysTodPriorityEntry 1}

   asamSysTodPriorityReference          OBJECT-TYPE
      SYNTAX INTEGER {
         none       (1), -- no tod source is specified for this priority
         local      (2), -- holdover specified (always enable last priority)         
         ieee1588-a (3), -- IEEE 1588-A source is specified for this priority
         ieee1588-b (4), -- IEEE 1588-B source is specified for this priority
         gnss-a     (5), -- gnss-a source is specified for this priority
         gnss-b     (6)  -- gnss-b source is specified for this priority
      }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
        " This object is used by the asamSysTodPriorityTable to assign 
          the tod source to the corresponding priority for the automatic
          selection of the tod reference. 

      ALCATEL NOTE: Applicable to NTs supporting the Enhanced clock mgmt
             SUPPORT: YES    INSRVMOD: YES          RANGE: 1..6
             PERSIST: YES    DEFVALUE: none (1)     UNITS: NA"

      ::= { asamSysTodPriorityEntry 2}

