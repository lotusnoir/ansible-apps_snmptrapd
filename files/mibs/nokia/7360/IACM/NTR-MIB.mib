-- ====================================================================
-- The following objects are for the implementation of R4.3 :
--       - DTAG special configurations (synce-transfer & split)mode
--       - SSM out for T4 (BITS)
--       - SSM out for Synce ports
--       - SSM out for xDSL ports
-- ====================================================================


ntrConfigT4outTable  OBJECT-TYPE
      SYNTAX SEQUENCE OF NtrConfigT4outEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION     
       "This table contains the T4 D-PLL parameters, required for the R4.3 DTAG
        configurations. The table has only one entry. 
        In standard mode, the T4 parameters are optional.
 
                T4mode            T4PrioRef-1    T4PrioRef-2
                 standard           none	          none
                 synce-transfer     synce-1         synce-2
                 synce-split        synce-1         synce-2

      When we go from standard to synce-transfer or to synce-split, the system
      will copy the (T0) prio-ref-1 and prio-ref-2 to the T4 prio-references.
      The operator is allowed to overrule this, by filling in the T4PrioRef-1
      and T4PrioRef-2. For the synce-split mode, the system will automatically
      configure the BITS as reference to T0 DPLL. Once operating in synce-transfer
      or synce-split mode, the operator can modify at any time the T4 parameters
      (as long as there is no conflict).

      In standard mode no T4 parameters can be modified (not the T4PrioRef-1,
      not T4PrioRef-2, not T4DampingFactor, not T4JitterBandwith)

      TABLESIZE: 1
         " 
     ::=  { asamSystemClockSelect 28 }

   ntrConfigT4outEntry OBJECT-TYPE
      SYNTAX NtrConfigT4outEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An entry in the ntrConfigT4outTable."
      INDEX { ntrT4Index }

     ::= { ntrConfigT4outTable 1}

   NtrConfigT4outEntry ::= 
     SEQUENCE {
               ntrT4Index      		INTEGER,
               ntrT4mode     		 	INTEGER,
               ntrT4DampingFactor		INTEGER,
               ntrT4JitterBandwidth	INTEGER,
               ntrT4PrioRef-1			SysClkT4PriorityReference,
               ntrT4PrioRef-2			SysClkT4PriorityReference
               }

   ntrT4Index  OBJECT-TYPE
      SYNTAX INTEGER (1)
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION     
       "This object is used by the ntrConfigT4outTable to identify an entry the
        T4out D-PLL parameters. Only one entry is foreseen in R4.3

        ALCATEL NOTE: Applicable to NTs supporting the Enhanced clock mgmt
             SUPPORT: YES    INSRVMOD: YES          RANGE: 1
             PERSIST: YES    DEFVALUE: NA           UNITS: NA"

      ::= { ntrConfigT4outEntry 1}


   ntrT4mode OBJECT-TYPE
      SYNTAX INTEGER {
         standard (1),    		-- standard configuration
         synce-transfer (2),  	-- DTAG configuration
         synce-split (3)      	-- DTAG configuration
      }
      ACCESS read-write
      STATUS optional
      DESCRIPTION     
       "This object indicates the configuration mode.
        Standard and 2 specific DTAG configurations are supported.
        The parameter is only needed to change the mode. This parameter 
        is no longer needed if other T4 parameters are modified,
        and we are in the synce-transfer or synce-split mode.

      ALCATEL NOTE: Applicable to NTs supporting the Enhanced clock mgmt
             SUPPORT: YES    INSRVMOD: YES          RANGE: 1..3
             PERSIST: YES    DEFVALUE: 1            UNITS: NA"

      ::= { ntrConfigT4outEntry 2}


   ntrT4DampingFactor OBJECT-TYPE
      SYNTAX INTEGER {
         factor-1-2   (1),     -- 1.2 db
         factor-2-5   (2),     -- 2.5 db
         factor-5-0   (3),     -- 5.0 db
         factor-10-0  (4),     -- 10.0 db
         factor-20-0  (5)      -- 20.0 db
      }

      ACCESS read-write
