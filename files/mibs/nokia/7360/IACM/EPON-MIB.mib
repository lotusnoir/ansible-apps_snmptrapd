-- *********************************************
--
-- EPON QUEUE PROFILE TABLE
--
-- This table contains information  on an
-- ONT.  Using this information, the OLT can determine 
-- the following exchanges with the ONT:  
-- 1)  Number of queues needed and 2) Relative weight for each queue
-- 3)Relative threshold for each queue.  Rows are created by the operator.
--
-- 
-- *********************************************
eponQueueProfileTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EponQueueProfileEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"This object contains a table for queue profiles. A Queue profile is used to associate with
		 logical link. 
		1) Number of queues needed.
		2) Relative weight for each queue.
		3) Relative threshold for each queue.  
	 
	 ALCATEL NOTE:currently the queue number and threshold could not be configued at ONT. hence, it will
	 always use default queue profile. it does not allow to create/modify/delete entry in this table.
             SUPPORT: YES   TABLESIZE: DEP"
::= { eponProfilesGroup 1 }

eponQueueProfileEntry
 OBJECT-TYPE
	SYNTAX 	EponQueueProfileEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
         "An entry in the eponQueueProfileTable. 
          Row creation : an entry is created  when operator creates queue profile. 
          Row deletion : an entry is deleted  when operator deletes queue profile.
          ALCATEL NOTE:
             SUPPORT: YES"
INDEX {eponQueueProfileIndex }
::= {eponQueueProfileTable 1}

EponQueueProfileEntry ::=
	SEQUENCE { 
		eponQueueProfileIndex		   AsamProfileIndex,
		eponQueueProfileName		   AsamProfileName,
		eponQueueProfileRefCount	   AsamProfileRefCount,
		eponQueueProfileRowStatus 	   RowStatus,
		eponQueueProfileQ1Weight		INTEGER,
		eponQueueProfileQ2Weight		INTEGER,
		eponQueueProfileQ3Weight		INTEGER,
		eponQueueProfileQ4Weight		INTEGER,
		eponQueueProfileQ5Weight		INTEGER,
		eponQueueProfileQ6Weight		INTEGER,
		eponQueueProfileQ7Weight		INTEGER,
		eponQueueProfileQ8Weight		INTEGER,
		eponQueueProfileQ1Threshold		INTEGER,
		eponQueueProfileQ2Threshold		INTEGER,
		eponQueueProfileQ3Threshold		INTEGER,
		eponQueueProfileQ4Threshold		INTEGER,
		eponQueueProfileQ5Threshold		INTEGER,
		eponQueueProfileQ6Threshold		INTEGER,
		eponQueueProfileQ7Threshold		INTEGER,
		eponQueueProfileQ8Threshold		INTEGER
}

