-- EPON PON CONFIGURATION TABLE
--
-- This table contains configuration information specific to
-- an OLT's control of a specific PON port. Upon creation of
-- an OLT line card, the agent automatically creates 8 rows 
-- in this table (one for each PON port).

-- *********************************************

eponPonConfigTable  OBJECT-TYPE
	SYNTAX SEQUENCE OF EponPonConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory  
	DESCRIPTION 
	 "  this table indicates pon interface.
	 ALCATEL NOTE:
             SUPPORT: YES   TABLESIZE: DEP"
	::= { eponPonGroup 1 }

eponPonConfigEntry  OBJECT-TYPE
	SYNTAX 	EponPonConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory       
	DESCRIPTION
         "An entry in the eponPonConfigTable. 
          Row creation : Pon entry is created by the system when EPON board planed. 
          for ELT8-A, 8 Pon entry are auto-created when board planning.
          Row deletion : Pon entry is deleted by the system when EPON board planed. 
          for ELT8-A, 8 Pon entry are auto-deleted when board planning.
          ALCATEL NOTE:
             SUPPORT: YES"
	INDEX	{ eponPonConfigIfIndex }
	::= { eponPonConfigTable 1 }

EponPonConfigEntry ::= SEQUENCE {
    eponPonConfigIfIndex	            IfIndex,
    eponPonConfigPollingDiscoveryPeriod	    INTEGER,
    eponPonConfigDBAPollingInterval0        INTEGER,  
    eponPonConfigDBAPollingInterval1        INTEGER,
    eponPonConfigDBAPollingInterval2        INTEGER,
    eponPonConfigDBAPollingInterval3        INTEGER,
    eponPonConfigDBAPollingInterval4        INTEGER,
    eponPonConfigDBAPollingInterval5        INTEGER,
    eponPonConfigDBAPollingInterval6        INTEGER,
    eponPonConfigDBAPollingInterval7        INTEGER,  
    eponPonAuthMode                         INTEGER,
    eponPonPmCollectInd	                    INTEGER,
    eponPonConfigFecDownstreamInd           INTEGER,
    eponPonConfigRssiInd                    INTEGER,
    eponRSSIProfileId                       INTEGER,
    eponPonSFPType                          INTEGER,
    eponOpticalModuleType                   INTEGER,
    eponPonPortMode                         INTEGER,
    eponPonGetNextFree                      INTEGER,
    eponPonMaxOntNum                        INTEGER,
    eponPonFreeOntNum                       INTEGER,   
    eponPonConfigRogueTestCmd               INTEGER,
    eponPonIdlePwrTCAEnable                 INTEGER,
    eponPonIdlePwrTCAThreshold              INTEGER,
    eponPonIdlePwrCurrent                   INTEGER,
    eponPonRateLimitProfileId               INTEGER,
    eponPonDisablePortAsPkgOverload         Boolean
           }

