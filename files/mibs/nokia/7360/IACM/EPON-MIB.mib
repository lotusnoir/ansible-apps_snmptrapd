-- EPON ONT FILTER PROFILE TABLE
--
-- This table contains the classification and filter rules on an
-- LanEtherFilter physical UNI port or user VLAN Interface
--
-- 
-- *********************************************

eponOntFilterProfileTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EponOntFilterProfileEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"This object contains a table for defining classfication
		and filter rules by operator. The max field for one rule could be 
		specified is 4.
		 ALCATEL NOTE:
             SUPPORT: YES   TABLESIZE: DEP"
::= { eponProfilesGroup 16 }

eponOntFilterProfileEntry OBJECT-TYPE
	SYNTAX 	EponOntFilterProfileEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		 "An entry in the eponOntFilterProfileTable. 
          Row creation : an entry is created  when operator creates OntFilter profile.
          Row deletion : an entry is created  when operator deletes OntFilter profile.
          ALCATEL NOTE:
             SUPPORT: YES"
	INDEX {eponOntFilterProfileIndex }
::= {eponOntFilterProfileTable 1}

EponOntFilterProfileEntry ::=
	SEQUENCE { 
		eponOntFilterProfileIndex		           AsamProfileIndex,
		eponOntFilterProfileName		           AsamProfileName,
		eponOntFilterProfileRefCount              AsamProfileRefCount,
		eponOntFilterProfileRowStatus 	           RowStatus,
		eponOntFilterProfileClassfierNum          INTEGER,
		eponOntFilterProfileClassfierFields1      EponTcClassfierFields,
		eponOntFilterProfileClassfierValue1       OCTET STRING (SIZE (16)),
		eponOntFilterProfileClassfierOperators1   EponTcClassfierOperators,	
		eponOntFilterProfileClassfierFields2      EponTcClassfierFields,
		eponOntFilterProfileClassfierValue2       OCTET STRING (SIZE (16)),
		eponOntFilterProfileClassfierOperators2   EponTcClassfierOperators,
		eponOntFilterProfileClassfierFields3      EponTcClassfierFields,
		eponOntFilterProfileClassfierValue3       OCTET STRING (SIZE (16)),
		eponOntFilterProfileClassfierOperators3   EponTcClassfierOperators,
		eponOntFilterProfileClassfierFields4      EponTcClassfierFields,
		eponOntFilterProfileClassfierValue4       OCTET STRING (SIZE (16)),
		eponOntFilterProfileClassfierOperators4   EponTcClassfierOperators
}

eponOntFilterProfileIndex OBJECT-TYPE 
	SYNTAX AsamProfileIndex
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	    "This object is the index to the eponOntFilterProfileTable.
	    ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    YES      INSRVMOD:   NO
                 RANGE:      NA       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
"
::= { eponOntFilterProfileEntry 1}

eponOntFilterProfileName 	OBJECT-TYPE 
	SYNTAX	AsamProfileName
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	    "The corresponding name for a given profile id.
        The name corresponding to the ProfileId must also be unique.
	    ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    YES      INSRVMOD:   NO
                 RANGE:      NA       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA
                "
::= {eponOntFilterProfileEntry 2}

eponOntFilterProfileRefCount OBJECT-TYPE
	SYNTAX	AsamProfileRefCount
	ACCESS  read-only
	STATUS	mandatory
	DESCRIPTION
	" Indicates the number of entities using this profile. If the
