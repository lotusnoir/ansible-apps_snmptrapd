--  EPON PON ROGUE TEST RESULT TABLE
--	
-- For a specific PON, this table contains the 
-- rogue ont Test result information. It include rogue 
-- onu,unknow onu which is the OLT cannot get onu tx  
-- power controlcapibility and unsupport onu which is 
-- cannot support disable onu tx power control.
-- *********************************************

eponPonRogueTestResultTable  OBJECT-TYPE
	SYNTAX SEQUENCE OF EponPonRogueTestResultEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "This table indicates rogue ONT test result.
        This table include rogue onu,unknow onu and unsupport
        onu. unknow onu is the OLT cannot get onu tx power control 
        capibility, cannot judge if the onu is rogue onu.
        unsupport onu is the onu cannot support disable onu tx 
        power control,not support on/off test. This table will be 
        generated when rogue onu detection finish. If  manager 
        start rogue onu detection, this table will be cleaned.
        The index are eponPonConfigIfIndex and eponPonRogueTestResultOntNum,
        eponPonConfigIfIndex is the index of eponPonConfigTable."
        ::= { eponPonGroup 6 }
        
eponPonRogueTestResultEntry OBJECT-TYPE
        SYNTAX   EponPonRogueTestResultEntry
        ACCESS  not-accessible
        STATUS  mandatory
        INDEX   {  eponPonConfigIfIndex,
		      eponPonRogueTestResultOntNum  }
        ::= { eponPonRogueTestResultTable 1 }

EponPonRogueTestResultEntry ::=  SEQUENCE { 
    eponPonRogueTestResultOntNum                  INTEGER,
    eponPonRogueTestResultOntMAC                  MacAddress,
    eponPonRogueTestResultOntIfindex              IfIndex,
    eponPonRogueTestResultOntStatus               INTEGER,
    eponPonRogueTestResultRogueAlmStatus          INTEGER
}
                       
eponPonRogueTestResultOntNum  OBJECT-TYPE
	SYNTAX 	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
	"This attribute indicates the ONT number of rogue test result sequence
	 on the specific PON 
	ALCATEL NOTE:
	ACCESS:     NA       USAGE:      NA
	PERSIST:    NO       INSRVMOD:   NO
	RANGE:      NA       DEFVALUE:   NA
	UNITS:      NA       SPARSE:     NA  "
	::= { eponPonRogueTestResultEntry 1 }


eponPonRogueTestResultOntMAC  OBJECT-TYPE
	SYNTAX 	MacAddress
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
        "This attribute indicates the MAC address of the rogue test result ont
        on the specific PON. if no MAC could be get, NULL will be shown.
	ALCATEL NOTE:
	ACCESS:     NA       USAGE:      NA
	PERSIST:    NO       INSRVMOD:   NO
	RANGE:      NA       DEFVALUE:   NA
	UNITS:      NA       SPARSE:     NA  "
	::= { eponPonRogueTestResultEntry 2 }  

eponPonRogueTestResultOntIfindex  OBJECT-TYPE
	SYNTAX 	IfIndex
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
	"This attribute indicates the ONT ifindex of the rogue test
	on the specific PON.For unplanned ONT which has no ont ifindex,
	NULL will be shown, and using 0 to represent its value.
	ALCATEL NOTE:
	ACCESS:     NA                    USAGE:      NA
	PERSIST:    NO                    INSRVMOD:   NO
	RANGE:      0,1..2147483647       DEFVALUE:   NA
	UNITS:      NA                    SPARSE:     NA "
::= { eponPonRogueTestResultEntry 3 }

eponPonRogueTestResultOntStatus          OBJECT-TYPE
	SYNTAX 	INTEGER 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
	"This attribute indicates the ont status in rogue onu test result.
	unknow(1) - the OLT cannot get onu tx power control capibility, cannot judge if the onu is rogue onu.
	not-support(2) - the onu cannot support disable onu tx power control,not-support on/off test.
	rogueOnu(3)   - the onu is rogue onu.		
        noTest(4).- the onu is not tested, which has never online and no mac availabe for test
	ALCATEL NOTE:
	ACCESS:     NA      USAGE: NA
	PERSIST:    NO      INSRVMOD:   NO
	RANGE:      1..4    DEFVALUE:   NA
	UNITS:      NA      SPARSE:     NA
	DESCR:      NA"
::= { eponPonRogueTestResultEntry 4}

eponPonRogueTestResultRogueAlmStatus OBJECT-TYPE
	SYNTAX 	INTEGER 
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
        "This attribute indicates rogue onu alarm status of onts in this table.
	This attribute allow user to clear rogue onu alarm by clear(3).only 
	clear(3) can be set, and the eponPonRogueTestResultRogueAlmStatus must
	be active(2).
	no-roguealarm(1) - the onu never report rogue alarm.
	active(2) - the onu has rogue alarm.
	clear (3) - the onu had rogue alarm,but the alarm be cleared.
	ALCATEL NOTE:
	ACCESS:     NA      USAGE: NA
	PERSIST:    NO      INSRVMOD:   NO
	RANGE:      1..3    DEFVALUE:   NA
	UNITS:      NA      SPARSE:     NA
	DESCR:      NA"
::= { eponPonRogueTestResultEntry 5}

