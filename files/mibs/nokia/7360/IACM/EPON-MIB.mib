eponPmCollectInd  OBJECT-TYPE
	SYNTAX 	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
		" This indicates whether 
		  Performance Monitors should be collected for all ONTs.
	
		  1 = PM collection enabled
		  2 = PM collection disabled

		 Default = 1 = enabled.   "
	::= { eponSystemGroup 1 }
	

eponRogueONTAdmin  OBJECT-TYPE
	SYNTAX 	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
		" This attribute allows the manager to choose auto or manual  
		  	   disable a rogue onu.
		  1 =  autodisable. If an onu is detected as rogue onu, system 
					will auto disable the onu tx power control.
		  2 =  manualdisable. If an onu is detected as rogue onu, system 
					will do nothing for the onu. Suggest manager manual disable
					the onu.
		 Default = 2  "
	::= { eponSystemGroup 3 }
	
eponOnuSnConflictLevel  OBJECT-TYPE
	SYNTAX 	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
		"  This indicates ONT SN conflict level.		
		   0  = pon level 
			 1  = board level
 			 DF = 0"
	::= { eponSystemGroup 4 }
		       
-- *********************************************
-- EPON ONU ALARM FLAG TABLE
--
-- This table contains a list of software versions, firmware versions and alarm states of the ONT.
-- When OLT receives an CTC2.1 alarm from ONT, and this ONT SW version matches one of the entries in this table, 
-- OLT will parse the alarm state according to the value of the entrys,
-- eponOnuAlmFlagAlarmState. CTC2.1 define that alarm state bit 1 means alarm clear and 0 means alarm report. 
--
-- *********************************************

eponOnuAlmFlagTable  OBJECT-TYPE
	SYNTAX	SEQUENCE OF EponOnuAlmFlagEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"This object contains a table for alarm state of ONT Software version and firmware version\
            ALCATEL NOTE:
             SUPPORT: YES   TABLESIZE: 250"
::= { eponSystemGroup 5 }

eponOnuAlmFlagEntry OBJECT-TYPE
	SYNTAX 	EponOnuAlmFlagEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"Rows of this table contain information about alarm state of ONT SW version and firmware version. 
            ALCATEL NOTE:
             SUPPORT: YES"    
	INDEX {eponOnuAlmFlagIndex }
::= { eponOnuAlmFlagTable  1}

EponOnuAlmFlagEntry ::=
	SEQUENCE { 
		eponOnuAlmFlagIndex            INTEGER,
		eponOnuAlmFlagRowStatus        RowStatus,
		eponOnuAlmFlagSwVersion	       OCTET STRING (SIZE(0..16)),
                eponOnuAlmFlagFwVersion        OCTET STRING (SIZE(0..16)),
		eponOnuAlmFlagAlarmState       INTEGER
		}

eponOnuAlmFlagIndex OBJECT-TYPE 
	SYNTAX INTEGER
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	    "This object is the index to the eponOnuAlmFlagTable.
	     Range is 1 .. 250  
		ALCATEL NOTE:
                 ACCESS:     NA       USAGE:      NA
                 PERSIST:    YES      INSRVMOD:   NO
                 RANGE:      1-250       DEFVALUE:   NA
                 UNITS:      NA       SPARSE:     NA"
::= { eponOnuAlmFlagEntry 1}

eponOnuAlmFlagRowStatus  OBJECT-TYPE
	SYNTAX 	RowStatus
	ACCESS	read-write
	STATUS	mandatory
