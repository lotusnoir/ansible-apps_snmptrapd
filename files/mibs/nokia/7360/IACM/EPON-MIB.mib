-- Administrative State 1 = unlock, 2 = lock

PonAdminState ::= INTEGER {enabled(1),disabled(2)}

Boolean ::= INTEGER (0..1)
EponTcClassfierFields ::= INTEGER (0..255)
--			{
--				destination mac (0),
--				source mac (1),
--				vlan priority (2),
--				vlan id (3),
--				ethernet type (4),
--				destination ip (5),
--				source ip (6),
--				ip version (7),
--				ip dscp (8),
--				ip precedence (9),
--				source l4 port (10),
--				destination l4 port (11),
--				IP protocol (12),
--				ipv6 traffic class (13),
--				ipv6 flow label (14),
--				ipv6 next header (15),
--				destination ipv6 address(16),
--				source ipv6 address(17),
--				destination ipv6 prefix(18),
--				source ipv6 prefix (19),
--        not defined(255)
