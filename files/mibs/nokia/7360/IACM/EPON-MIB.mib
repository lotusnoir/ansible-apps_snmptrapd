-- *********************************************
-- EPON PON DISCOVERED ONT TABLE
--	
-- For a specific PON, this table contains the 
-- MAC address of ONTs that have been auto-discovered but have
 --not yet been manually provisioned (i.e. ENTERED)
-- by the operator. In TL1, these are referred to as 
-- NEWEONT alarms. The operator does not directly
-- create or delete rows in this table.  The OLT
-- agent automatically manages the creation and
-- deletion of rows. Rows are automatically created
-- when an ONT is auto-discovered on a specific PON.
-- Rows are not automatically deleted just because a
-- NEWEONT alarm is cleared (the reason being that the
-- AWS still needs to reference the associated row to
-- determine the MAC address for the cleared NEWEONT alarm).
-- So, NEWEONT alarms are normally
-- cleared when an operator (or the system) creates an ONT on a
-- PON (with MAC address) that has a match in this
-- table )  This table is used to store ONT information that
-- is required to be displayed with the NEWEONT alarm
-- (i.e. ONT's MAC address). The table indexes (i.e. 
-- eponPonDiscovOntIfIndex and eponPonDiscovOntNum)
-- match up with the Index1 and Index 2 fields 
-- actually contained in the Alarm MIB (for a
-- NEWEONT alarm).
-- *********************************************

eponPonDiscovOntTable  OBJECT-TYPE
	SYNTAX SEQUENCE OF EponPonDiscovOntEntry
	ACCESS	not-accessible
	STATUS	mandatory  
	DESCRIPTION 
	"  this table indicates the discoveried ONT.
	 ALCATEL NOTE:
             SUPPORT: YES   TABLESIZE: DEP"
	::= { eponPonGroup 2 }

eponPonDiscovOntEntry  OBJECT-TYPE
	SYNTAX 	EponPonDiscovOntEntry
	ACCESS	not-accessible
	STATUS	mandatory  
	DESCRIPTION 
	"An entry in theeponPonDiscovOntTable . 
     Row creation :Rows are automatically created when an ONT is auto-discovered on a specific PON
     Row deletion : Rows are not automatically deleted just because a NEWEONT alarm is cleared
     ALCATEL NOTE:
        SUPPORT: YES"
	INDEX	{ eponPonConfigIfIndex,
		      eponPonDiscovOntNum }
	::= { eponPonDiscovOntTable 1 }

EponPonDiscovOntEntry ::= SEQUENCE {
	eponPonDiscovOntNum			             INTEGER,
	eponPonDiscovOntMacAddr		           MacAddress ,
    eponPonDiscovOntLogicalAuthID                DisplayString (SIZE(0..24)),
    eponPonDiscovOntAuthfailType INTEGER        

}


eponPonDiscovOntNum  OBJECT-TYPE
	SYNTAX 	INTEGER (1..128)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
	" it identified a specified ONT number. 
	ALCATEL NOTE:
	ACCESS:     NA       USAGE:      NA
    PERSIST:    NO     INSRVMOD:   NO
    RANGE:      1..128    DEFVALUE:   NA
    UNITS:      NA       SPARSE:     NA  "
	::= { eponPonDiscovOntEntry 1 }


eponPonDiscovOntMacAddr  OBJECT-TYPE
	SYNTAX 	MacAddress
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
		"The  MAC address of the ONT that has been auto-discovered
		 on the specific PON. 
		 	ALCATEL NOTE:
	ACCESS:     NA       USAGE:      NA
    PERSIST:   NO        INSRVMOD:   NO
    RANGE:      NA       DEFVALUE:   NA
    UNITS:      NA       SPARSE:     NA  "

	::= { eponPonDiscovOntEntry 2 }  

eponPonDiscovOntLogicalAuthID OBJECT-TYPE
        SYNTAX  DisplayString (SIZE(0..24))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            " The Logical ID of the ONT that has been auto_discovered on the specific PON.

            ALCATEL NOTE:
                 ACCESS: NA        usage:NA
                 persistent: NO    INSRVMOD: NO
                 RANGE:NA          DEFVALUE: NA
                 UNITS: NA         SPARSE: NA "
        
        ::= { eponPonDiscovOntEntry 3 }

eponPonDiscovOntAuthfailType OBJECT-TYPE
        SYNTAX  INTEGER (0..3)
        ACCESS  read-only
