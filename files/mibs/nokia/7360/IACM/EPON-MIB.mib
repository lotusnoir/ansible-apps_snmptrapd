EponTcClassfierOperators  ::= INTEGER (0..255)
--			{
--				nevermatch (0),
--				equal (1),
--				not equal (2),
--				less than or equal (3),
--				greater than or equal (4),
--				field exist (5),
--				field not exist (6),
--				always match (7),
--        not defined(255) 
--			}	

-- Operational State 1 = enabled, 2 = disabled

PonOperState ::= INTEGER {up(1),down(2)}

