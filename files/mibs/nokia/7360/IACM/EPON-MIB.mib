-- *********************************************
--
-- EPON ONT POTS Port Performance TABLE
--
-- Table of number of previous statistics intervals for the traffic on a 
-- ONT POTS Port
--
-- 
-- *********************************************

eponPOTSPerfInfoTable    OBJECT-TYPE 
        SYNTAX    SEQUENCE OF EponPOTSPerfInfoEntry 
        ACCESS    not-accessible
        STATUS    mandatory 
        DESCRIPTION 
             "Table of number of previous statistics intervals for the traffic on a 
            ONT POTS interface.  
            ALCATEL NOTE:
             SUPPORT: YES   TABLESIZE: DEP"
::= {eponStatisticsGroup 16 } 

eponPOTSPerfInfoEntry OBJECT-TYPE 
        SYNTAX    EponPOTSPerfInfoEntry 
        ACCESS    not-accessible 
        STATUS    mandatory 
        DESCRIPTION 
            "An entry in the eponPOTSPerfInfoEntry. 
          Row creation : an entry is created by the system when ONT pots port level statistics enable. 
            ALCATEL NOTE:
             SUPPORT: YES"      
             INDEX   { eponOntIndex, eponPotsPortId } 
