-- *********************************************
-- EPON ONT SOFTWARE VERSION CONTROL TABLE
--
--
-- This table associates ONT HW Version/Variant with appropriate
-- ONT SW Version (download and planned).  The instructions in this table are used when
-- the following are TRUE: 
-- 1) Either Planned SW Version and/or Download SW Version (in ONT Table) are set to AUTO,
-- 2) Actual HW Version and specified SW Variant (in ONT Table) have a match in this table.
-- When such case is TRUE,  the logic behaves as if the appropriate AUTO selection (in ONT Table)
-- was instead replaced by the respective SW Version specified in this table.
--
--  Rows are created by the operator.
--
-- *********************************************

eponOntSwVerCtlTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EponOntSwVerCtlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"This object contains a table for automating the download
		and activation of ONT software (based on ONT HW version). "
::= { eponOntSwGroup 1 }

eponOntSwVerCtlEntry OBJECT-TYPE
	SYNTAX 	EponOntSwVerCtlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"Rows of this table contain ONT HW version keys and
		 associated ONT SW versions. "
	INDEX {eponOntSwVerCtlIndex }
::= {eponOntSwVerCtlTable 1}

EponOntSwVerCtlEntry ::=
	SEQUENCE { 
		eponOntSwVerCtlIndex		          INTEGER,
		eponOntSwVerCtlRowStatus 	          RowStatus,
		eponOntSwVerCtlHwVersion		  DisplayString(SIZE(0..8)),
		eponOntSwVerCtlPlannedSwVersion 	  DisplayString(SIZE(0..16)),
                eponOntSwVerCtrlUpgradeMode               INTEGER,
                eponOntSwVerCtlPlannedVariant DisplayString(SIZE(0..16))
                
		}

eponOntSwVerCtlIndex OBJECT-TYPE 
	SYNTAX INTEGER
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	    "This object is the index to the eponOntSwVerCtlTable.
	     Range is 1 .. 500  "
::= { eponOntSwVerCtlEntry 1}

eponOntSwVerCtlRowStatus OBJECT-TYPE 
	SYNTAX	RowStatus
	ACCESS	read-write
	STATUS  mandatory
	DESCRIPTION
		"This object is used to create a new row or modify or
         		delete an existing row in this table. "
::= { eponOntSwVerCtlEntry  2}

eponOntSwVerCtlHwVersion 	OBJECT-TYPE 
	SYNTAX 	DisplayString(SIZE(0..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
		" This is the ONT Hardware Version. Has same connotation as bponOntVersionNumber
		  attribute in eponOntTable.  This attribute is a key in this table.  The 
		  attribute MUST be unique, otherwise it is rejected.

		  Modification is NOT permitted (i.e. CANNOT a key field). 

		  No default - must be supplied by operator.  "
::= {eponOntSwVerCtlEntry 3}


eponOntSwVerCtlPlannedSwVersion  OBJECT-TYPE
	SYNTAX 	DisplayString(SIZE(0..16))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
		" This attribute has same connotation as eponOntSwPlannedVersion attribute in eponOntTable. When eponOntSwPlannedVersion is set to special value AUTO, and that ONT's hardware version have a match in this table, then this attribute is used in place of the eponOntSwPlannedVersion.
		  
		  Modification is permitted.
"
::= {eponOntSwVerCtlEntry 4}

eponOntSwVerCtrlUpgradeMode             OBJECT-TYPE
	SYNTAX 	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION 
		" 
		  This attribute is used to specify  Software upgarde Mode    
                   NONE (0)
		   CTC (1)
		   TK  (2)
		  
		 ALCATEL NOTE:
		 ACCESS: NA        usage: NA
		 persistent: NA    INSRVMOD:NA
         RANGE:0..2        DEFVALUE: 1
         UNITS: NA         SPARSE: NA "
	::= { eponOntSwVerCtlEntry 5 }  

eponOntSwVerCtlPlannedVariant   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                " This attribute has same connotation as eponOntPlannedVariant attribute in eponOntTable.   
                  This attribute is a key in this table.  The combination of this attribute and the
                  eponOntSwVerCtlHwVersion attribute MUST be unique, otherwise it is rejected.
      
                  Modification is permitted (i.e. CANNOT a key field).
      
                  Maximum of 16 displayable characters.
      
                  Default = null string.  "
::= {eponOntSwVerCtlEntry 6}
eponOntSwVerCtlIndexNext    OBJECT-TYPE
        	SYNTAX          INTEGER
        	ACCESS          read-only
        	STATUS          mandatory
        	DESCRIPTION     
        	        	" This object contains an appropriate value to be used for eponOntSwVerCtlIndex 
		   when creating entries in the eponOntSwVerCtlTable. 
		   Range is 0 .. 500.
         		   The value 0 indicates that no unassigned entries are available. To 
         		   obtain the next avaialble index value for a new entry, the
        		   manager can issue a management protocol retrieval operation to obtain the
         		   current value of this object.  After each retrieval, the agent should
        		   modify the value to the next unassigned index. "
::= {eponOntSwGroup 5 }


-- *********************************************
-- EPON ONT SOFTWARE VERSION INFO TABLE
--
-- This table contains a listing of ONT Software Versions that are currently resident on the 
-- the NT file system.
--
-- For this initial GET of the 1st row, the OLT agent will delay
-- the GET-RESPONSE while it replenishes the entire
-- table with fresh information
--
-- *********************************************

eponOntSwVerInfoTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EponOntSwVerInfoEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"This object contains a table for listing the ONT Software version
		 files currently resident on the NT file system "
::= { eponOntSwGroup 2 }

eponOntSwVerInfoEntry OBJECT-TYPE
	SYNTAX 	EponOntSwVerInfoEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"Rows of this table contain information about ONT SW version
		 files resident on the NT file system. "
	INDEX {eponOntSwVerInfoIndex }
::= {eponOntSwVerInfoTable 1}

EponOntSwVerInfoEntry ::=
	SEQUENCE { 
		eponOntSwVerInfoIndex		    INTEGER,
		eponOntSwVerInfoSwVersion	    DisplayString,
		eponOntSwVerInfoSwVersionSize 	INTEGER
		}

eponOntSwVerInfoIndex OBJECT-TYPE 
	SYNTAX INTEGER
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
	    "This object is the index to the eponOntSwVerInfoTable.
	     Range is 1 .. 250  "
::= { eponOntSwVerInfoEntry 1}

eponOntSwVerInfoSwVersion  OBJECT-TYPE
	SYNTAX 	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
		" This attribute contains the ONT Software Version identifier.

		   If there are no ONT software version files on the NT system, or if the information
		   is unobtainable for any reason - then this attribute contains the null string. "
::= {eponOntSwVerInfoEntry 2}


eponOntSwVerInfoSwVersionSize	OBJECT-TYPE 
	SYNTAX 	INTEGER (0..4294967295)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION 
		" This attribute contains the ONT Software Version size (in bytes).

		   If there are no ONT software version files on the NT system, or if the information
		   is unobtainable for any reason - then this attribute contains the value 0. "
::= {eponOntSwVerInfoEntry 3}

