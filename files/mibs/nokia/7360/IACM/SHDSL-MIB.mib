-- Textual conventions

ShdslUnitId ::= INTEGER {
        stuC(1),
        stuR(2),
        sru1(3),
        sru2(4),
        sru3(5),
        sru4(6),
        sru5(7),
        sru6(8),
        sru7(9),
        sru8(10)
}

ShdslUnitBitmap ::= INTEGER (0..1023)
        -- Bitmap with each bit representing a unit in an SHDSL span
        -- as follows:
        -- Bit     Unit
        -- ***     ******
        --   0     STU-C
        --   1     STU-R
        --   2     SRU-1
        --   3     SRU-2
        --   4     SRU-3
        --   5     SRU-4
        --   6     SRU-5
        --   7     SRU-6
        --   8     SRU-7
        --   9     SRU-8
    --
    -- The meaning of the bit state (0/1) is defined by individual objects.

ShdslSegmentSide ::= INTEGER {
        networkSide(1),
        customerSide(2)
}

ShdslLoopbackMode ::= INTEGER {
        noLoopback(1),
        normalLoopback(2),
        specialLoopback(3)
}

ShdslSpanOperationalMode ::= INTEGER {
        native(1),
        ima(2),
        efm(3),
	tdm(4)
}

ShdslSpanSpectralProfile ::= INTEGER {
        symmetricProfile(1),
        assymmetricProfile(2)
}

ShdslSpanWireMode ::= INTEGER {
        twoWire(1),
        fourWire(2),
        sixWire(3),
        eightWire(4)
}

ShdslWirePair ::= INTEGER {
        firstPair(1),
        secondPair(2),
        thirdPair(3),
        fourthPair(4)
}

ShdslWirePairAssignment ::= INTEGER {
        lowerNumberFirst(1),
        higherNumberFirst(2),
        unknown (3),
        notApplicable (4)
}

ShdslWirePairAssignmentExt ::= INTEGER (0..228)
        -- 8-bit bitmap with each 2-bit combination representing a particular
        -- meaning as follows (max 11100100b = 228), 
        -- set to 0 for 2-wire mode:
        -- Bit     Meaning
        -- ***     ********
        -- 1,0     wirePair of the 4*i-3 numbered port for 6/8-wire,
        --          2*i-1 numbered port for 4-wire
        -- 3,2     wirePair of the 4*i-2 numbered port for 6/8-wire, 
        --          2*i numbered port for 4-wire
        -- 5,4     wirePair of the 4*i-1 numbered port for 6/8-wire, 
        --          set to 0 for 4-wire
        -- 7,6     wirePair of the 4*i numbered port for 8-wire, 
        --          set to 0 for 4/6-wire
        -- 
        -- wirePair   value
        -- *******    ******
        -- 1stPair	00
        -- 2ndPair	01
        -- 3rdPair	10
        -- 4thPair	11
 

ShdslSpanMgmtBySTUR ::= INTEGER {
        disabled(1),
        enabled (2)
}

ShdslSpanState ::= INTEGER {
    unknown(1),
    unequipped(2),
    faulty(3),
    idle(4),
    startup(5),
active(6),
testing(7),
firmwareDownloading(8)
}


ShdslStatusBitmap ::= INTEGER (0.. 8191)
        -- Bitmap with each bit representing a particular condition
        -- as follows:
        -- Bit     Condition
        -- ***     **********
        --   0     No Error
        --   1     LOSW Failure
        --   2     Loop Attenuation Alarm
        --   3     SNR Margin Alarm
        --   4     DC Continuity Fault
        --   5     Device Fault
        --   6     Configuration Error
        --   7     Loopback(s) Active
        --   8     Non Conform to STU-C Enable/Disable of STU-R Initiated
                 --Management Flow
        --   9     ATM LCD (Loss of Cell Delineation)
        --  10     Loss of Power (based on reception of Dying Gasp indication)
        --  11     ATM NCD (No Cell Delineation) 
        --  12     Segment Termination Activation Failure

ShdslPowerBackoffMode ::= INTEGER {
    default(1),
    selected(2),
    disabled(3)
}

ShdslTipRingReversalMode ::= INTEGER {
    normal(1),
    reversed(2)
}

ShdslRegionalSetting ::= INTEGER (0..3)
--Bitmap with each bit a representing supported regional setting
-- per ITU-T G.992.1 Annexes, as follows:
        -- Bit     Setting
        -- ***     ******
        --   0     G.991.2 Annex A/F, Region 1
        --   1     G.991.2 Annex B/G, Region 2

ShdslSpanEFMHandshakeSelection ::= INTEGER { 
auto(1), -- auto select Ieee/itu according to CPE 
ieee(2), -- IEEE 2Base-TL 
itu(3), -- ITU-T G.991.2 PTM 64/65-octet-based 
notApplicable(4) -- not applicable,for ATM/IMA mode 
} 

ShdslSpanEFMHsMode ::= INTEGER {
    unknown(1),
    ieee(2),-- IEEE 2Base-T
    itu(3), -- ITU-T G.991.2 PTM 64/65oTC
    notApplicable(4)  -- not applicable,for ATM/IMA mode
}

ShdslSpanCTCEnable ::= INTEGER {
        disabled(1),
        enabled (2)
}
 
--added by dongxinz 04/06/2009
ShdslActivationState ::= INTEGER {
    active(1), -- segment termination is in data state as per G.991.2.
    idle(2), -- segment termination is in idle state when the line is disabled,
             -- or when the line is enabled but has not yet detected G.hs tones
             -- (R-TONES-REQ) from peer during each auto activation.
    unknown(3), -- segment termination status unknown
initialization(4), -- segment termination is in initializing, including first
                       -- and second G.hs, optional line probing, and core 
                       -- activation as per G.991.2
    exception(5), -- segment termination is in exception state as per G.991.2
testing(6), -- segment termination is in test state
firmwareDownloading(7) -- segment termination not started due to
                      -- chipset firmware download is still ongoing. 
} 
 
--added by tbliu 10/17/2010
ShdslSpanBondingRateCtlMode::= INTEGER  {
	span(1),
        link(2)
}               

-- SHDSL Configuration Group
-- =========================

-- SHDSL Span Configuration Table
-- ==============================
shdslSpanConfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF ShdslSpanConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This table supports overall configuration of the SHDSL span.

         ALCATEL NOTE:
            TABLESIZE:   DEP [number of SHDSL lines]
            DESCR:       "
 ::= { shdslConfiguration 1 }


shdslSpanConfEntry OBJECT-TYPE
    SYNTAX  ShdslSpanConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry in this table represents a single SHDSL span.
         The SHDSL span includes the STU-C, STU-R, and any
         intermediate SRU units.  

         STU-C -- { SRU-1 } ... {SRU-8} == STU-R

         The SHDSL span is associated with a single SHDSL interface
         in the ifTable.  The entry is indexed by the ifIndex of the
         corresponding SHDSL interface.

         Configuration of individual units in the span is
         done via the shdslUnitConfTable.

         Configuration of individual segment terminations in the span is
         done via the shdslSegmentTerminationConfTable.

         ALCATEL NOTE: Interfaces in ISAM are manually provisioned.
         Entries exist in this table for all available
         interfaces in the system, however entries are
         only 'provisioned' based on the RowStatus of this
         entry."
    INDEX   { ifIndex }
::= { shdslSpanConfTable 1 }

ShdslSpanConfEntry ::=
        SEQUENCE {
        shdslSpanRowStatus                   RowStatus,
        shdslSpanConfNumRepeaters     INTEGER,
        shdslSpanOperationalMode        ShdslSpanOperationalMode,
        shdslSpanSpectralProfile            ShdslSpanSpectralProfile,
        shdslSpanWireMode                   ShdslSpanWireMode,
--        shdslSpanConfiguredDataRate    INTEGER,
        shdslSpanMgmtBySTUR           ShdslSpanMgmtBySTUR,
        shdslSpanRegionalSetting          ShdslRegionalSetting,
        shdslSpanMinRequestedDataRate  INTEGER,
        shdslSpanMaxRequestedDataRate  INTEGER,
        shdslSpanTargetRMDownWC     INTEGER,
        shdslSpanTargetRMDownCC     INTEGER,
        shdslSpanTargetRMUpWC     INTEGER,
        shdslSpanTargetRMUpCC     INTEGER,
        shdslSpanLineProbeEnable    INTEGER,
        shdslSpanTCPAMSelection     INTEGER, 
        shdslSpanEFMHandshakeSelection   ShdslSpanEFMHandshakeSelection,
        shdslSpanCTCEnable          ShdslSpanCTCEnable,
        shdslSpanBondingRateControlMode    ShdslSpanBondingRateCtlMode,
        shdslSpanMinRequestedDataRateLink1     INTEGER,
        shdslSpanMaxRequestedDataRateLink1     INTEGER,
	shdslSpanMinRequestedDataRateLink2     INTEGER,
	shdslSpanMaxRequestedDataRateLink2     INTEGER,
	shdslSpanMinRequestedDataRateLink3     INTEGER,
	shdslSpanMaxRequestedDataRateLink3     INTEGER,
	shdslSpanMinRequestedDataRateLink4     INTEGER,
	shdslSpanMaxRequestedDataRateLink4     INTEGER,
	shdslSpanTCPAMSelectionLink1        INTEGER,
	shdslSpanTCPAMSelectionLink2        INTEGER,
	shdslSpanTCPAMSelectionLink3        INTEGER,
	shdslSpanTCPAMSelectionLink4        INTEGER,
	shdslSpanAdminState		    INTEGER	
}

shdslSpanRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "This object controls the configuration of the associated entry
         in this table, and thereby the associated SHDSL interface.
         See description of RowStatus for syntax & behavior.

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       MAND
            PERSIST:     YES          INSRVMOD:    YES
            RANGE:       notInService | active
            DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
DESCR:       In the ISAM system, entries in this table
         may NOT be created/deleted by a manager.  The agent automatically
         creates entries associated with all available shdsl interfaces
         in the system.

         Only supported values for this object are:
            'notInService' - This interface is not to be used.
                             (unprovisioned)
            'active'       - This interface is provisioned to be used.
                             (provisioned)

         By default interfaces are marked 'notInService'.  In this
         state, the manager may configure various parameters associated
         with this entry, but these changes are not stored persistently
         (ie: re-initialization of the agent would lose any changes).
         Also, no fault/performance monitoring is available in this state.
         To provision the interface for use and to store its configuration
         persistently, the manager shall set this object to 'active'.
         If the configuration associated with this entry is not accepted by
         the agent, the set will be denied.

         To remove an interface (unprovision), the manager shall set this
         object to 'notInService'.  Doing this also resets all configuration
         parameters associated with this entry to their default values.
         An interface in the 'native' mode ( see shdslSpanOperationalMode )
         may not be unprovisioned as long as VPL/VCL entries exist on the
         associated ATM interface.  Interfaces in 'ima' mode may not be
         unprovisioned if they are in use as an IMA link. 

         The individual links associated with this span will be made 'up' when 
         the span set to 'active'. Similarly links associated with this span 
         will be made 'down' when the span set to 'notInService'"

::= { shdslSpanConfEntry 1 }

shdslSpanConfNumRepeaters OBJECT-TYPE
    SYNTAX  INTEGER  (0..8)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "This parameter configures the number of SRU (repeater) units to
         be configured for this SHDSL span.  It controls the number of
         SRU (repeater) entries in the shdslUnitConfTable. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       OPT
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       0..8         DEFVALUE:    0
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"

    DEFVAL  { 0 }
    ::= { shdslSpanConfEntry 2 }

shdslSpanOperationalMode OBJECT-TYPE
    SYNTAX      ShdslSpanOperationalMode
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "This object specifies the mode in which this SHDSL span
         is to be configured.  Two options are supported:
            native : SHDSL span used to directly transport ATM traffic.
            ima    : SHDSL span used as a link in an IMA group.
            efm    : SHDSL span used to transport  EFM traffic.
	    tdm    : SHDSL span used to transport  TDM traffic.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       OPT
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       native | ima | efm | tdm
            DEFVALUE:    native
            UNITS:       NA           SPARSE:      NA
            DESCR:       The value of this object can only be set prior to 
                         activating the associated entry 
                         (see 'shdslSpanRowStatus').
                         Only Interfaces which are provisioned in 'ima'
                         mode may be associated with an IMA Group using the
                         imaLinkTable in the IMA-MIB."

    DEFVAL {native}
    ::= { shdslSpanConfEntry 3 }


shdslSpanSpectralProfile OBJECT-TYPE
    SYNTAX      ShdslSpanSpectralProfile
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "This object specifies the spectral mode in which this SHDSL span
         is to be configured  with.  Two options are supported:

            symmetricProfile 
            assymmetricProfile. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       OPT
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       symmetricProfile
            DEFVALUE:    symmetricProfile
            UNITS:       NA           SPARSE:      NA
            DESCR:       asymmericProfile is not supported. 
"

     DEFVAL {symmetricProfile}      
 ::= { shdslSpanConfEntry 4 }

shdslSpanWireMode OBJECT-TYPE
    SYNTAX      ShdslSpanWireMode
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "This object specifies whether multi-pair operation is in place.

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       OPT
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       twoWire | fourWire | sixWire | eightWire
            DEFVALUE:    twoWire
            UNITS:       NA           SPARSE:      NA
            DESCR:       The value of this object can only be set
         prior to activating the associated entry (see shdslSpanRowStatus).
        
         Only the lines corresponding to odd numbered port
         (starting form 1)can be provisioned in 4-Wire mode due to a 
         legacy phy-assignment restriction.

         Only the lines corresponding to 4*I-3 numbered port
         (starting form 1)can be provisioned in 6-Wire/8-Wire mode to 
          simplify M-pair provision conditions.
          
          If the shdslSpanBondingRateControlMode is link and 
             shdslSpanWireMode is fourWire mode, only shdslSpanMinRequestedDataRateLink1/2 and shdslSpanMaxRequestedDataRateLink1/2  take effect.
          If the shdslSpanBondingRateControlMode is link 
             and shdslSpanWireMode is sixWire mode, only shdslSpanMinRequestedDataRateLink1/2/3 and shdslSpanMaxRequestedDataRateLink1/2/3 take effect.
          If the shdslSpanBondingRateControlMode is link and 
             shdslSpanWireMode is eightWire mode, only shdslSpanMinRequestedDataRateLink1/2/3/4 and shdslSpanMaxRequestedDataRateLink1/2/3/4 take effect.
             
          If shdslSpanWireMode is twoWire, only shdslSpanTCPAMSelectionLink1 take effect.        
          If shdslSpanWireMode is fourWire, only shdslSpanTCPAMSelectionLink1/2 shall effect.
          If shdslSpanWireMode is sixWire, only shdslSpanTCPAMSelectionLink1/2/3 shall effect.
          IfshdslSpanWireMode is eightWire, only shdslSpanTCPAMSelectionLink1/2/3/4 shall effect. "

     DEFVAL {twoWire}
 ::= { shdslSpanConfEntry 5 }


