  domainMacAuthFailure OBJECT IDENTIFIER 
  ::= { domainMIB 15}
  
    domainTrapMacAddress OBJECT-TYPE
	SYNTAX MacAddress 
        ACCESS read-only
        STATUS  mandatory
        DESCRIPTION
            "Conflict Mac Address which Radius Server distribute 
	     Mac based authentication on System level."
    ::= { domainMacAuthFailure 1}

    domainTrapIfIndex OBJECT-TYPE
	SYNTAX InterfaceIndex 
        ACCESS read-only
        STATUS  mandatory
        DESCRIPTION
            "The IfIndex of a user interface with Mac address
             to identify the mac based authentication session."
    ::= { domainMacAuthFailure 2}

    domainTrapDynAssignVlanId OBJECT-TYPE
	SYNTAX Gauge
        ACCESS read-only
        STATUS  mandatory
        DESCRIPTION
            " Dynamic Assignment of Forwarder ID for a
	      mac based authentication session."
    ::= { domainMacAuthFailure 3}

    domainTrapQoSProfName OBJECT-TYPE
	SYNTAX OCTET STRING (SIZE(0..32))
        ACCESS read-only
        STATUS  mandatory
        DESCRIPTION
            " Dynamic Qos Profile Name for a mac based 
	      authentication session."
    ::= { domainMacAuthFailure 4}

    domainTrapMacAuthFailureReason OBJECT-TYPE
	SYNTAX INTEGER 
        ACCESS read-only
        STATUS  mandatory
        DESCRIPTION
            "The reason for dhcp session preemption.
             1.Invalid Dynamic VLAN Assignment Failure: VLAN returned
	       from RADIUS server is invalid: not configured locally, 
	       or mismatched before and after re-auth or occupied.
             2.Static Mac Exist Failure: request mac address is already
	       configured as static mac.
	     3.Bridge Port Not Exist Failure: Bridge Port not exist.
	     4.Invalid Dynamic QoS Profile Assignment Failure:  
	       QoS profile returned from RADIUS server is 
	       not configured locally, or mismatched before and after
	       re-auth.
	     5.Invailed RADIUS VSA infomation
	     6.No vailed vlan information from RADIUS
	     7.Mac based Authentication Attache failed"
    ::= { domainMacAuthFailure 5}
 
