E1-MIB DEFINITIONS ::= BEGIN

IMPORTS
    OBJECT-TYPE
        FROM RFC-1212
    RowStatus, TruthValue, DisplayString
        FROM SNMPv2-TC
    Counter, enterprises, TimeTicks
        FROM RFC1155-SMI
    ifIndex
        FROM ITF-MIB
    asam
        FROM SYSTEM-MIB
    NetworkTimeInSeconds
        FROM ASAM-TC-MIB;


asamDsx1E1MIB OBJECT IDENTIFIER ::= { asam 16 }

e1MIB OBJECT IDENTIFIER ::= { asamDsx1E1MIB 3 }

E1LineCoding ::= INTEGER {
	ami(1),
	hdb3(2)
}

E1FramingMode ::= INTEGER {
	unframed(1),
	framed(2) 
}

E1TransmitClockSource ::= INTEGER {
	external(1),
	internal(2)
}

E1LoopbackType ::= INTEGER {
	noLoop(1),
        local-Payload(2),
  	remote-Payload(3),
	remote-Line(4),
	local-Line(5)
}

E1BERTestPattern ::= INTEGER {
	framed-twoPow11(1),	-- Framed Pseudo-random repeating test pattern with 2^11 bits
	framed-twoPow15(2),     -- Framed Pseudo-random repeating test pattern with 2^15 bits
        unframed-twoPow11(3),	-- Unframed Pseudo-random repeating test pattern with 2^11 bits
	unframed-twoPow15(4),  -- Unframed Pseudo-random repeating test pattern with 2^15 bits
	none(5)			-- Default Value
}

E1BERTestStatus ::= INTEGER {
	none(1),
	running(2),
	aborted(3),
	completed(4)
}

E1SignalMode ::= INTEGER {
                   none(1),
                   robbedBit(2),
                   bitOriented(3),
                   messageOriented(4),
                   other(5)
}

E1LineImpedanceMode ::= INTEGER {
                   unbalanced75ohms(1),
                   balanced120ohms(2)
}

E1CRCEnable ::= INTEGER {
				disable(1),
				enable(2)
}

E1MultiframingEnable ::= INTEGER {
				disable(1),
				enable(2)
}

E1SegmentSide ::= INTEGER {
				nearEnd(1),
				farEnd(2)
}


-- Groups in e1MIB

e1Configuration OBJECT IDENTIFIER ::= { e1MIB 1 }
e1Status        OBJECT IDENTIFIER ::= { e1MIB 2 }
e1Performance   OBJECT IDENTIFIER ::= { e1MIB 3 }
e1Diagnostic    OBJECT IDENTIFIER ::= { e1MIB 4 }

-- Textual conventions


-- E1 configuration table
-- ======================
e1ConfigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF E1ConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This table supports overall configuration of the E1 interface.

         ALCATEL NOTE:
            TABLESIZE:   DEP [number of E1 lines]
            DESCR:       "
 ::= { e1Configuration 1 }

e1ConfigEntry OBJECT-TYPE
    SYNTAX E1ConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
	 "An entry in this table represents a single E1 interface.
 	  The E1 configuration is associated with single E1 interface 
          in the ifTable. The entry is indexed by the ifIndex of corresponding 
          E1 interface

	ALCATEL NOTE: Interfaces in ISAM are manually provisioned.
         Entries exist in this table for all available
         interfaces in the system, however entries are
r        only 'provisioned' based on the e1RowStatus of this
         entry."

    INDEX   { ifIndex }
::= { e1ConfigTable 1 }


E1ConfigEntry ::=
          SEQUENCE {
			e1RowStatus			RowStatus,
			e1LineCoding			E1LineCoding,
			e1FramingMode		E1FramingMode,
			e1EnableCRC			E1CRCEnable,
			e1EnableMultiFraming	E1MultiframingEnable,
			e1TransmitClockSource 	E1TransmitClockSource,
			e1SignalMode      E1SignalMode,
			e1LineImpedance   E1LineImpedanceMode,
			e1AdminState		 INTEGER
     	 }


e1RowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
        "This object controls the configuration of the associated entry
         in this table, and thereby the associated SHDSL interface.
         See description of RowStatus for syntax & behavior.

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       MAND
            PERSIST:     YES          INSRVMOD:    YES
            RANGE:       notInService | active
            DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
    DESCR:       :       In the ISAM system, entries in this table
         may NOT be created/deleted by a manager.

         Entries are created in this table when 'shdslSpanOperationalMode'
	is set to 'tdm'. Number of entries in this table is equal to
	number of lines set to TDM mode. If 'shdslSpanOperationalMode' of
	any E1 line is set to other than 'tdm', the specific row corresponds
	to that line is deleted from this table.
"

::= { e1ConfigEntry 1 }


e1LineCoding OBJECT-TYPE
	SYNTAX  E1LineCoding
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Encoding type of this interface.
		
		   ami - Specifies Alternate Mark Inversion (AMI) as the linecode type
		   hdb3 - Specifies high-density binary 3 (HDB3) as the linecode type. This is the default for E1 lines.

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: ami | hdb3			DEFVALUE: hdb3
			UNITS: NA			SPARSE:  NO
			DESCR: NA 							"	
	
	DEFVAL {hdb3}
::= { e1ConfigEntry 2 }


e1FramingMode OBJECT-TYPE
	SYNTAX  E1FramingMode
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Specifies framing of e1 interface.
			unframed - Unframed E1.
			framed - Framed E1 type.
             

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: unframed|framed		DEFVALUE: unframed
			UNITS: NA			SPARSE:  NO
			DESCR:NA								"
	
	DEFVAL {unframed}
::= { e1ConfigEntry 3 }


e1EnableCRC OBJECT-TYPE
	SYNTAX  E1CRCEnable
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Whether to enable or disable CRC4 for this e1 interface.
		   disable - no CRC4
		   enable - Enables CRC4.
		 This setting makes sense only if e1FramingMode to be set to 'framed'.
             

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: disable | enable		DEFVALUE: disable
			UNITS: NA			SPARSE:  NO
			DESCR: NA				"
	
	DEFVAL {disable}
::= { e1ConfigEntry 4 }


e1EnableMultiFraming OBJECT-TYPE
	SYNTAX  E1MultiframingEnable
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Specifies if multi framing to be used. 
		  disable - No multi framing.
		  enable - Enable multi framing.
		
		This parameter needs e1FramingMode to be set to 'framed'.

             

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: disable | enable		DEFVALUE: disable
			UNITS: NA			SPARSE:  NO
			DESCR: NA	 "
	
	DEFVAL {disable}
::= { e1ConfigEntry 5 }


e1TransmitClockSource OBJECT-TYPE
	SYNTAX  E1TransmitClockSource
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Specifies clock source to be used for this e1 interface.
		   external - Specifies that the network clock source is used. This is the default.
		   internal - Specifies that the internal clock source is used.
             

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: external | internal	DEFVALUE: external
			UNITS: NA			SPARSE:  NO
			DESCR: NA "
	
	DEFVAL {external}
::= { e1ConfigEntry 6 }


e1SignalMode OBJECT-TYPE
	SYNTAX  E1SignalMode
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"'none' indicates that no bits are reserved for
          signaling on this channel.

          'robbedBit' indicates that DS1 Robbed Bit Signaling
          is in use.

          'bitOriented' indicates that E1 Channel Associated
          Signaling is in use.

          'messageOriented' indicates that Common Channel
          Signaling is in use on either channel 16 of
          an E1 link or channel 24 of a DS1.

		 others if the mode is different from the ones 
		 Specified above.
		
		Note: e1SignalMode is NOT supported.FR - ALU02327678             

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: 1..5 		DEFVALUE: none
			UNITS: NA			SPARSE:  NO
			DESCR: NA "
	
	DEFVAL {none}
::= { e1ConfigEntry 7 }


e1LineImpedance OBJECT-TYPE
	SYNTAX  E1LineImpedanceMode
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Nominal line impedance.             

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: unbalanced75ohms|balanced120ohms    
			DEFVALUE: balanced120ohms
			UNITS: NA			SPARSE:  NO
			DESCR: NA "
	
	DEFVAL {balanced120ohms}
::= { e1ConfigEntry 8 }

e1AdminState  OBJECT-TYPE
    SYNTAX      INTEGER{
				 up(1),
            	    down(2)
		}
   ACCESS      read-write
   STATUS      mandatory
   DESCRIPTION
         "This object specifies the admin state E1 interface.

        ALCATEL NOTE:
        ACCESS:      NA          USAGE:       OPT
        PERSIST:     YES         INSRVMOD:    NA
        RANGE:       up|down
        DEFVALUE:    down
        UNITS:       NA          SPARSE:      NO
        DESCR:       NA"
    DEFVAL {down}
 ::= { e1ConfigEntry 9 }


--E1 Operational Table
--====================

e1OperationalTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF E1OperationalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This table contains the operational parameters for the provisioned E1 interface.

         ALCATEL NOTE:
            TABLESIZE:   DEP [number of E1 lines]
            DESCR:       "
 ::= { e1Status 1 }

e1OperationalEntry OBJECT-TYPE
    SYNTAX  E1OperationalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
	    "An entry in this table represents the status of single E1 interface.
		The entry is indexed by the ifIndex of corresponding E1 interface"

    INDEX   { ifIndex }
::= { e1OperationalTable 1 }


E1OperationalEntry ::=
          SEQUENCE {
		    e1ActualDatarate		INTEGER,
			e1ActualLineCoding		E1LineCoding,
			e1ActualFramingMode		E1FramingMode,
			e1ActualCRC			E1CRCEnable,
			e1ActualMultiframing	        E1MultiframingEnable,
			e1ActualTransmitClockSource 	E1TransmitClockSource,
			e1OperationalStatus			INTEGER,
			e1ActualLineImpedance	E1LineImpedanceMode
     }


e1ActualDatarate OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Retrieves Actual datarate on this e1 interface.

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: 192..2048			DEFVALUE: NA
			UNITS: kb/s			SPARSE:  NA
			DESCR: The data rate will be a multiple of 64 kb/s.
                    While the E1 line is not activated,
                    the value of this object is 0.
        "
::= { e1OperationalEntry 1 }


e1ActualLineCoding OBJECT-TYPE
	SYNTAX  E1LineCoding
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Retrieves the actual Encoding type of this interface.
		
		   ami - Specifies Alternate Mark Inversion (AMI) as the linecode type
		   hdb3 - Specifies high-density binary 3 (HDB3) as the linecode type.

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: NA			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA  
        "
	
	
::= { e1OperationalEntry 3 }

e1ActualFramingMode OBJECT-TYPE
	SYNTAX  E1FramingMode
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Retrieves the framing of e1 interface.
			unframed - Unframed E1.
			framed - Framed E1 type.


		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: NA			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA  
        "
	
	
::= { e1OperationalEntry 4 }

e1ActualCRC OBJECT-TYPE
	SYNTAX  E1CRCEnable
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"CRC4 setting of this e1 interface.
		   disable - no CRC4
		   enable - Enables CRC4.



		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: NA			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA 
        "
	
	
::= { e1OperationalEntry 5 }

e1ActualMultiframing OBJECT-TYPE
	SYNTAX  E1MultiframingEnable 
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Specifies if multi framing is used. 
		  disable - No multi framing.
		  enable - Enable multi framing.

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: NA			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA 
        "
	
	
::= { e1OperationalEntry 6 }

e1ActualTransmitClockSource OBJECT-TYPE
	SYNTAX  E1TransmitClockSource
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Specifies clock source of this e1 interface.
		   external - Specifies that the network clock source is used. This is the default.
		   internal - Specifies that the internal clock source is used.


		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: NA			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA 
        "
	
	
::= { e1OperationalEntry 7 }

e1OperationalStatus OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"shows the line status of the e1 interface.
		active(1) - Specifies the line is in active state.
         	noactive(2) - Specifies the line is in not in service.	        

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  NA
			PERSIST: NO			INSRVMOD: NA
			RANGE: active | noactive			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA 
        "
	
	
::= { e1OperationalEntry 8 }

e1ActualLineImpedance OBJECT-TYPE
	SYNTAX  E1LineImpedanceMode
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"shows the actual line impedance of the e1 interface.

		ALCATEL NOTE:
			ACCESS: NA			USAGE:  OPT
			PERSIST: YES			INSRVMOD: NA
			RANGE: unbalanced75ohms|balanced120ohms
			DEFVALUE: NA
			UNITS: NA			SPARSE:  NA
			DESCR: NA
        "
	
	
::= { e1OperationalEntry 10 }


--E1 Performance Tables
--=====================

e1Curr15MinTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF E1Curr15MinEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This table contains 15 minutes counters for provisioned E1 interfaces.

         ALCATEL NOTE:
            TABLESIZE:   DEP [number of E1 lines]
            DESCR:       "
 ::= { e1Performance 1 }

e1Curr15MinEntry OBJECT-TYPE
    SYNTAX E1Curr15MinEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
	    "An entry in this table represents 15 Min PM counter values of single E1 interface.
		The entry is indexed by the ifIndex and e1Curr15MinSegmentSide of corresponding E1 interface"

		

    INDEX   {ifIndex,e1Curr15MinSegmentSide}
::= { e1Curr15MinTable 1 }


E1Curr15MinEntry ::=
		SEQUENCE {
			e1Curr15MinSegmentSide					E1SegmentSide,
			e1Curr15MinTimeElapsed			INTEGER,
			e1Curr15MinES					Counter,
			e1Curr15MinSES					Counter,
			e1Curr15MinSEFS			        Counter,
			e1Curr15MinUAS					Counter,
			e1Curr15MinCSS					Counter,
			e1Curr15MinPCV					Counter,
			e1Curr15MinLES					Counter,
			e1Curr15MinBES					Counter,
			e1Curr15MinLCV					Counter,
			e1Curr15MinValidIntervals       INTEGER,
			e1Curr15MinInvalidDataFlag		INTEGER
		}
		
e1Curr15MinSegmentSide  OBJECT-TYPE
    SYNTAX  E1SegmentSide
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This object identifies the particular side(near-end/far-end)
		of an E1 interface.
	NOTE: far-end PM data collection is not supported for E1

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    NA
            RANGE:       nearEnd | farEnd
            DEFVALUE:    nearEnd
            UNITS:       NA           SPARSE:      NA
			DESCR:       NA"

	 DEFVAL {nearEnd}
    ::= { e1Curr15MinEntry 1 }
	
e1Curr15MinTimeElapsed OBJECT-TYPE
    SYNTAX       INTEGER (0..900)
    ACCESS   	 read-only
    STATUS       mandatory
    DESCRIPTION
        "Total elapsed seconds in the current 15-minute interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       0..900       DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 2 }

e1Curr15MinES OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Errored Seconds (ES) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 3 }


e1Curr15MinSES OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Severely Errored Seconds (SES) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 4 }


e1Curr15MinSEFS OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Severely Errored Framed Seconds (SEFS) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 5 }


e1Curr15MinUAS OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Unavailable Seconds (UAS) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 6 }


e1Curr15MinCSS OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Controlled Slip Seconds (CSS) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 7 }

e1Curr15MinPCV OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Path Code Violation (PCV) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 8 }


e1Curr15MinLES OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Line Error Seconds (LES) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 9 }


e1Curr15MinBES OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Bursty Error Seconds (BES) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       seconds      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 10 }

e1Curr15MinLCV OBJECT-TYPE
    SYNTAX       Counter
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        "Count of Line Code Violation (LCV) in the current 15-minute 
        interval. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 12 }

e1Curr15MinValidIntervals      OBJECT-TYPE
    SYNTAX              INTEGER (0..96)
    ACCESS              read-only
    STATUS              mandatory
    DESCRIPTION 
       "The number of previous intervals for which data has been
         stored. In normal conditions the value of this object equals the
         maximum number of intervals supported by the system. If the 
         the relevant device was brought online more recently,
         the value will be the number of complete 15 minute
         intervals the device has been online. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       0..96        DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
	::= {e1Curr15MinEntry 13}

e1Curr15MinInvalidDataFlag OBJECT-TYPE
    SYNTAX       INTEGER (0..1)
    ACCESS   read-only
    STATUS       mandatory
    DESCRIPTION
        " This flag indicates the validity of the corresponding PM data 
        in current 15 minutes. If the measured time deviates by more than
		10 seconds from the nominal interval duration (900 seconds for 
		a 15 minutes interval), then the data is seen as invalid. 
        The flag is set to 1 when the PM data is incomplete or invalid and set to 0
        when the PM data is complete and valid. 

         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       0..1           DEFVALUE:   NA
            UNITS:       NA      SPARSE:      NA
            DESCR:       NA"
    ::= { e1Curr15MinEntry 14 }


e1Curr1DayTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF E1Curr1DayEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This table contains 1 day counters for provisioned E1 interfaces.

         ALCATEL NOTE:
            TABLESIZE:   DEP [number of E1 lines]
            DESCR:       "
 ::= { e1Performance 2 }

e1Curr1DayEntry OBJECT-TYPE
    SYNTAX E1Curr1DayEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
	    "An entry in this table represents 1 day PM counter values of single E1 interface.
		The entry is indexed by the ifIndex and e1Curr1DaySegmentSide of corresponding E1 interface"

		

    INDEX   { ifIndex,e1Curr1DaySegmentSide}
::= { e1Curr1DayTable 1 }


E1Curr1DayEntry ::=
		SEQUENCE {
			e1Curr1DaySegmentSide					E1SegmentSide,
			e1Curr1DaysTimeElapsed 			INTEGER,
			e1Curr1DayES					Counter,
			e1Curr1DaySES					Counter,
			e1Curr1DaySEFS			        Counter,
