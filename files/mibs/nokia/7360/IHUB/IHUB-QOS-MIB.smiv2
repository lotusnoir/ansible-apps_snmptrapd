-- ----------------------------------------
-- Ingress QoS Policy Table
-- ----------------------------------------

ihubQosIngressPolicyTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF IhubQosIngressPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
    "This object contains a table for creating and storing policies for ingress
     QoS. A maximum of 32 policies are allowed."
::= { ihubIngressQos 1  }


ihubQosIngressPolicyEntry OBJECT-TYPE
    SYNTAX IhubQosIngressPolicyEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    	  "This table contains as row a single ingress QoS policy.
          Policy (1) is the default policy which is created
          automatically. The other policies are created by user.
          The default policy cannot be modified or deleted."
    	INDEX {ihubQosIngressPolicyIndex}
::= { ihubQosIngressPolicyTable 1}


IhubQosIngressPolicyEntry ::=
 SEQUENCE
    { 
       ihubQosIngressPolicyIndex                  Integer32, 
       ihubQosIngressPolicyRowStatus              RowStatus, 
       ihubQosIngressPolicyScope                  TItemScope,
       ihubQosIngressPolicyDescription            TItemDescription,
       ihubQosIngressPolicyRefCount               PolicyRefCount,
       ihubQosIngressPolicyDefaultFC              TNamedItem,
       ihubQosIngressPolicyDefaultProfile         TProfile,
       ihubQosIngressPolicyUntaggedDot1p          Dot1PPriority,
       ihubQosIngressPolicyLastChanged            TimeStamp,
       ihubQosIngressPolicyColorSelector	  INTEGER
    }

ihubQosIngressPolicyIndex OBJECT-TYPE
    SYNTAX       Integer32 (1..65535)
    MAX-ACCESS       not-accessible
    STATUS       current
    DESCRIPTION
        "The policy ID is the index to this table."
    ::= { ihubQosIngressPolicyEntry 1 }

ihubQosIngressPolicyRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Row Status for the policy. The deletion of this row
         has an action of removing the dependent rows in the 
         following tables :
           - ihubQosIngressDscpTable
           - ihubQosIngressPrecedenceTable
           - ihubQosIngressDot1pTable"
    ::= { ihubQosIngressPolicyEntry 2 }

ihubQosIngressPolicyScope OBJECT-TYPE
    SYNTAX       TItemScope
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Scope of the policy. If the scope is 'exclusive', it can only be
         associated with a single service (VPRN, IES, V-VPLS, EPIPE, VPLS, 
         base router). If the scope is 'template', it can be associated with 
         multiple items"
    DEFVAL { template }
    ::= { ihubQosIngressPolicyEntry 3 }

ihubQosIngressPolicyDescription OBJECT-TYPE
    SYNTAX       TItemDescription
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "User provided description of the policy."
    DEFVAL { ''H }
    ::= { ihubQosIngressPolicyEntry 4 }

ihubQosIngressPolicyRefCount  OBJECT-TYPE
    SYNTAX       PolicyRefCount
    MAX-ACCESS       read-only
    STATUS       current
    DESCRIPTION
        "Indicates the number of entities using this policy. If the
         reference count is greater than 0, then this policy is 'in use',
         and may NOT be deleted. If the value is 0, then this policy is
         'not in use', and can be deleted."
    ::= { ihubQosIngressPolicyEntry 5 }

ihubQosIngressPolicyDefaultFC OBJECT-TYPE
    SYNTAX       TNamedItem
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Specifies the forwarding class which should be used for 
         all traffic that does not match a specific classification 
         specified in this ingress QoS policy. If not specified 
         explicitly, all traffic will be classified as best-effort 
         'be'."
    DEFVAL { 'be'H }
    ::= { ihubQosIngressPolicyEntry 6 }

ihubQosIngressPolicyDefaultProfile OBJECT-TYPE
    SYNTAX       TProfile
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Specifies the profile (i.e. in or out) which should be used for 
         all traffic that does not match a specific classification 
         specified in this ingress QoS policy. If not specified 
         explicitly, all traffic will be classified as out of profile."
    DEFVAL { in }
    ::= { ihubQosIngressPolicyEntry 7 }

ihubQosIngressPolicyUntaggedDot1p OBJECT-TYPE
    SYNTAX       Dot1PPriority
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Specifies the p-bits which should be used for all untagged 
         traffic."
    DEFVAL { 0 }
    ::= { ihubQosIngressPolicyEntry 8 }

ihubQosIngressPolicyLastChanged OBJECT-TYPE
    SYNTAX       TimeStamp
    MAX-ACCESS       read-only
    STATUS       current
    DESCRIPTION
        "Timestamp of last change to this row in ihubQosIngressTable. Note 
         that this does not include changes to ihubQosIngressPolicyRefCount."
    ::= { ihubQosIngressPolicyEntry 9 }

ihubQosIngressPolicyColorSelector OBJECT-TYPE
    SYNTAX	 INTEGER {
			config-entry-based (0),
                        packet-dei-based (1)
                          }	     
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Allows to choose whether color profile is based on packet-DEI-bit or
         policy configuration."
    DEFVAL {0}
    ::= { ihubQosIngressPolicyEntry 10 }

-- --------------------------------------
-- Ingress QoS DSCP Table
-- --------------------------------------

ihubQosIngressDSCPTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IhubQosIngressDSCPEntry
    MAX-ACCESS       not-accessible
    STATUS       current
    DESCRIPTION
        "A list of all DSCP entries for all ingress QOS policies. A maximum of 
         32 x 64 = 2048 entries are allowed."
    ::= { ihubIngressQos 2 }

ihubQosIngressDSCPEntry OBJECT-TYPE
    SYNTAX       IhubQosIngressDSCPEntry
    MAX-ACCESS       not-accessible
    STATUS       current
    DESCRIPTION
        "Information about a particular DSCP entry for a ingress QOS policy.
         Entries are created by user. Entries are deleted by user."
    INDEX { ihubQosIngressPolicyIndex, ihubQosIngressDSCP }
    ::= { ihubQosIngressDSCPTable 1 }

IhubQosIngressDSCPEntry ::= SEQUENCE
    {
        ihubQosIngressDSCP                   TDSCPName,
        ihubQosIngressDSCPRowStatus          RowStatus,
        ihubQosIngressDSCPFC                 TNamedItemOrEmpty,
        ihubQosIngressDSCPProfile            TProfileOrNone,
        ihubQosIngressDSCPLastChanged        TimeStamp
    }

ihubQosIngressDSCP OBJECT-TYPE
    SYNTAX       TDSCPName
    MAX-ACCESS       not-accessible
    STATUS       current
    DESCRIPTION
        "Specifies the DSCP name."
    ::= { ihubQosIngressDSCPEntry 1 }

ihubQosIngressDSCPRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "This object allows creation/deletion of rows in this table."
    ::= { ihubQosIngressDSCPEntry 2 }

ihubQosIngressDSCPFC OBJECT-TYPE
    SYNTAX       TNamedItemOrEmpty
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Specifies the forwarding class to be used to classify 
         all the traffic that matches the specific DSCP i.e. 
         ihubQosIngressDSCP. Unless ihubQosIngressDSCPFC 
         is specified explicitly, this match will be ignored 
         for classification purposes."
    DEFVAL { 'be'H }
    ::= { ihubQosIngressDSCPEntry 3 }

ihubQosIngressDSCPProfile OBJECT-TYPE
    SYNTAX       TProfileOrNone
    MAX-ACCESS       read-write
    STATUS       current
    DESCRIPTION
        "Specifies the profile (in or out) to be used to classify 
         all the traffic that matches the specific DSCP i.e. 
         ihubQosIngressDSCP. Unless ihubQosIngressDSCPProfile 
         is specified explicitly, this match will be ignored 
         for classification purposes."

    DEFVAL { in }
    ::= { ihubQosIngressDSCPEntry 4 }

ihubQosIngressDSCPLastChanged OBJECT-TYPE
    SYNTAX       TimeStamp
    MAX-ACCESS       read-only
    STATUS       current
    DESCRIPTION
        "Timestamp of last change to this row in ihubQosIngressDSCPTable."
    ::= { ihubQosIngressDSCPEntry 5 }


