-- ********************************************
-- System Level Ingress Dot1p to FC/Color Table
-- ********************************************

ihubIngressSystemDot1pTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IhubIngressSystemDot1pEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The System level configuration table used to  specify the input color to be used to classify 
         all the traffic that matches the specific Dot1p. The input color set at the system level will 
         be taken by the per Service Egress rate limit configured in  fadSvcBaseExtTable or 
         ihubEgressPerCosQosTable. The table has in total eight entries corresponding to each Dot1p.
         " 
    ::= { ihubIngressSystemCosQos 2 }

ihubIngressSystemDot1pEntry OBJECT-TYPE
    SYNTAX       IhubIngressSystemDot1pEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Information about a particular Dot1p entry at system level."
    INDEX { ihubIngressSystemDot1pValue }
    ::= { ihubIngressSystemDot1pTable 1 }

IhubIngressSystemDot1pEntry ::= SEQUENCE
    {
        ihubIngressSystemDot1pValue              Dot1PPriority,
        ihubIngressSystemDot1pProfile            TProfileOrNone,
        ihubIngressSystemDot1pLastChanged        TimeStamp
    }

ihubIngressSystemDot1pValue OBJECT-TYPE
    SYNTAX       Dot1PPriority
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        " Specifies the dot1p value."
    ::= { ihubIngressSystemDot1pEntry 1 }

ihubIngressSystemDot1pProfile OBJECT-TYPE
    SYNTAX       TProfileOrNone
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "Specifies the profile (in or out) to be used to classify 
         all the traffic that matches the specific Dot1p i.e. 
         ihubIngressSystemDot1pValue. ihubIngressSystemDot1pProfile 
         is required to be configured during an entry creation.
         If ihubIngressSystemDot1pProfile is not explicitly specified
         for a dot1p then it is considered as 'in' by default"
    ::= { ihubIngressSystemDot1pEntry 2 }

ihubIngressSystemDot1pLastChanged OBJECT-TYPE
    SYNTAX       TimeStamp
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Timestamp of last change to this row in ihubIngressSystemDot1pTable."
    ::= { ihubIngressSystemDot1pEntry 3 }
