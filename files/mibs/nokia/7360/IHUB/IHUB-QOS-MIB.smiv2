-- ***************************************************
-- Egress Per Cos/VLAN Ratelimiting table
-- ****************************************************

ihubEgressPerCosQosTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IhubEgressPerCosQosEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table specifies the Egress per Cos Rate limiting parameters for
        a service (only for V-VPLS in >= ISR43 Release)"
    ::= { ihubEgressPerCosQos 1 }

ihubEgressPerCosQosEntry OBJECT-TYPE
    SYNTAX      IhubEgressPerCosQosEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Specifies rate limiting information at the per service/cos level"
    INDEX { svcId,ihubEgressPerCosQosPbitsIndex }
    ::= { ihubEgressPerCosQosTable 1 }

IhubEgressPerCosQosEntry ::=
    SEQUENCE {
    	ihubEgressPerCosQosPbitsIndex	 Integer32,    	
    	ihubEgressPerCosQosRowStatus     RowStatus,
    	ihubEgressPerCosQosPktPrio	 Integer32,
    	ihubEgressPerCosQosCirRate       TCIRRate,
	ihubEgressPerCosQosPirRate       TPIRRate,
	ihubEgressPerCosQosCBurstSize    IhubRateLimitBurstSize, 
        ihubEgressPerCosQosPBurstSize    IhubRateLimitBurstSize,
        ihubEgressPerCosQostrTcmMode     INTEGER,
        ihubEgressPerCosQosAdminStatus	 INTEGER,
	ihubEgressPerCosQosLastChanged	 TimeStamp
    }

ihubEgressPerCosQosPbitsIndex OBJECT-TYPE
	SYNTAX      Integer32 (1..8)
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
	" Specifies the Index to be used for the given set of 802.1p bits in  ihubEgressPerCosQosPktPrio. 
	  For aggregate metering the same ihubEgressPerCosQosPbitsIndex can be shared across different
	  dot1p's specified in the bitmap of ihubEgressPerCosQosPktPrio." 
	::= { ihubEgressPerCosQosEntry 1 }  
	
ihubEgressPerCosQosRowStatus OBJECT-TYPE
    SYNTAX       RowStatus
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "The status of this conceptual row. This object is used to
        manage creation and deletion of rows in this table.         
        The values supported during a set operation are 'createAndGo' 
         'destroy' ."
    ::= { ihubEgressPerCosQosEntry 2 }
    

 
ihubEgressPerCosQosPktPrio OBJECT-TYPE
	SYNTAX      Integer32 (1..255)				
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	"Specifies the dot1p value used to classify the packets to apply
	the egress rate limiting. It is specified as a bitmap of 8 bits where
	each bit corresponds to a dot1p value.
	For example: 
		Dot1p 0 : 0x1
		Dot1p 1 : 0x2
		Dot1p 2 : 0x4
		..
		Dot1p 7 : 0x80		
	To Specify Dot1p 0, 2,4 --> 0x15 	
	" 
        DEFVAL  { 255 }        
        ::= { ihubEgressPerCosQosEntry 3 }
        
ihubEgressPerCosQosCirRate OBJECT-TYPE
	SYNTAX      TCIRRate
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "The value of this object specifies committed information rate for the
	     service, in units of kbps. The default is -1 which indicates that
	     no rate limit is applied.
	ALCATEL NOTE: 
		 ACCESS:      read-write             USAGE:          OPT
		 PERSIST:     NA                     INSRVMOD:       NA
		 RANGE:       -1|0..100000000        DEFVALUE:       -1
		 UNITS:       kbps                   SPARSE:         NA
		 DESCR:       The value cannot exceed 100Gbps."
	DEFVAL          { -1 }
	::= { ihubEgressPerCosQosEntry 4 }

    
ihubEgressPerCosQosPirRate OBJECT-TYPE
        SYNTAX      TPIRRate
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The value of this object specifies peak information rate for the
             service, in units of kbps. The default is -1 which indicates that
             no rate limit is applied. 
             ALCATEL NOTE: 
                 ACCESS:      read-write             USAGE:          OPT
                 PERSIST:     NA                     INSRVMOD:       NA
                 RANGE:       -1|1..100000000        DEFVALUE:       -1
                 UNITS:       kbps                   SPARSE:         NA
                 DESCR:       The value cannot exceed 100Gbps."
        DEFVAL          { -1 }
        ::= { ihubEgressPerCosQosEntry 5 }
    
ihubEgressPerCosQosCBurstSize OBJECT-TYPE
        SYNTAX      IhubRateLimitBurstSize
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The value of this object specifies committed burst size in kBytes.
             A higher burst size will accommodate more bursty traffic without 
             causing packets to be dropped unless there is a sustained rate 
             greater than the configured rate limit. A lower burst size will 
             tend to drop some bursty traffic, even though the sustained rate 
             is less than the configured rate limit.
    
             ALCATEL NOTE: 
                 ACCESS:    read-write             USAGE:          OPT
                 PERSIST:   NA                     INSRVMOD:       NA
                 RANGE:     -1|1..262144           DEFVALUE:       -1
                 UNITS:     kB                     SPARSE:         NA
                 DESCR:     If no burst size is specified, a default value will be
                            used. The default value depends on the value of the 
                            rate limit."
        DEFVAL          { -1 }
        ::= { ihubEgressPerCosQosEntry 6 }
    
ihubEgressPerCosQosPBurstSize OBJECT-TYPE
        SYNTAX      IhubRateLimitBurstSize
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The value of this object specifies peak burst size in kBytes.
             A higher burst size will accommodate more bursty traffic without 
             causing packets to be dropped unless there is a sustained rate
             greater than the configured rate limit. A lower burst size will 
             tend to drop some bursty traffic, even though the sustained rate 
             is less than the configured rate limit.
    
            ALCATEL NOTE: 
                 ACCESS:    read-write             USAGE:          OPT
                 PERSIST:   NA                     INSRVMOD:       NA
                 RANGE:     -1|1..262144           DEFVALUE:       -1
                 UNITS:     kB                     SPARSE:         NA
                 DESCR:     If no burst size is specified, a default value will be
                            used. The default value depends on the value of the 
                            rate limit."
        DEFVAL          { -1 }
    ::= { ihubEgressPerCosQosEntry 7 }
    

ihubEgressPerCosQostrTcmMode   OBJECT-TYPE
        SYNTAX      INTEGER {
                        trTCM (1),
                        modifiedtrTCM (2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
           "When the value of ihubQostrTcmMode is set to  '1' the meter is compliant
            as per RFC 2698. When the value is set to '2' the meter is compliant 
            to RFC 4115."
    DEFVAL          { 1 }
    ::= { ihubEgressPerCosQosEntry 8 }    

ihubEgressPerCosQosAdminStatus OBJECT-TYPE
      SYNTAX      INTEGER {
                     enabled (0),
                     disabled (1)
                 }
      MAX-ACCESS read-write
      STATUS     current
      DESCRIPTION
          " Allows to enable/disable the rate-limiter applier for the set of 802.1ps in this entry."
     ::= { ihubEgressPerCosQosEntry 9 }

ihubEgressPerCosQosLastChanged OBJECT-TYPE
    SYNTAX       TimeStamp
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Timestamp of last change to this row in ihubEgressPerCosQosTable."
    ::= { ihubEgressPerCosQosEntry 10 }
    
