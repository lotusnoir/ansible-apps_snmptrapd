--  ==================================================================
--  |                     INGRESS/EGRESS STORM CONTROL                |
--  ==================================================================
--
--  Ingress (Downstream) storm control
--
ihubIngressSystemStormControlTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IhubIngressSystemStormControlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table specifies the storm control rate-limit
         parameters for downstream traffic per storm-type and aggregate."
    ::= { ihubIngressSystemStormControl 1 }
	
ihubIngressSystemStormControlEntry OBJECT-TYPE
    SYNTAX      IhubIngressSystemStormControlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Specifies downstream storm control rate-limit
         parameters at system-level"
    INDEX { ihubIngressSystemStormControlType }
    ::= { ihubIngressSystemStormControlTable 1 }
	
IhubIngressSystemStormControlEntry ::=
    SEQUENCE {
    	        ihubIngressSystemStormControlType INTEGER,
              ihubIngressSystemStormControlRate	Integer32,
              ihubIngressSystemStormControlBurstSize   IhubRateLimitBurstSize
             }
	
ihubIngressSystemStormControlType OBJECT-TYPE
    SYNTAX      INTEGER {
                    broadcast (1),
                    unknown-unicast (2),
                    unknown-multicast (3),
                    aggregate (4)
                  }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The ihubIngressSystemStormControlType variable specifies the
         each type or aggregate of all storm-types to be rate-limited
         in downstream direction. 
         Following  are the possible values:-
		 Broadcast:-
		 Broadcast frames with destination mac-address FF:FF:FF:FF:FF:FF
		 Unknown-Unicast:-
		 Destination Lookup-Failure Frames including unicast frames whose 
		 switching/routing information is unknown at the switch. A switch can take 
		 one of the following actions depending on the policy set at the switch:
			-Drop the frame (or Copy to CPU)
			-Broadcast the frame on all the ports in the VLAN
		 Unknown-Multicast:-
		 Multicast frames whose  switching/routing information is unknown at the switch. 
		 A switch can take one of the following actions depending on the policy set at the switch:
			-Drop the frame (or Copy to CPU)
			-Broadcast the frame on all the ports in the VLAN
		 Aggregate:-
		 All of the storm-types, i.e, Broadcast,Unknown-Unicast and Unknown-Multicast.
		 NOTE: Aggregate and per-storm-control rate-limit cannot be configured together."
    ::= { ihubIngressSystemStormControlEntry 1 }

ihubIngressSystemStormControlRate OBJECT-TYPE
    SYNTAX      Integer32 (-1 | 1..100000000) 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The ihubIngressSystemStormControlRate variable specifies the maximum ingress
         bandwidth (in kilobits per second)allowed for a particular storm-type or
         aggregate of all storm-types. A value of -1 means that the limit is the actual
         physical limit."
    DEFVAL { -1 }
    ::= { ihubIngressSystemStormControlEntry 2 }
	
ihubIngressSystemStormControlBurstSize OBJECT-TYPE
    SYNTAX      IhubRateLimitBurstSize
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "When an ingress storm-control rate-limit is configured, this parameter
         specifies the corresponding burst size in kBytes. Generally, a higher
         burst size will accommodate more bursty traffic without causing 
         packets to be dropped unless there is a sustained rate greater than 
         the configured rate limit. A lower burst size will tend to drop some
         bursty traffic, even though the sustained rate is less than the 
         configured rate limit.

        ALCATEL NOTE: 
             ACCESS:    read-write             USAGE:          OPT
             PERSIST:   NA                     INSRVMOD:       NA
             RANGE:     -1|1..262144           DEFVALUE:       -1
             UNITS:     kB                     SPARSE:         NA
             DESCR:     If no burst size is specified, a default value will be
                        used. The default value depends on the value of the 
                        rate limit." 
    DEFVAL { -1 }
    ::= { ihubIngressSystemStormControlEntry 3 }
	
