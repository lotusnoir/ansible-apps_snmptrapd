fadDot1agCfmMepLossRsltTable   OBJECT-TYPE
    SYNTAX     SEQUENCE OF FadDot1agCfmMepLossRsltEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
       "The fadDot1agCfmMepLossRsltTable indicates the results of
        all the Single-Ended Loss Tests, from one specific MEP to a
        specific destination MAC address.
       
        This table is not persistent over reboots of the chassis."
        
    ::= { fadCfmExt 16 }

fadDot1agCfmMepLossRsltEntry   OBJECT-TYPE
    SYNTAX     FadDot1agCfmMepLossRsltEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
       "An entry in fadDot1agCfmMepLossRsltTable represents the latest result of a 
       Y1731 Loss Measurement, if the test was started. Each row stores the result 
       for a Single-Ended Loss test indicated by the originating MEP of
       dot1agCfmMdIndex, dot1agCfmMaIndex, dot1agCfmMepIdentifier, 
       toward destination MAC address fadDot1agCfmMepLmMacAddr."
    INDEX { 
              dot1agCfmMdIndex,
              dot1agCfmMaIndex,
              dot1agCfmMepIdentifier,
              fadDot1agCfmMepLmMacAddr
          }
    ::= { fadDot1agCfmMepLossRsltTable 1 }      

FadDot1agCfmMepLossRsltEntry   ::= SEQUENCE {
    fadDot1agCfmMepLmMacAddr           MacAddress,
    fadDot1agCfmMepLmPduSent           Unsigned32,
    fadDot1agCfmMepLmPduReceived       Unsigned32,
    fadDot1agCfmMepLmLatestChangeTime  TimeStamp,
    fadDot1agCfmMepLmFramesRxLocal     Counter64,
    fadDot1agCfmMepLmFramesRxPeer      Counter64,  
    fadDot1agCfmMepLmFramesTxLocal     Counter64,
    fadDot1agCfmMepLmFramesTxPeer      Counter64,
    fadDot1agCfmMepLmFramesLossLocal   Counter64,
    fadDot1agCfmMepLmFramesLossPeer    Counter64, 
    fadDot1agCfmMepLmLossRatioLocal    TLossRatioHundrethsOfPercent, 
    fadDot1agCfmMepLmLossRatioPeer     TLossRatioHundrethsOfPercent,
    fadDot1agCfmMepLmPrevTxLocal       Counter32,
    fadDot1agCfmMepLmPrevRxLocal       Counter32,
    fadDot1agCfmMepLmPrevTxPeer        Counter32,
    fadDot1agCfmMepLmPrevRxPeer        Counter32,
    fadDot1agCfmMepLmCurrTxLocal       Counter32,
    fadDot1agCfmMepLmCurrRxLocal       Counter32,
    fadDot1agCfmMepLmCurrTxPeer        Counter32,
    fadDot1agCfmMepLmCurrRxPeer        Counter32,
    fadDot1agCfmMepLmEnableDuration    Unsigned32,
    fadDot1agCfmMepLmChangeCount       Unsigned32

}

fadDot1agCfmMepLmMacAddr  OBJECT-TYPE
    SYNTAX     MacAddress
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepLmMacAddr indicates the destination MAC address 
        of the the test."
    ::= { fadDot1agCfmMepLossRsltEntry 1 }
	
fadDot1agCfmMepLmPduSent  OBJECT-TYPE
    SYNTAX     Unsigned32
    UNITS      "seconds"
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
    	 "The object indicates the number of PDUs sent"
    ::= { fadDot1agCfmMepLossRsltEntry 2 }
    
fadDot1agCfmMepLmPduReceived OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    	 "The object indicates the number of PDUs received"
    ::= { fadDot1agCfmMepLossRsltEntry 3 }
    
fadDot1agCfmMepLmLatestChangeTime OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "fadDot1agCfmMepLmLatestChangeTime contains the sysUpTime at the most 
         Recently writable variable in the fadDot1agCfmMepLossRsltEntry row was 
         modified for this MEP."
    ::= { fadDot1agCfmMepLossRsltEntry 4 }

fadDot1agCfmMepLmFramesRxLocal   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmFramesRxLocal is the accumulated number of 
        frames received at the local service endpoint of this MEP, and within the 
        loss test duration"
    ::= { fadDot1agCfmMepLossRsltEntry 5 }
          
fadDot1agCfmMepLmFramesRxPeer   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmFramesRxPeer is the accumulated number of 
        frames received at the far-end service endpoint associated with 
        fadDot1agCfmMepLmMacAddr, and within the loss test duration."
    ::= { fadDot1agCfmMepLossRsltEntry 6 }

fadDot1agCfmMepLmFramesTxLocal   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmFramesTxLocal is the accumulated number of 
        frames transmitted at the local service endpoint of this MEP, and within 
        the loss test duration."
    ::= { fadDot1agCfmMepLossRsltEntry 7 }
          
fadDot1agCfmMepLmFramesTxPeer   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmFramesTxPeer is the accumulated number of 
        frames transmitted at the far-end service endpoint associated with 
        fadDot1agCfmMepLmMacAddr, and within the loss test duration."
    ::= { fadDot1agCfmMepLossRsltEntry 8 }
    
fadDot1agCfmMepLmFramesLossLocal   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmFramesLossLocal is the accumulated number of
        loss frames at the local service endpoint of this MEP, and within the loss 
        test duration."
    ::= { fadDot1agCfmMepLossRsltEntry 9 }   

fadDot1agCfmMepLmFramesLossPeer   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmFramesLossPeer is the accumulated number of 
        loss frames at the far-end service endpoint associated with       
        fadDot1agCfmMepLmMacAddr, and within the loss test duration."
    ::= { fadDot1agCfmMepLossRsltEntry 10 }

fadDot1agCfmMepLmLossRatioLocal   OBJECT-TYPE
    SYNTAX     TLossRatioHundrethsOfPercent
    UNITS      "hundredth of a percent"
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmLossRatioLocal is the accumulated Loss Ratio, 
        at the local service endpoint of this MEP, and within the loss test 
        duration.

        fadDot1agCfmMepLmLossRatioLocal is expressed in hundredth of a percent 
        accuracy.  
        It is calculated as follows:  
        (fadDot1agCfmMepLmFramesLossLocal * 10000) / fadDot1agCfmMepLmFramesTxPeer."

    ::= { fadDot1agCfmMepLossRsltEntry 11 }
          
fadDot1agCfmMepLmLossRatioPeer   OBJECT-TYPE
    SYNTAX     TLossRatioHundrethsOfPercent
    UNITS      "hundredth of a percent"
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmLossRatioPeer is the accumulated Loss Ratio, 
        at the far-end service endpoint associated with fadDot1agCfmMepLmMacAddr, 
        and within the loss test duration.
        
        fadDot1agCfmMepLmLossRatioPeer is expressed in hundredth of a percent 
        accuracy.  
        It is calculated as follows:
        (fadDot1agCfmMepLmFramesLossPeer * 10000) / fadDot1agCfmMepLmFramesTxLocal."
    ::= { fadDot1agCfmMepLossRsltEntry 12 }

fadDot1agCfmMepLmPrevTxLocal   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmPrevTxLocal is the value of TxFCf 
        in the second latest ETH-LMR frame this local MEP has received."
    ::= { fadDot1agCfmMepLossRsltEntry 13 }
    
fadDot1agCfmMepLmPrevRxLocal   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmPrevRxLocal is the counter of frames 
        received at the service end point of this local MEP at the time the second latest ETH-LMR frame is
        received and being processed."
    ::= { fadDot1agCfmMepLossRsltEntry 14 }

fadDot1agCfmMepLmPrevTxPeer   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmPrevTxPeer is the value of TxFCb 
        in the second latest ETH-LMR frame this local MEP has received."
    ::= { fadDot1agCfmMepLossRsltEntry 15 }
    
fadDot1agCfmMepLmPrevRxPeer   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmPrevRxPeer is the value of 'RxFCf' 
        in the second latest ETH-LMR frame this local MEP has received."
    ::= { fadDot1agCfmMepLossRsltEntry 16 }

fadDot1agCfmMepLmCurrTxLocal   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmCurrTxLocal is the value of 'TxFCf' 
        in the latest ETH-LMR frame this local MEP has received."
    ::= { fadDot1agCfmMepLossRsltEntry 17 }
    
fadDot1agCfmMepLmCurrRxLocal   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmCurrRxLocal is the counter of frames received at the 
        service end point of this local MEP at the time the latest ETH-LMR frame is
        received and being processed."
    ::= { fadDot1agCfmMepLossRsltEntry 18 }

fadDot1agCfmMepLmCurrTxPeer   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmCurrTxPeer is the value of 'TxFCb' in the lastest ETH-LMR 
        frame this local MEP has received."
    ::= { fadDot1agCfmMepLossRsltEntry 19 }
    
fadDot1agCfmMepLmCurrRxPeer   OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "fadDot1agCfmMepLmCurrRxPeer is the value of 'RxFCf' in the latest ETH-LMR 
        frame this local MEP has received."
    ::= { fadDot1agCfmMepLossRsltEntry 20 }

fadDot1agCfmMepLmEnableDuration  OBJECT-TYPE
    SYNTAX     Unsigned32
    UNITS      "seconds"
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepLmEnableDuration is the accumulation of time, 
        expressed in seconds, during which the loss-test is enabled.  
         
        For Single-Ended loss-tests, the value of fadDot1agCfmMepLmEnableDuration is counted 
        from when it's initiated until when it's ended.  It is not accumulative from one 
        single-ended test to the next."  
    ::= { fadDot1agCfmMepLossRsltEntry 21 }

fadDot1agCfmMepLmChangeCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Value of fadDot1agCfmMepLmChangeCount is the number of frames, either the received
        ETH-LMR , that were used for the calculation of loss measurement results 
        of this fadDot1agCfmMepLossRsltEntry row for this MEP."
    ::= { fadDot1agCfmMepLossRsltEntry 22 }

