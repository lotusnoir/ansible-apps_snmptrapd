fadvRtrIgmpProxyIfRxDrops OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "This object contains information about the number of received IGMP
         messages that were dropped in this proxy interface"
    ::= { fadvRtrIgmpProxyIfStatsEntry 11 }

fadvRtrIgmpProxyGrpSrcTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FadvRtrIgmpProxyGrpSrcEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The fadvRtrIgmpProxyGrpSrcTable is a igmp global table, This table contains all groups forwarded to upstream proxy interfaces"
    ::= { fadIgmpProxyMgmtExt 4 }

fadvRtrIgmpProxyGrpSrcEntry OBJECT-TYPE
    SYNTAX      FadvRtrIgmpProxyGrpSrcEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each row represents information related to IGMP proxy instance."
    INDEX {
                        vRtrID, 
			vRtrIfIndex, 
			fadvRtrIgmpProxyGSGrpAddress, 
			fadvRtrIgmpProxyGSSrcAddress
	  }

    ::= { fadvRtrIgmpProxyGrpSrcTable 1 }

FadvRtrIgmpProxyGrpSrcEntry	 ::=
    SEQUENCE {
	    fadvRtrIgmpProxyGSGrpAddress               IpAddress,
	    fadvRtrIgmpProxyGSSrcAddress               IpAddress,
	    fadvRtrIgmpProxyGSType                     INTEGER
    }


fadvRtrIgmpProxyGSGrpAddress OBJECT-TYPE
   SYNTAX     IpAddress
   MAX-ACCESS not-accessible
   STATUS     current
   DESCRIPTION
           " Specify the IGMP group IP address."
   ::={ fadvRtrIgmpProxyGrpSrcEntry 1 }


fadvRtrIgmpProxyGSSrcAddress OBJECT-TYPE
   SYNTAX     IpAddress
   MAX-ACCESS not-accessible
   STATUS     current
   DESCRIPTION
        "Specify the IGMP group source IP address."
   ::={ fadvRtrIgmpProxyGrpSrcEntry 2}

fadvRtrIgmpProxyGSType OBJECT-TYPE
    SYNTAX  INTEGER {
                       starG(1),
                       sG(2)
                    }

    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "It specify the type of IGMP Group Type i.e (S,G) in case of version 3 and (*,G) in case of version 2."
    ::= { fadvRtrIgmpProxyGrpSrcEntry 3 }

	
svcIgmpSnpgStaticGrpSrcTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SvcIgmpSnpgStaticGrpSrcEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table svcIgmpSnpgStaticGrpSrcTable contains statically configured (S,G)/(*,G) entries 
	for igmp snooping supported services."    
	
    ::= { fadIgmpSnpgMgmtExt 1 }

svcIgmpSnpgStaticGrpSrcEntry OBJECT-TYPE
    SYNTAX      SvcIgmpSnpgStaticGrpSrcEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the svcIgmpSnpgStaticGrpSrcTable. Entries can be created or 
		 deleted by an SNMP SET request to svcIgmpSnpgStaticRowstatus."       

    INDEX { svcId, svcIgmpSnpgStaticGroupAddr, svcIgmpSnpgStaticSourceAddr }

    ::= { svcIgmpSnpgStaticGrpSrcTable 1 }

SvcIgmpSnpgStaticGrpSrcEntry ::=
    SEQUENCE {
	    svcIgmpSnpgStaticGroupAddr                IpAddress,
	    svcIgmpSnpgStaticSourceAddr               IpAddress,
	    svcIgmpSnpgStaticRowstatus                RowStatus
    }
svcIgmpSnpgStaticGroupAddr OBJECT-TYPE
    SYNTAX     IpAddress
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "The value of the object svcIgmpSnpgStaticGroupAddr specifies the group multicast 
		 address for this statically configured (S,G) entry for this VLAN VPLS service."
			
    ::= { svcIgmpSnpgStaticGrpSrcEntry 1 }
	
svcIgmpSnpgStaticSourceAddr OBJECT-TYPE
    SYNTAX     IpAddress
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "The value of the object svcIgmpSnpgStaticSourceAddr specifies the Source IP address 
		 for this statically configured(S,G) entry for this VLAN VPLS service.
		 For a (*,G) entry this object must be set to 0.0.0.0."
			
    ::= { svcIgmpSnpgStaticGrpSrcEntry 2 }
	
svcIgmpSnpgStaticRowstatus    OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of the object svcIgmpSnpgStaticRowstatus specifies the row status used for 
		 creation or deletion of svcIgmpSnpgStaticGrpSrcTable entries. The supported values are 
		 createAndGo(4) and destroy(6)."
		 
    ::= { svcIgmpSnpgStaticGrpSrcEntry 3 }

