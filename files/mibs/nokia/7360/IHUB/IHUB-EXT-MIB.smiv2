      fadY1731CfmPMCurrent15MinTestType              EthCfmY1731PMType,
      fadY1731CfmPMCurrent15MinTimeElapsed		Integer32,
      fadY1731CfmPMCurrent15MinTimeMeasured    	Integer32,
      fadY1731CfmPM15MinValidIntervals			Integer32,
      fadY1731CfmPMCurrent15MinInvalidDataFlag		TruthValue,
      fadY1731CfmPMCurrent15MinPDUSent			Counter32,
      fadY1731CfmPMCurrent15MinPDURecv			Counter32,
      fadY1731CfmPMCurrent15MinRxLocal 			Counter64, 
      fadY1731CfmPMCurrent15MinRxPeer 			Counter64,
      fadY1731CfmPMCurrent15MinTxLocal 			Counter64,
      fadY1731CfmPMCurrent15MinTxPeer 			Counter64,
      fadY1731CfmPMCurrent15MinLossLocal 		Counter64,
      fadY1731CfmPMCurrent15MinLossPeer 		Counter64,
      fadY1731CfmPMCurrent15MinLossRatioMinLocal     TLossRatioHundrethsOfPercent,
      fadY1731CfmPMCurrent15MinLossRatioMaxLocal     TLossRatioHundrethsOfPercent,
      fadY1731CfmPMCurrent15MinLossRatioAvgLocal     TLossRatioHundrethsOfPercent,
      fadY1731CfmPMCurrent15MinLossRatioMinPeer      TLossRatioHundrethsOfPercent,
      fadY1731CfmPMCurrent15MinLossRatioMaxPeer      TLossRatioHundrethsOfPercent,
      fadY1731CfmPMCurrent15MinLossRatioAvgPeer      TLossRatioHundrethsOfPercent,
	  fadY1731CfmPMCurrent15MinTestId			Unsigned32,
      fadY1731CfmPMCurrent15MinRemoteMepId		Dot1agCfmMepId,
      fadY1731CfmPMCurrent15MinUnack			Counter32,
	  fadY1731CfmPMCurrent15MinPDUDiscarded			Counter32,
     fadY1731CfmPMCurrent15MinDataSize         Unsigned32

}

fadY1731CfmPMCurrent15MinTestType    OBJECT-TYPE
    SYNTAX      EthCfmY1731PMType 
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The value of fadY1731CfmPMCurrent15MinTestType indicates the type
         of test this row details."
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 1 }

fadY1731CfmPMCurrent15MinTimeElapsed OBJECT-TYPE
    SYNTAX  	Integer32 (0..900)
    MAX-ACCESS  read-only
    STATUS  	current
    DESCRIPTION
        "The amount of time, measured in seconds, that has elapsed since the 
         beginning of the current 15 minute interval."
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 2 }

fadY1731CfmPMCurrent15MinTimeMeasured OBJECT-TYPE
    SYNTAX  	Integer32 (0..900)
    MAX-ACCESS  read-only
    STATUS  	current
    DESCRIPTION
        "The amount of time, measured in seconds, that statistics for this entry
         have been actually counted for the current 15 minute interval. This value
         will be the same as the elapsed duration except in a situation where 
         performance monitoring data could not be collected  for this test for 
         some reason."
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 3 }

fadY1731CfmPM15MinValidIntervals OBJECT-TYPE
    SYNTAX  	Integer32 (0..96)
    MAX-ACCESS  read-only
    STATUS  	current
    DESCRIPTION
        "The number of previous 15 min intervals for which valid data was collected 
         for this test. 
         This is equal to the number of entries in the 
         fadY1731CfmPMPrevious15MinIntervalLossTable for this test."
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 4 }

fadY1731CfmPMCurrent15MinInvalidDataFlag OBJECT-TYPE
    SYNTAX      TruthValue 
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
	  "The object indicates the validity of the corresponding PM data. It is
          set to false when the PM data is complete and valid and set to true when
          the PM data is incomplete or invalid."
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 5}

fadY1731CfmPMCurrent15MinPDUSent OBJECT-TYPE
    SYNTAX  	Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
	  "The object indicates the number of PDUs sent"
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 6}

fadY1731CfmPMCurrent15MinPDURecv OBJECT-TYPE
    SYNTAX  	Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
	  "The object indicates the number of PDUs received"
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 7}

fadY1731CfmPMCurrent15MinRxLocal   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadY1731CfmPMCurrent15MinRxLocal is the accumulated number of
         frames received at the local service endpoint of this MEP(RxFCl), and 
         within the loss test duration of 15 Minutes"
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 8 }
          
fadY1731CfmPMCurrent15MinRxPeer   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadY1731CfmPMCurrent15MinRxPeer is the accumulated number of 
         frames received at the far-end service endpoint(RxFCf), and within the 
         loss test duration of 15 Minutes.

         For SLM, this is the value of TxFCf."
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 9 }

fadY1731CfmPMCurrent15MinTxLocal   OBJECT-TYPE
    SYNTAX     Counter64
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The Value of fadY1731CfmPMCurrent15MinTxLocal is the accumulated number of 
         frames transmitted at the local service endpoint of this MEP(TxFCf), and 
         within the loss test duration of 15 Minutes"
    ::= { fadY1731CfmPMCurrent15MinIntervalLossEntry 10 }
          
fadY1731CfmPMCurrent15MinTxPeer   OBJECT-TYPE
