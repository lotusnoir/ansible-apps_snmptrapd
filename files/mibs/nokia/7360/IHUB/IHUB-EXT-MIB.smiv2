    fadDot1agCfmMepSingleLMPriority            Unsigned32,
    fadDot1agCfmMepSingleLMCount               Unsigned32,
    fadDot1agCfmMepSingleLMInterval            Unsigned32
}

fadDot1agCfmMpSvcId OBJECT-TYPE
    SYNTAX          TmnxServId
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "The value of the object svcId specifies the  
                     Service identifier to which the Maintenance point belongs. 
                     An MP which has not yet been made part of a SAP or SDP 
                     will have the value as zero"
    DEFVAL { 0 }
    ::= {fadDot1agCfmMepEntry 1 }

fadDot1agCfmMepLbmTestTlvIncluded OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION   "Indicates whether a Test TLV or Data TLV is included as  the optional TLV
                   in the loopback message.
                   A value of 'true' indicates that the Test TLV is to be included.
                   A value of 'false' indicates that the Data TLV is to be included."                 
    DEFVAL { false }
::= { fadDot1agCfmMepEntry 2 }

fadDot1agCfmMepLbmTestPattern   OBJECT-TYPE
    SYNTAX     INTEGER {
                   allZerosNoCrc  (0),
                   allZerosCrc    (1),
                   prbsNoCrc      (2),
                   prbsCrc        (3),
                   allOnesNoCrc   (4),
                   allOnesCrc     (5)
               }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepLbmTestPattern specifies the content
         of the test TLV of the LBM frames.none (0) specifies no test pattern 
        'allZerosNoCrc (0)' and 'allZerosCrc (1)'
         specifies the test TLV content as all zero values (without and with CRC
         calculations respectively). 'prbsNoCrc (3) and 'prbsCrc (4) specifies
         the test TLV content as pseudo random bit sequence (without and with CRC32
         calculations respectively).'allOnesNoCrc (4)' and 'allOnesCrc (5)',
         specifies the data content as all ones (without and with CRC
         calculations respectively).
         The length of the pattern is the value of fadDot1agCfmMepTransmitLbmTestTlvLen.

         ALCATEL NOTE : DESCR : prbsCrc will be only be supported "
    DEFVAL { prbsCrc }
    ::= { fadDot1agCfmMepEntry 3 }

fadDot1agCfmMepTransmitLbmTestTlvLen
OBJECT-TYPE
    SYNTAX      Unsigned32 (64..1500)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "The length of the test pattern to be included in the test TLV
       of the Loopback message"
    DEFVAL { 64 }
    ::= { fadDot1agCfmMepEntry 4 }

fadDot1agCfmMepLbrInBitErrors
OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The total number of loopback replies with bit errors
        received.
       "
    ::= { fadDot1agCfmMepEntry 5 }

fadDot1agCfmMepLbrInCrcErrors
OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The total number of loopback replies with CRC errors
        received.
       "
    ::= { fadDot1agCfmMepEntry 6}

fadDot1agCfmMepY1731PmEnabled
OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If set to true, the MEP will generate and respond to Y1731 PM PDUs.
         This value cannot be set to false if there are entries for this MEP in 
         fadY1731CfmPMConfigTable."
    DEFVAL { false }
    ::= { fadDot1agCfmMepEntry 7 }

fadDot1agCfmMepY1731SingleLMPriorityBmp
OBJECT-TYPE
     SYNTAX      Integer32 (0..256)				
     MAX-ACCESS  read-write
     STATUS      current
     DESCRIPTION
        "The value of fadDot1agCfmMepY1731SingleLMPriorityBmp specifies bitmap of 
         CoS values for which local Tx and Rx aggregated counters are maintained. 
         These counters are populated in LMM and LMR PDUs for loss calculation.
	  
         If value 'all' is specified, per vlan (all CoS) aggregated Tx and Rx 
         counters will be maintained.
        
         If value 'none' is specified, no counters will be maintained.

         It is specified as a bitmap of 8 bits where each bit corresponds to a 
         dot1p value.
	     For example: 
            Dot1p 0   : 0x1
            Dot1p 1   : 0x2
            Dot1p 2   : 0x4
            ..
            Dot1p 7   : 0x80		
            Dot1p all : 0x100
            Dot1p none: 0x0 

      	  To Specify Dot1p 0, 2,4 --> 0x15" 	

     DEFVAL  { 0 }
    ::= { fadDot1agCfmMepEntry 8 }

fadDot1agCfmMepSingleLMMacAddress   OBJECT-TYPE
    SYNTAX     MacAddress
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepSingleLMMacAddress   specifies the peer MAC 
        address used as a target for the Single-Ended Loss Measurement.
        Setting this object will trigger a Single-ended Loss Measurement test to 
        the specified MAC address.
         
        Upon completion of the test, the MacAddress will revert to its default 
        value, indicating that another test is possible."
         
    DEFVAL { '000000000000'H }
    ::= { fadDot1agCfmMepEntry 9 }

fadDot1agCfmMepSingleLMPriority     OBJECT-TYPE
    SYNTAX     Unsigned32 (0..7)
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepSingleLMPriority specifies the priority used 
        in the generated test frame for the Single-Ended Loss Measurement test."
    DEFVAL { 7 }
    ::= { fadDot1agCfmMepEntry 10 }

fadDot1agCfmMepSingleLMCount    OBJECT-TYPE
    SYNTAX     Unsigned32 (2..5)
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepSingleLMCount specifies the number of frames 
        with ETH-LM request information to be sent to the peer MAC address for this 
        Single-Ended Loss Measurement test"
    DEFVAL { 2 }
    ::= { fadDot1agCfmMepEntry 11 }
   
fadDot1agCfmMepSingleLMInterval    OBJECT-TYPE
    SYNTAX     Unsigned32 (1000)
    UNITS      "milliseconds"
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The value of fadDot1agCfmMepSingleLMInterval specifies at what intervals
         to transmit ETH-LM frames for the Single-Ended Loss Measurement test."
    DEFVAL { 1000 }
    ::= { fadDot1agCfmMepEntry 12 }

