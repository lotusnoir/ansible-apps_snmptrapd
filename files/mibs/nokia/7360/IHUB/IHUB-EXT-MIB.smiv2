-- 
-- Y1731 PM Configuration Table
-- 
fadY1731CfmPMConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FadY1731CfmPMConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Defines the Y1731 CFM PM Configuration Table for providing performance
         monitoring test operations using ETH-LM, ETH-DM and ETH-SLM. 
         The results of these tests are stored in the Y1731 CFM PM test 
         result tables." 
    ::= { fadCfmExt 7 }

fadY1731CfmPMConfigEntry OBJECT-TYPE
    SYNTAX      FadY1731CfmPMConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines an entry in the FadY1731CfmPMConfigTable." 
    INDEX {
            dot1agCfmMdIndex,
            dot1agCfmMaIndex,
            dot1agCfmMepIdentifier,
            fadY1731CfmPMConfigSessionId
          }
    ::= { fadY1731CfmPMConfigTable 1 }

FadY1731CfmPMConfigEntry ::=
    SEQUENCE {
        fadY1731CfmPMConfigSessionId             Integer32,
        fadY1731CfmPMConfigRowStatus             RowStatus,
        fadY1731CfmPMConfigMepMacAddress         MacAddress,
        fadY1731CfmPMConfigType                  EthCfmY1731PMType,
        fadY1731CfmPMConfigAdminStatus           INTEGER,
        fadY1731CfmPMConfigPriority              Unsigned32,
        fadY1731CfmPMConfigInterval              Unsigned32,
        fadY1731CfmPMConfigMeasurementInterval   Unsigned32,
        fadY1731CfmPMConfigDataSize              Unsigned32
    }

fadY1731CfmPMConfigSessionId      OBJECT-TYPE
    SYNTAX      Integer32 (1..960)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigSessionId indicates the session identifier
         used to initiate the Y1731 PM test."
    ::= { fadY1731CfmPMConfigEntry 1 }
	
fadY1731CfmPMConfigRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object allows entries to be created and deleted
         in the fadY1731CfmPMConfigTable."
    REFERENCE
        "See definition of RowStatus in RFC 2579, 'Textual
        Conventions for SMIv2.'"
    ::= { fadY1731CfmPMConfigEntry 2 }

fadY1731CfmPMConfigMepMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigMepMacAddress specifies the target unicast 
         MAC address for the test."
    DEFVAL { '000000000000'H }
    ::= { fadY1731CfmPMConfigEntry 3 }
	
fadY1731CfmPMConfigType           OBJECT-TYPE
    SYNTAX      EthCfmY1731PMType 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigType indicates the type
         of test this row details."
	DEFVAL {0}
    ::= { fadY1731CfmPMConfigEntry 4 }

fadY1731CfmPMConfigAdminStatus OBJECT-TYPE
    
    SYNTAX      INTEGER {
                          enabled(1), -- test should be started
                          disabled(2) -- test should be stopped
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigAdminStatus indicates the desired
         administrative state of the Y1731 CFM PM test.

         When this object has the value 'enabled' an attempt to modify
         the test parameters (fadY1731CfmPMConfigPriority, 
         fadY1731CfmPMConfigInterval, fadY1731CfmPMConfigMeasurementInterval will 
         fail with an inconsistentValue error.
        
         When the test is on-going and if this objects value is changed to 
         'disbled', history and current results of this test will be cleared. "
    DEFVAL { disabled }
    ::= { fadY1731CfmPMConfigEntry 5 }

fadY1731CfmPMConfigPriority     OBJECT-TYPE
    SYNTAX     Unsigned32 (0..7)
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigPriority specifies the priority used in
         the generated test frame for the Y1731 PM (LM/DM/SLM) test.

         This parameter can be set only when fadY1731CfmPMConfigAdminStatus is 
         set to disabled."
    DEFVAL { 7 }
    ::= { fadY1731CfmPMConfigEntry 6 }

fadY1731CfmPMConfigInterval     OBJECT-TYPE
    SYNTAX      Unsigned32 (1..10 | 60|300)
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigInterval specifies the time to wait
         between the transmission of one OAM request and the next.
         This parameter can be set only when fadY1731CfmPMConfigAdminStatus is 
         set to disabled.
	  
         The applicability, range, and default value of this object vary with
         fadY1731CfmPMConfigType, as shown in the following table.
		 
         fadY1731CfmPMConfigType	Min	Max	Default
           
	                ethCfmTWSlm	1  	10  	1
			 ethCfmTWDelay	1  	10  	1
		 ethCfmSinglEndedLoss	60  	300  	60"

    DEFVAL { 1 }
    ::= { fadY1731CfmPMConfigEntry 7 }	

fadY1731CfmPMConfigMeasurementInterval          OBJECT-TYPE
    SYNTAX      Unsigned32 (60 | 300)
    UNITS      "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigMeasurementInterval specifies the time
         after which the synthetic loss calculation is made periodically. 

         If the value of this object is set to 'X' seconds, loss measurement
         calculations are done after every 'X' seconds.

         This includes the default SLM timeout of 5 seconds in which the agent will 
         not transmit any further SLM PDUs and will wait for the response from 
         remote node. 
         Upon the expiration of the timeout, the agent assumes that the message 
         response will not be received. Any response received after the timeout 
         period has expired is silently discarded.

  This parameter is optional and is valid only if fadY1731CfmPMConfigType   
  'ethCfmTWSlm'. 
  
  This can be set only when fadY1731CfmPMConfigAdminStatus is set to 
  disabled."
    DEFVAL { 300 }
    ::= { fadY1731CfmPMConfigEntry 8 }

fadY1731CfmPMConfigDataSize OBJECT-TYPE
    SYNTAX      Unsigned32 (0..1500)
    UNITS       "octets"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of fadY1731CfmPMConfigDataSize specifies the number of
         octets in the Value field of the Data TLV of the Ethernet CFM packet.
         If zero is specified, the packet has no Data TLV.

         This parameter is optional and is valid only if fadY1731CfmPMConfigType
         'ethCfmTWSlm'."
    DEFVAL { 0 }
    ::= { fadY1731CfmPMConfigEntry 9 }

-- 
-- Y1731 PM Current 15-Minutes Loss Statistics Table  
-- 
fadY1731CfmPMCurrent15MinIntervalLossTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF FadY1731CfmPMCurrent15MinIntervalLossEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "The fadY1731CfmPMCurrent15MinIntervalLossTable contains the current 15  
         minutes interval loss statistics per Y1731 PM loss test."
    ::= { fadCfmExt 8 }

fadY1731CfmPMCurrent15MinIntervalLossEntry OBJECT-TYPE
    SYNTAX     FadY1731CfmPMCurrent15MinIntervalLossEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "Table entry for the current 15 minute interval loss statistics per 
         Y1731 PM loss test."
    INDEX {
	     dot1agCfmMdIndex,
            dot1agCfmMaIndex,
            dot1agCfmMepIdentifier,
            fadY1731CfmPMConfigSessionId
          }
    ::= { fadY1731CfmPMCurrent15MinIntervalLossTable 1 }

