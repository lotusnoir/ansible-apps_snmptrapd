--
--  OSPF LSB Overflow Notification
--
fadOspfLsdbOverflowClear NOTIFICATION-TYPE 
    OBJECTS { 
        tmnxOspfRouterId,        -- The originator of the notification.
        tmnxOspfExtLsdbLimit
    } 
    STATUS       current 
    DESCRIPTION 
        "A fadOspfLsdbOverflowClear notification is generated when the 
         tmnxOspfExitOverflowInterval timer has expired.
         This timer has been started when the OSPF instance entered the 
         Overflow state.
         NB: this tmnxOspfExitOverflowInterval is configurable in the 
             TIMETRA-OSPF-NG-MIB.
         NB: this notification is the counterpart of tmnxOspfLsdbOverflow." 
         ::= { fadOspfExtNotifications 1 } 

fadOspfLsdbApproachingOverflowClear NOTIFICATION-TYPE 
    OBJECTS { 
        tmnxOspfRouterId,        -- The originator of the notification.
        tmnxOspfExtLsdbLimit
    } 
    STATUS       current 
    DESCRIPTION 
        "A fadOspfLsdbApproachingOverflowClear notification is generated 
         when the number of LSAs in the router's link-state database 
         has decreased below 80% of the value indicated by tmnxOspfExtLsdbLimit.
         NB: this notification is the counterpart of 
             tmnxOspfLsdbApproachingOverflow.
         NB: there is a hysterisis of 10% between tmnxOspfLsdbApproachingOverflow
             and fadOspfLsdbApproachingOverflowClear." 
         ::= { fadOspfExtNotifications 2 } 

fadOspfOperState  OBJECT-TYPE
    SYNTAX  INTEGER {
                inService (1),
                outOfService (2)}
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     
        "This object indicates the current operating state of this OSPF
         protocol instance."
    ::= { fadOspfExtObjects 1 }

fadOspfInstanceStateChange NOTIFICATION-TYPE 
