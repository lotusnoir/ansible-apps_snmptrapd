IHUB-EXT-MIB DEFINITIONS ::= BEGIN

IMPORTS
        IpAddress, MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, enterprises, Integer32,
        Gauge32, Counter32, Unsigned32, Counter64
               FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, RowStatus, TruthValue, TimeStamp, MacAddress
               FROM SNMPv2-TC
        InetAddress,InetAddressType,InetAddressPrefixLength, InetPortNumber
               FROM INET-ADDRESS-MIB
        tmnxPortEtherEntry, tmnxPortNotifyPortId, tmnxPortPortID
               FROM TIMETRA-PORT-MIB
        svcBaseInfoEntry, svcTlsInfoEntry, iesIfEntry, svcId, svcVplsVlanId,
        PWTemplateId       
               FROM TIMETRA-SERV-MIB
        TmnxEnabledDisabled, TmnxPortID, TmnxServId,
        TCIRRate, TPIRRate, TmnxEncapVal,  SdpBindId, TmnxManagedRouteStatus, TTcpUdpPort, TTcpUdpPortOperator,
        TmnxLinkMapProfileIdOrZero
               FROM TIMETRA-TC-MIB
        tMirrorSourceIndex
               FROM TIMETRA-MIRROR-MIB
        tmnxOspfRouterId, tmnxOspfExtLsdbLimit,
        tmnxOspfAdminState
               FROM TIMETRA-OSPF-NG-MIB
        tBgpPeerNgMaxPrefix
             FROM TIMETRA-BGP-MIB
        isisSysAdminState
               FROM ISIS-MIB
        vRtrIsisOperState  
               FROM TIMETRA-ISIS-MIB  
        tCpmIpFilterEntry, tCpmIPv6FilterEntry 
             FROM TIMETRA-SECURITY-MIB
        vRtrID, vRtrIfIndex,vRtrConfEntry
             FROM TIMETRA-VRTR-MIB
        TFilterID ,tIPv6FilterParamsEntry, tIPFilterParamsEntry
             FROM TIMETRA-FILTER-MIB
        tmnxNtpServersEntry
             FROM TIMETRA-NTP-MIB
        dot3adAggPortEntry
             FROM IEEE8023-LAG-MIB
        tLagConfigEntry,tLagIndex
             FROM TIMETRA-LAG-MIB
        sapPortId, sapEncapValue, sapBaseInfoEntry, sapBaseStatsEntry, tmnxGreTunnelEntry
             FROM TIMETRA-SAP-MIB
        tmnxDot1agCfmStackEntry,tmnxDot1agCfmSdpBindStackEntry,tmnxDot1agCfmSapMipEntry, tmnxDot1agCfmSdpMipEntry
             FROM TIMETRA-IEEE8021-CFM-MIB  
        dot1agCfmMdIndex, dot1agCfmMaIndex,
        dot1agCfmMepIdentifier,Dot1agCfmMepId,
        dot1agCfmMepEntry
             FROM IEEE8021-CFM-MIB
        TNetworkPolicyID,TNamedItem,TmnxVRtrMplsLspID,TmnxAdminState
             FROM TIMETRA-TC-MIB
        vRtrIfEntry,vRtrIfDHCPEntry,vRtrIfDHCP6Entry,vRtrIfDHCPRelayStatsEntry
             FROM TIMETRA-VRTR-MIB
        DisplayString
             FROM SNMPv2-TC
        MplsTunnelIndex
             FROM MPLS-TE-MIB
        pwTemplateEntry
             FROM TIMETRA-SDP-MIB
        InterfaceIndexOrZero
             FROM IF-MIB
        ;



