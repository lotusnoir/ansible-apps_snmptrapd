fadPortExtPMCurrent15MinIntervalTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF FadPortExtPMCurrent15MinIntervalEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "The fadPortExtPMCurrent15MinIntervalTable contains the current 15 minute interval statistics per port."
    ::= { fadPortExtPMStatistics 1 }

fadPortExtPMCurrent15MinIntervalEntry OBJECT-TYPE
    SYNTAX     FadPortExtPMCurrent15MinIntervalEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "Table entry for the current 15 minute interval statistics per port."
    INDEX { tmnxPortPortID }
    ::= { fadPortExtPMCurrent15MinIntervalTable 1 }

FadPortExtPMCurrent15MinIntervalEntry ::= SEQUENCE {
    fadPortExtPMCurrent15MinTimeElapsed     	Integer32,
    fadPortExtPMCurrent15MinTimeMeasured    	Integer32,
    fadPortExtPMCurrent15MinInPackets    	Counter64,
    fadPortExtPMCurrent15MinInOctets   		Counter64,
    fadPortExtPMCurrent15MinInErrors   		Counter64,
    fadPortExtPMCurrent15MinInPacketDrops 	Counter64,
    fadPortExtPMCurrent15MinInDiscards     	Counter64,
    fadPortExtPMCurrent15MinOutPackets      	Counter64,
    fadPortExtPMCurrent15MinOutOctets    	Counter64,
    fadPortExtPMCurrent15MinOutErrors    	Counter64,
    fadPortExtPMCurrent15MinOutPacketDrops  	Counter64,
    fadPortExtPMCurrent15MinOutDiscards      	Counter64, 
    fadPortExtPM15MinValidIntervals         	Integer32,
    fadPortExtPMCurrent15MinInvalidDataFlag     TruthValue,
    fadPortExtPMCurrent15MinOutputBw	    	Counter64,
    fadPortExtPMCurrent15MinInputBw      	Counter64,
    fadPortExtPMCurrent15MinRxCRCAlignErrors	Counter64,
    fadPortExtPMCurrent15MinTxCRCAlignErrors	Counter64,
    fadPortExtPMCurrent15MinTxCollisions	       Counter64
	
}

fadPortExtPMCurrent15MinTimeElapsed OBJECT-TYPE
    SYNTAX      Integer32 (0..900)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The amount of time, measured in seconds, that has elapsed since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 1 }

fadPortExtPMCurrent15MinTimeMeasured OBJECT-TYPE
    SYNTAX      Integer32 (0..900)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The amount of time, measured in seconds, that statistics for this entry have been actually counted for the current 15 minute interval. This value will be the same as the elapsed duration except in a situation where performance monitoring data could not be collected  for this port for some reason."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 2 }

fadPortExtPMCurrent15MinInPackets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of packets received on this port since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 3 }

fadPortExtPMCurrent15MinInOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of bytes received on this port since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 4 }

fadPortExtPMCurrent15MinInErrors OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of packets with errors received on this port since the beginning of the current 15 minute interval."
