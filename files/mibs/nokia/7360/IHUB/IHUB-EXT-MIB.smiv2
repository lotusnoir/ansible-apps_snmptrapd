fadPortExtStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF FadPortExtStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The fadPortExtStatsTable augments the tmnxPortEtherTable.
         This table has the same entries."
     ::= { fadPortExtStatistics 1 }
	 
fadPortExtStatsEntry OBJECT-TYPE
    SYNTAX      FadPortExtStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each row entry represents an entry for each Ethernet port in 
         the system. Entries cannot be created and deleted via SNMP 
         SET  operations."
     AUGMENTS { tmnxPortEtherEntry }
     ::= { fadPortExtStatsTable  1 }

FadPortExtStatsEntry ::= SEQUENCE {
     fadPortExtStatsRxPkts64Octets             Counter64,
     fadPortExtStatsTxPkts64Octets             Counter64,
     fadPortExtStatsRxPkts65to127Octets        Counter64,
     fadPortExtStatsTxPkts65to127Octets        Counter64,
     fadPortExtStatsRxPkts128to255Octets       Counter64,
     fadPortExtStatsTxPkts128to255Octets       Counter64,
     fadPortExtStatsRxPkts256to511Octets       Counter64,
     fadPortExtStatsTxPkts256to511Octets       Counter64,
     fadPortExtStatsRxPkts512to1023Octets      Counter64,
     fadPortExtStatsTxPkts512to1023Octets      Counter64,
     fadPortExtStatsRxPkts1024to1518Octets     Counter64,
     fadPortExtStatsTxPkts1024to1518Octets     Counter64,
     
     fadPortExtStatsRxOversizePkts             Counter64,
     fadPortExtStatsTxOversizePkts             Counter64,
     fadPortExtStatsRxFragments                Counter64,
     fadPortExtStatsTxFragments                Counter64,
     fadPortExtStatsRxJabbers                  Counter64,
     fadPortExtStatsTxJabbers                  Counter64,
     fadPortExtStatsRxUndersizePkts            Counter64,
     fadPortExtStatsTxUndersizePkts            Counter64,
     fadPortExtStatsRxDropEvents               Counter64,
     fadPortExtStatsTxDropEvents               Counter64,
     fadPortExtStatsRxCRCAlignErrors           Counter64,
     fadPortExtStatsTxCRCAlignErrors           Counter64
}

	 
