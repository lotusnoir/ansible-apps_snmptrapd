    ::= { fadIesIfExtEntry 2 }

--  ==================================================================
--  |                  START DUPLICATE MAC ALARM MIB                 |
--  ==================================================================
--
--  Duplicate MAC Alarm Objects
--

fadDupMacIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "This is the value of the index of a fadDupMacOccurence trap.
                     It must uniquely identify a trap for a long time period. As
                     such, this index must be incremented every time a 
                     fadDupMacOccurence trap is sent out. When maximum value is 
                     reached, the index shall overflow into minimum value. The 
                     receiver of the fadDupMacOccurence trap should not do any 
                     calculations on this index.
                     This is also the value of the index of a fadDupMacClear trap.
                     In such trap, the index shall have the same value as the 
                     corresponding fadDupMacOccurence trap. As such, the receiver
                     of the fadDupMacClear trap should be able to link this trap
                     with the appropriate fadDupMacOccurence trap."
    ::= { fadFdbExtNotifyObjs 1 }

fadDupMacAddr OBJECT-TYPE
    SYNTAX          MacAddress
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "The value of the object fadDupMacAddr specifies the
                     48-bit IEEE 802.3 MAC address that is duplicated."
    ::= { fadFdbExtNotifyObjs 2 }

fadDupMacSapPortId OBJECT-TYPE
    SYNTAX          TmnxPortID
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     " This is the ID of the access port on which MAC address
                     with value fadDupMacAddr first has been learned or configured
                     in the scope of the VPLS defined by svcID, It's
                     value must be set to 0x1e000000 -an invalid portid- when
                     fadDupMacSdpBindId is different from 0."
::= { fadFdbExtNotifyObjs 3 }

fadDupMacSapEncapValue OBJECT-TYPE
    SYNTAX          TmnxEncapVal
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "This is the value of the label used to identify the 
                     SAP on which MAC address with value fadDupMacAddr 
                     first has been learned or configured in the scope of 
                     the VPLS defined by svcID. 
                     This fadDupMacSapEncapValue is only applicable when 
                     fadDupMacSapPortId contains a valid portid, together they 
                     identify this SAP."
    ::= { fadFdbExtNotifyObjs 4 }
    
fadDupMacSdpBindId OBJECT-TYPE
    SYNTAX          SdpBindId
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     " This is the Identification of the SDP binding on which MAC 
                    address with value fadDupMacAddr first has been learned or 
                    configured in the scope of the VPLS defined by svcID. 
                    It's value is applicable when fadDupMacType=extern and                 
                    fadDupMacSapPortId is set to an invalid port-id."
    ::= { fadFdbExtNotifyObjs 9 }

fadDupMacDupCVlan OBJECT-TYPE
    SYNTAX          TmnxEncapVal
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "The value of the object fadDupMacDupCVlan specifies the
                     12-bit IEEE 802.1Q C-VLAN ID that is seen in the
                     frames causing the Duplicated MAC Alarm."
    ::= { fadFdbExtNotifyObjs 5 }

fadDupMacDupSapPortId OBJECT-TYPE
    SYNTAX          TmnxPortID
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "This is the ID of the access port on which MAC duplication
                     with value fadDupMacAddr occurred in the scope of the VPLS
                     defined by svcID."
    ::= { fadFdbExtNotifyObjs 6 }

fadDupMacDupSapEncapValue OBJECT-TYPE
    SYNTAX          TmnxEncapVal
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     "This is the value of the label used to identify the SAP on
                     which MAC duplication with value fadDupMacAddr occurred. 
                     This fadDupMacDupSapEncapValue together with 
                     fadDupMacDupSapPortId identify this SAP."
    ::= { fadFdbExtNotifyObjs 7 }
    
fadDupMacDupSdpBindId OBJECT-TYPE
    SYNTAX          SdpBindId
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     " This is the ID of the SDP binding on which MAC duplication
                      of fadDupMacType =self occurred in the scope of the VPLS
                     defined by svcID. Value defined when fadDupMacType=self"
    ::= { fadFdbExtNotifyObjs 10 }

fadDupMacType OBJECT-TYPE
    SYNTAX          INTEGER {
                       extern (1),
                       self (2)
                    }
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION     " This specifies the severity of the MAC duplication:
                        extern  a MAC address already learned from a host at user  
                               premises or at network side, is detected as source 
                               MAC address on a residential SAP. In this case:
                               fadDupMacDupSapPortId and fadDupMacDupSapEncapValue
                               identify this residential SAP.                              
                               fadDupMacSapPortId/fadDupMacSapEncapValue or 
                               fadDupMacSdpBindId identify where the MAC was first 
                               learned.                                                         
                       self    a MAC address belonging to the System itself or 
                               to a port of the System, is detected as source MAC
                               address on a SAP/SDPbinding. In this case:
                               fadDupMacSapPortId and fadDupMacSapEncapValue and 
                               fadDupMacSdpBindId are dummy;
                               fadDupMacDupSapPortId and fadDupMacDupSapEncapValue
                               identify the violating SAP or fadDupMacDupSdpBindId 
                               identify the violating SDP binding."
    ::= { fadFdbExtNotifyObjs 8 }


--
--  Duplicate MAC Notification
--
fadDupMacOccurence NOTIFICATION-TYPE 
    OBJECTS { 
       svcId,                      -- Service Id.
       fadDupMacAddr,              -- The duplicated MAC address.
       fadDupMacSapPortId,         -- SAP on which MAC is first associated.
       fadDupMacSapEncapValue,     -- SAP on which MAC is first associated.
       fadDupMacSdpBindId,       -- SDP binding on which MAC is first associated
       fadDupMacDupCVlan,          -- CVLAN of duplicated MAC.
       fadDupMacDupSapPortId,      -- SAP on which MAC is duplicated.
       fadDupMacDupSapEncapValue,  -- SAP on which MAC is duplicated.
       fadDupMacDupSdpBindId,     -- SDP binding on which MAC is duplicated.
       fadDupMacIndex,
       fadDupMacType

    } 
    STATUS       current 
    DESCRIPTION 
       "A fadDupMacOccurence notification is generated when:
         -the MAC learning detects the occurrence of a MAC address on a residential 
          SAP, while the same MAC address is in the Forwarding Database associated 
          to another SAP and this MAC address is not yet aged-out in the Forwarding 
          Database. 
         -the MAC learning detects the occurrence of a MAC address on a SAP or SDP
          binding that is equal to the system MAC or any system port MAC."
         
         ::= { fadFdbExtNotifications 1 } 
         
fadDupMacClear NOTIFICATION-TYPE 
    OBJECTS { 
        svcId,                      -- Service Id.
        fadDupMacAddr,              -- The duplicated MAC address.
        fadDupMacSapPortId,         -- SAP on which MAC is first associated.
        fadDupMacSapEncapValue,     -- SAP on which MAC is first associated.
        fadDupMacSdpBindId,         -- SDP binding on which MAC is first associated
        fadDupMacDupCVlan,          -- CVLAN of duplicated MAC.
        fadDupMacDupSapPortId,      -- SAP on which MAC is duplicated.
        fadDupMacDupSapEncapValue,  -- SAP on which MAC is duplicated.
        fadDupMacDupSdpBindId,      -- SDP binding on which MAC is duplicated.
        fadDupMacIndex,
        fadDupMacType
    } 
    STATUS       current 
    DESCRIPTION 
         "A fadDupMacClear notification is generated dubMacAgeTime
         after the Duplicate MAC event is detected.
         Here, dubMacAgeTime is fixed 300s (5 minutes).
         For each residential SAP on which the same fadDupMacAddr was detected, a 
         separate fadDupMacClear notification shall be timed and generated. 
         Each such SAP shall be identified by 
         fadDupMacDupSapPortId and fadDupMacDupSapEncapValue."
         ::= { fadFdbExtNotifications 2 } 

