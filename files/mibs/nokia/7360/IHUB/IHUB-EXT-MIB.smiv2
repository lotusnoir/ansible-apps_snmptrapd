fadSvcTlsExtEntry OBJECT-TYPE
    SYNTAX      FadSvcTlsExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "TLS (VPLS) specific information about a specific service. 
         Entries cannot be created and deleted via SNMP SET 
         operations."
    AUGMENTS { svcTlsInfoEntry }
    ::= { fadSvcTlsExtTable 1 }

FadSvcTlsExtEntry ::=
    SEQUENCE {
        fadVplsU2uDistinctSaps     TmnxEnabledDisabled,
        fadVplsUnrestrictMacMove   TmnxEnabledDisabled,
        fadVplsExtFwdDeviceIpAddressType InetAddressType,
        fadVplsExtFwdDeviceIpAddress   InetAddress
    }

fadVplsU2uDistinctSaps OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabled
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is only meaningfull for v-VPLS and VPLS services.
         When enabled, this object means that user to user traffic is allowed
         between distinct SAPs. 
         The value of this object can be specified when the row is created 
         and can be changed at any time"
    DEFVAL          { disabled }
    ::= { fadSvcTlsExtEntry 1 }

fadVplsUnrestrictMacMove OBJECT-TYPE
    SYNTAX      TmnxEnabledDisabled
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is only meaningfull for VPLS service.
         When enabled, this object means that the network side has no priority 
         anymore over user side about MAC@ learning. When enabled, MAC@ move is 
         allowed from network side to user side. The value of this object can be 
         specified when the row is created and can be changed at any time"
    DEFVAL          { disabled }
    ::= { fadSvcTlsExtEntry 2 }

fadVplsExtFwdDeviceIpAddressType OBJECT-TYPE
