fadPortExtPMCurrent15MinInPacketDrops OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
	
   
	"The number of packets dropped on this port since the beginning of the current 15 minute interval.(Includes errors and discards)"
   ::= {fadPortExtPMCurrent15MinIntervalEntry 6 }


fadPortExtPMCurrent15MinInDiscards OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
	
   
	"The number of packets discarded on this port since the beginning of the current 15 minute interval."
   ::= {fadPortExtPMCurrent15MinIntervalEntry 7 }





fadPortExtPMCurrent15MinOutPackets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of packets transmitted on this port since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 8 }

fadPortExtPMCurrent15MinOutOctets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of bytes transmitted on this port since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 9 }

fadPortExtPMCurrent15MinOutErrors OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of packets with errors transmitted on this port since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 10 }

fadPortExtPMCurrent15MinOutPacketDrops OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION






	"The number of packets dropped on this port since the beginning of the current 15 minute interval.(Includes errors and discards)"
   ::= {fadPortExtPMCurrent15MinIntervalEntry 11 }

fadPortExtPMCurrent15MinOutDiscards OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of packets  discarded on this port since the beginning of the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 12 }



fadPortExtPM15MinValidIntervals OBJECT-TYPE
    SYNTAX      Integer32 (0..96)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of previous 15 min intervals for which valid data was collected for this port. This is equal to the number of entries in the fadPortExtPMPrevious15MinIntervalTable for this port."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 13}

fadPortExtPMCurrent15MinInvalidDataFlag OBJECT-TYPE
    SYNTAX      TruthValue 
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
	"The object indicates the validity of the corresponding PM data.It is set to false when the PM data is complete and valid and set to true when the PM data is incomplete or invalid."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 14}

fadPortExtPMCurrent15MinOutputBw          OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the average Output Bandwidth since the beginning of the  
         current 15 minute interval."
        
    ::= { fadPortExtPMCurrent15MinIntervalEntry 15 }      

fadPortExtPMCurrent15MinInputBw          OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the average input Bandwidth since the beginning of the 
         current 15 minute interval."
        
    ::= { fadPortExtPMCurrent15MinIntervalEntry 16 }   

fadPortExtPMCurrent15MinRxCRCAlignErrors          OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "Specifies the count of the total number of packets received since the 
          beginning of the current 15 minute interval that had a length (excluding 
         framing bits, but including FCS octets) of between 64 and 1518 octets,  
         inclusive, but had either a bad Frame Check Sequence (FCS) with an 
         integral number of octets (FCS Error) or a bad FCS with a non-integral 
         number of octets (Alignment Error)."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 17 }   
    
fadPortExtPMCurrent15MinTxCRCAlignErrors          OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the count of total number of packets transmitted since the 
         beginning of the current 15 minute interval that had a length (excluding 
         framing bits, but including FCS octets) of between 64 and 1518 octets, 
         inclusive, but had either a bad Frame Check Sequence (FCS) with an 
         integral number of octets (FCS Error) or a bad FCS with a non-integral 
         number of octets (Alignment Error)."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 18 }       

    
fadPortExtPMCurrent15MinTxCollisions          OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the count of Transmit Collisions Error since the beginning of 
         the current 15 minute interval."
    ::= { fadPortExtPMCurrent15MinIntervalEntry 19 }   

