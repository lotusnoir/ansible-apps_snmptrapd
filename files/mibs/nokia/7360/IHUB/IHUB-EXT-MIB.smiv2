    ::= { fadvRtrIgmpProxyGeneralTable 1 }

FadvRtrIgmpProxyGeneralEntry ::=
    SEQUENCE {
	    fadvRtrIgmpGenProxyVersion                       INTEGER,
	    fadvRtrIgmpGenProxyLoadBalancTimer               Unsigned32,
        fadvRtrIgmpGenProxyHoldOffTimer                  Unsigned32,
        fadvRtrIgmpGenProxyRevertible                    INTEGER,
        fadvRtrIgmpGenProxyIfOperState                   INTEGER,
	    fadvRtrIgmpGenProxyAdminState                    TmnxAdminState,
