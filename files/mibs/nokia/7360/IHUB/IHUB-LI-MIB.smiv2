IHUB-LI-MIB DEFINITIONS ::= BEGIN

-- Import list need to delete the unused ones!

IMPORTS
           Counter32,
           Integer32,
           MODULE-IDENTITY,
           OBJECT-TYPE,  
           enterprises,
           IpAddress,
           Unsigned32,
	   Counter64
                   FROM SNMPv2-SMI
	
	   OBJECT-GROUP,
	   MODULE-COMPLIANCE
	   	   FROM SNMPv2-CONF
   
           InetAddress,
           InetAddressType,
           InetAddressPrefixLength, 
           InetAddressIPv6
                   FROM INET-ADDRESS-MIB
  
           DateAndTime,
           RowStatus,
           TruthValue,
           DisplayString,
           TEXTUAL-CONVENTION
                   FROM SNMPv2-TC;
   
           

ihubLiMIB   
        MODULE-IDENTITY
        LAST-UPDATED    "201002200000Z"
        ORGANIZATION    "Alcatel"
        CONTACT-INFO
            "Tom Batsele
             Email:  asam.mibsupport@alcatel-lucent.com
            " 
        DESCRIPTION
            "This document is the SNMP MIB module to manage and provision
             Lawfull intercept targets on Alcatel-Lucent IHUB for
             the 7302 ISAM, 7330 ISAM FTTN, and 7342 ISAM FTTU systems.
            "

--
--  Revision History
--


        REVISION        "201002200000Z"
        DESCRIPTION     "Rev 0.1                    20 Feb 2010 00:00
                         Initial version of the IHUB-LI-MIB."


::= { enterprises alcatel(637) asd(61) asam(1) 88}  

  


ihubLiSession          OBJECT IDENTIFIER ::= { ihubLiMIB 1 }
ihubLiConfig           OBJECT IDENTIFIER ::= { ihubLiMIB 2 }
ihubLiConformance      OBJECT IDENTIFIER ::= { ihubLiMIB 3 }

-- ----------------------------
-- Textual Conventions
-- -------------------------

     
IhubLiItemDescription ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "80a"
    STATUS       current
    DESCRIPTION
        "Description for an item.  Note, use only NVT ASCII displayable characters
        here, no control characters, no UTF-8, etc."
     SYNTAX OCTET STRING (SIZE(0..80))
     
IhubLiIPFilterID ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS       current
    DESCRIPTION
        "identification number of a IP filter"
    SYNTAX Unsigned32  (0..32)
   
IhubLiRouterServiceID ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS       current
    DESCRIPTION
        " The Service Id which uniquely identifies a router instance. 
         value= 0 : base router 
         value > 0: VPRN service id 
         value= -1: not applicable.
	"
    SYNTAX Integer32(-1|0..2147483647)    

IhubLiTcpUdpPort ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS       current
    DESCRIPTION
        "The number of a TCP or UDP port.
         Well known port numbers include
         ftp-data(20), ftp(21), telnet(23), smtp(25), http(80),
         pop3(110), nntp(119), snmp(161), snmptrap(162), etc."
    REFERENCE "http://www.iana.org/assignments/port-numbers"
    SYNTAX Integer32 (0 | 1..65535)
    
    
IhubLiTcpUdpPortOperator ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
       "The operator specifies the manner in which a couple of other
        MIB objects in the table are supposed to be used.

        Operator        Value1               Value2
        ----------------------------------------------------
        none(0)         Any(0)               Any(0)
        eq(1)           Specified Value      Any(0)
        range(2)        Starting Value       Ending Value
        lt(3)           Specified Value      Any(0)
        gt(4)           Specified Value      Any(0)

        'Any(0)' specifies that, this object can accept any values
         but would default to 0. "
    SYNTAX       INTEGER {
                    none(0),
                    eq(1),
                    range(2),
                    lt(3),
                    gt(4)
                 }
                 
IhubLiIpAddressPrefixLength ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS       current
    DESCRIPTION
        "The number of bits to match in an IP address mask."
    SYNTAX Integer32 (0..32)

IhubLiDSCPValueOrAny ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS   current
    DESCRIPTION
       "The IP header Differentiated Services Code-Point that may be
       used for discriminating among traffic streams. The value -1 is
       used to indicate a wild card i.e. any value."
    REFERENCE
        "RFC 2474, RFC 2780"
    SYNTAX   Integer32 (-1 | 0..63)
                 

-- ----------------------------
-- MIB Objects begin here
-- ----------------------------

--
-- This MIB defines the session attributes 
-- 

ihubLiSessionAdminStatus   OBJECT-TYPE
    SYNTAX      INTEGER {
                    enabled (0),
                    disabled (1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of ihubLiSessionAdminStatus indicates whether Lawful Intercept (LI)
         is globally enabled
         "
         ::= { ihubLiSession 1 }

ihubLiSessionOperStatus   OBJECT-TYPE
    SYNTAX      INTEGER {
                        up (0),
                        down (1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of ihubLiSessionOperStatus indicates whether Lawful Intercept (LI)
         is operational up
         "
         ::= { ihubLiSession 2 }

         
ihubLiMediation        OBJECT IDENTIFIER ::= { ihubLiSession 3 }
ihubLiTarget             OBJECT IDENTIFIER ::= { ihubLiSession 4 }
ihubLiFilter                OBJECT IDENTIFIER ::= { ihubLiSession 5 }

ihubLiSessionRetentionValue  OBJECT-TYPE
    SYNTAX     Integer32 (0.. 2147483647)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION

    " Indicates in minutes how long session data  ( e.g mediation entries, filter entries ) is retained 
      when no targets are existing anymore. This retention timer cleans up not needed resources.
      When 0 is specified, the data will be kept until explicitly deleted.
      By default the data is kept for 1 day;
               "
    DEFVAL { 1440 }      
         ::= { ihubLiSession 6}
  

--
-- This MIB defines the addressing of the mediation functions 
-- 

ihubLiMediationTable OBJECT-TYPE
     SYNTAX     SEQUENCE OF IhubLiMediationEntry
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
     
        "The Call Content Mediation Function Table lists the mediation functions 
         to which intercepted traffic should be sent.
         The intercepted traffic is sent over IP, and one can specify: 
          the IP address of the device ( IPv4 address, )
          the IP DSCP settings to be used
          the Ethernet Pbit settings to be used
          the IP protocol and encapsulation to be used ( e.g gre, udp, tcp, sctp,...
         Entries are added to this table via ihubLiMediationStatus in 
         accordance with the RowStatus convention 
         
          ALCATEL NOTE :
	               TABLESIZE : 4
         
        "
     ::= { ihubLiMediation 1 }


ihubLiMediationEntry OBJECT-TYPE
     SYNTAX     IhubLiMediationEntry
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
        "The entry describes a single session maintained with an
	 application on a Mediation Device (i.e a mediation function ) 
	"
     INDEX      { ihubLiMediationIndex }
     ::= { ihubLiMediationTable 1 }
     

IhubLiMediationEntry ::= SEQUENCE {
        ihubLiMediationIndex              Integer32,
        ihubLiMediationRowStatus          RowStatus,
        ihubLiMediationDescription          IhubLiItemDescription,
        ihubLiMediationAddressType        InetAddressType,
        ihubLiMediationDestAddress        InetAddress,
        ihubLiMediationSrcAddress         InetAddress,
        ihubLiMediationDscp                    Integer32,
        ihubLiMediationPbit                        Integer32,
        ihubLiMediationTransportMethod    INTEGER,
        ihubLiMediationAdminStatus        INTEGER,         
        ihubLiMediationOperStatus         INTEGER,
        ihubLiMediationRouterServiceId  IhubLiRouterServiceID
        }


ihubLiMediationIndex OBJECT-TYPE
     SYNTAX     Integer32 (1..4)
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
        "Uniquely identifies a mediation function on this system. 
        "
     ::= { ihubLiMediationEntry 1 }

ihubLiMediationRowStatus OBJECT-TYPE
     SYNTAX     RowStatus
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
       "The status of this conceptual row. This object is used to
        manage creation and deletion of rows in this table.
         
        The values supported during a set operation are 'createAndGo' 
         'destroy' .
        
        An entry can only be destroyed when not referred to by 
        ihubLiTargetMediationIndex and adminStatus being in state 'disabled'        .
       "
     ::= { ihubLiMediationEntry 2 }


ihubLiMediationDescription  OBJECT-TYPE
    SYNTAX       IhubLiItemDescription
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "User-provided description for this mediation function."
    DEFVAL { ''H }
    ::= { ihubLiMediationEntry 3 }


ihubLiMediationAddressType OBJECT-TYPE
     SYNTAX     InetAddressType
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The type of ihubLiMediationDestAddress.
        Restriction: only ipv4 supported 
        "
     DEFVAL { ipv4 }
     ::= { ihubLiMediationEntry 4 }

ihubLiMediationDestAddress OBJECT-TYPE
     SYNTAX     InetAddress
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The IP Address of the Mediation function's network interface
         to which to direct intercepted traffic. 
         This parameter must be provided when creating a row.
         The value should be different then: loopback address,  class D address, local interface address 
          to be accepted
        "
     ::= { ihubLiMediationEntry 5 }

ihubLiMediationSrcAddress OBJECT-TYPE
     SYNTAX     InetAddress
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The IP adress used as source IP for encapsulating
         When 0.0.0.0 is specified then system IP address will be used:
         When a value is specified it should be an address matching a subnet of an interface of
         the specified router, otherwise the request is rejected. 
        "
     DEFVAL       { '00000000'H } -- 0.0.0.0
     ::= { ihubLiMediationEntry 6 }

ihubLiMediationDscp OBJECT-TYPE
     SYNTAX     Integer32 (0..63)
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The Differentiated Services Code Point the intercepting
         device applies to the IP packets encapsulating the
         intercepted traffic.
        "
     DEFVAL { 34 }        -- by default, AF41, code 100010
     ::= { ihubLiMediationEntry 7 }


ihubLiMediationPbit OBJECT-TYPE
     SYNTAX     Integer32 (0..7)
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The P-bit setting the intercepting device applies to the IP packets 
        encapsulating the intercepted traffic.
        "
     DEFVAL { 3 }        
     ::= { ihubLiMediationEntry 8 }


ihubLiMediationTransportMethod OBJECT-TYPE
     SYNTAX     INTEGER { 
			   gre (1)
			 }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The IP protocol used for transferring intercept data to the mediation
         function.
        "
     DEFVAL { 1 }   
     ::= { ihubLiMediationEntry 9 }

ihubLiMediationAdminStatus OBJECT-TYPE
     SYNTAX      INTEGER {
                    enabled (0),
                    disabled (1)
                }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
         "
           Admin state; allows to enable/disable interception to this device
          A Mediation entry can only be modified when being in state 'disabled' except for the description 
          field
        "
        DEFVAL {disabled}
     ::= { ihubLiMediationEntry 10 }

ihubLiMediationOperStatus OBJECT-TYPE
     SYNTAX     INTEGER { 
			   up(1),
                           downAdmin(2),
                           downGlobal(3),
                           downRouter(4),
                           downSrcIP(5),
                           downOther(6)
                           }
     MAX-ACCESS read-only
     STATUS     current
     DESCRIPTION
         "
           The operational state will indicate down ( in order of priority)  when
-	ihubLiMediationAdminStatus is disabled ( downAdmin )
-	ihubLiSessionOperStatus is down ( downGlobal)
-	ihubLiSessionIntercept is disabled ( downGlobal )
-	router instance referred by ihubLiMediationRouterServiceId  is operational down (  downRouter ) 
-	In case a ihubLiMediationSrcAddress has been specified but the matching subnet interface is operational down ( downSrcIP )
-	In case ihubLiMediationSrcAddress is specified as 0.0.0;0, and all IP interface of the router are operational down ( downSrcIP )
             
         "
     ::= { ihubLiMediationEntry 11 }

ihubLiMediationRouterServiceId OBJECT-TYPE
      SYNTAX     IhubLiRouterServiceID
      MAX-ACCESS read-create
      STATUS     current
      
      DESCRIPTION
         "The router id is the Service Id of the VPRN or 0 for base router.
          note: value -1 is not allowed
          When a not existing value is specified the operational state wil be down.
          R4.2 restriction: only base router supported.
         "
        DEFVAL {0}
      ::= { ihubLiMediationEntry 12  }  
 
--
-- Mediation free index
--

 ihubLiMediationGetNextFreeIndex  OBJECT-TYPE
    SYNTAX  Integer32(0..4)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
         "The next Free Index value that can be used for creating
	 a new entry in the Mediation Table.
         This object contains an appropriate value to be used for 
         ihubLiMediationIndex  when creating entries in the ihubLiMediationTable. 
         The value 0 indicates that no unassigned entries are available. To 
         obtain the ihubLiMediationIndex  value for a new entry, the
         manager issues a management protocol retrieval operation to obtain the
         current-value of this object. After each retrieval, the agent should
         modify the value to the next unassigned index.
       "
    ::= { ihubLiMediation 2 }


--
-- This MIB defines the target(s) that have to be placed under intercept 
-- 

ihubLiTargetTable OBJECT-TYPE
     SYNTAX       SEQUENCE OF IhubLiTargetEntry
     MAX-ACCESS not-accessible
     STATUS       current
     DESCRIPTION
        "The Intercept Target Table lists the targets to be 
         intercepted. 
        
         Targets should be non overlapping; as packets from 
         a target can only once be duplicated with a specific
         Lawful Intercept IDentifier (LIID)

         Entries are added to this table via ihubLiTargetRowStatus in
         accordance with the RowStatus convention.
         
         Up to 16 targets can be created, but only for 8 the rowStatus can be active
         
         ALCATEL NOTE :
	               TABLESIZE : 16
         "
     ::= { ihubLiTarget 1  }


ihubLiTargetEntry OBJECT-TYPE
     SYNTAX     IhubLiTargetEntry
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
        "A target entry indicates a single target to be
         intercepted to a Mediation Device. 
        "
     INDEX { ihubLiTargetIndex}
     ::= { ihubLiTargetTable 1 } 


IhubLiTargetEntry ::= SEQUENCE {
        ihubLiTargetIndex                  Integer32,
        ihubLiTargetRowStatus              RowStatus,
        ihubLiTargetDescription            IhubLiItemDescription,
        ihubLiTargetEgressLIID             Integer32,
        ihubLiTargetIngressLIID            Integer32,
        ihubLiTargetMediationIndex         Integer32,
        ihubLiTargetEgressIPFilterIndex    IhubLiIPFilterID, 
        ihubLiTargetEgressIPv6FilterIndex  IhubLiIPFilterID, 
        ihubLiTargetIngressIPFilterIndex   IhubLiIPFilterID, 
        ihubLiTargetIngressIPv6FilterIndex IhubLiIPFilterID, 
        ihubLiTargetTimeout                DateAndTime,
        ihubLiTargetAdminStatus            INTEGER,
        ihubLiTargetOperStatus             INTEGER,
        ihubLiTargetEnableStats	           TruthValue
}

ihubLiTargetIndex OBJECT-TYPE
     SYNTAX     Integer32 (1..16)
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
        "The unique identification of the target in this system.
        "
     ::= { ihubLiTargetEntry 1 }

ihubLiTargetRowStatus OBJECT-TYPE
     SYNTAX     RowStatus
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The status of this conceptual row. This object manages
        creation and deletion of rows in this table.
        
        The values supported during a set operation are 'createAndGo' 
	 and   'destroy' 
	        
	The adminStatus needs first to be set to 'disabled' before destroy is allowed, 
	destroyal will lead also to destroyal of coresponding row details:
	ihubLiTargetStaticIPTable, ihubLiTargetCircuitTable, ihubLiTargetRemoteTable
	
        "
     ::= { ihubLiTargetEntry 2 }
     
ihubLiTargetDescription  OBJECT-TYPE
    SYNTAX       IhubLiItemDescription
    MAX-ACCESS   read-create
    STATUS       current
    DESCRIPTION
        "User-provided description for this target."
    DEFVAL { ''H }
    ::= { ihubLiTargetEntry 3 }

ihubLiTargetEgressLIID OBJECT-TYPE
     SYNTAX     Integer32 (0..2147483647)
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "This object contains a value called the Lawful Intercept IDentifier (LIID) 
        that should be passed for this target together with the egress intercepted data to the
        mediation function specified by ihubLiTargetMediationIndex
        A value of 0 means not applicable.
        Depending on the value of ihubLiMediationProtEncapsMethod the LLID can be constrained
        -gre1: value must fit in 12bit vlan-id, allowed values are 1..4095
       This parameter must be provided when creating a row and cannot be modified.
        "
     ::= { ihubLiTargetEntry 4 }

ihubLiTargetIngressLIID OBJECT-TYPE
     SYNTAX     Integer32 (0..2147483647)
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "This object contains a value called the Lawful Intercept IDentifier (LIID) 
        that should be passed for this target together with the ingress intercepted data to the
        mediation function specified by ihubLiTargetMediationIndex
        A value of 0 means not applicable.
        Depending on the value of ihubLiMediationProtEncapsMethod the LLID can be constrained
        -gre1: value must fit in 12bit vlan-id, allowed values are 1..4095
       This parameter must be provided when creating a row and cannot be modified.
        "
     ::= { ihubLiTargetEntry 5 }


ihubLiTargetMediationIndex OBJECT-TYPE
     SYNTAX     Integer32 (1..4)
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "Indicates to which mediation function that data should be sent
         When invalid mediation index, set operation is rejected
          This parameter must be provided when creating a row and cannot be modified.
         "
     ::= { ihubLiTargetEntry 6 } 
	
ihubLiTargetEgressIPFilterIndex OBJECT-TYPE
     SYNTAX     IhubLiIPFilterID
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "A index used to identify a lawful intercept target filter in the system ihubLiIPFilterTable
         This field is optional, 0 is used to indicate this
         Note that IP addresses specified in the filter are only taken into account when
         they match a target IP address.
        "
      DEFVAL { 0 }
     ::= { ihubLiTargetEntry 7  }  
     
ihubLiTargetEgressIPv6FilterIndex OBJECT-TYPE
  SYNTAX     IhubLiIPFilterID
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
     "A index used to identify a lawful intercept target IPv6 filter in the system ihubLiIPv6FilterTable
      This field is optional, 0 is used to indicate this
      Note that IP addresses specified in the filter are only taken into account when
      they match a target IP address.
     "
   DEFVAL { 0 }
::= { ihubLiTargetEntry 8  }  

ihubLiTargetIngressIPFilterIndex OBJECT-TYPE
  SYNTAX     IhubLiIPFilterID
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
     "A index used to identify a lawful intercept target filter in the system ihubLiIPFilterTable
      This field is optional, 0 is used to indicate this
      Note that IP addresses specified in the filter are only taken into account when
      they match a target IP address.
     "
   DEFVAL { 0 }
::= { ihubLiTargetEntry 9  }  

ihubLiTargetIngressIPv6FilterIndex OBJECT-TYPE
  SYNTAX     IhubLiIPFilterID
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
     "A index used to identify a lawful intercept target IPv6 filter in the system ihubLiIPv6FilterTable
      This field is optional, 0 is used to indicate this
      Note that IP addresses specified in the filter are only taken into account when
      they match a target IP address.
     "
   DEFVAL { 0 }
::= { ihubLiTargetEntry 10  }  

ihubLiTargetTimeout OBJECT-TYPE
     SYNTAX     DateAndTime
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The time at which this row and all related targetrows ( circuit, remote, static) 
         should be automatically removed,and the intercept function cease. 
         Since the initiating network manager may 
         be the only device able to manage a specific intercept or know of its
         existence, this acts as a fail-safe for the failure or removal
         of the network manager. 
         
         Minimum value : none, as long as the time is > to current time 
  Maximum value : current time + 20 days. 
	 
	"
    
     ::= { ihubLiTargetEntry 11 }



ihubLiTargetAdminStatus OBJECT-TYPE
     SYNTAX      INTEGER {
                    enabled (0),
                    disabled (1)
                }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
         "
          Admin state; allows to enable/disable interception for this target
          Modification of next fields ihubLiTargetIngress/EgresIP(v6)FilterIndex,ihubLiTargetTimeout, 
          ihubLiTargetDescription is allowed in enabled state. For modification of other fields the admin 
          state should  be set to disabled.
         "
        DEFVAL {disabled}
     ::= { ihubLiTargetEntry 12 }
     
ihubLiTargetOperStatus OBJECT-TYPE
    SYNTAX     INTEGER { 
			   up(1),
                           downAdmin(2),
                           downMediation(3),
                           downDetailsNotSpecified(4),
                           downOther(5)
                            }
     MAX-ACCESS read-only
     STATUS     current
     DESCRIPTION
         "'up': interception of traffic to this target is enabled 
 The operational state will indicate down ( in order of priority) :
          'downAdmin': ihubLiTargetAdminStatus is disabled
          'downMediation': ihubLiTargetMediationIndex point to a mediation device which is not oper up      
          'downDetailsNotSpecified': ihubLiTargetStaticIPTable, ihubLiTargetTable, 
           ihubLiTargetRemoteTable not created
          'downOther': Other reasons
            "
     ::= { ihubLiTargetEntry 13 }
 
ihubLiTargetEnableStats OBJECT-TYPE
    SYNTAX          TruthValue
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     "The value of the object ihubLiTargetEnableStats specifies
                     whether the agent collects accounting statistics for this
                     target. When the value is 'true' the agent
                     collects statistics on this target.
                     "
    DEFVAL          { false }
    ::= { ihubLiTargetEntry 14 }

--
-- The target static IP table
--

ihubLiTargetStaticIPTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IhubLiTargetStaticIPEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "The Intercept Target IP Filter Table allows to specify for a target 
         that does not support DHCP a list of static IP address ranges
         for which IP packets should be sent to the mediation function.
         Up to 8 static IP address ranges can be specified per targetIndex
        "
    ::= { ihubLiTarget 2 }


ihubLiTargetStaticIPEntry OBJECT-TYPE
    SYNTAX       IhubLiTargetStaticIPEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Information about static IP address ranges 
        "
    INDEX { ihubLiTargetIndex, ihubLiTargetStaticIPIndex }
    ::= { ihubLiTargetStaticIPTable 1 }


IhubLiTargetStaticIPEntry ::= SEQUENCE
    {
        ihubLiTargetStaticIPIndex               Unsigned32,
        ihubLiTargetStaticRowStatus             RowStatus,
        ihubLiTargetStaticAddressType           InetAddressType,
        ihubLiTargetStaticAddress               InetAddress,
        ihubLiTargetStaticAddressLength         InetAddressPrefixLength,
        ihubLiTargetStaticRouterServiceId            	IhubLiRouterServiceID
    }

ihubLiTargetStaticIPIndex OBJECT-TYPE
     SYNTAX     Unsigned32  (1..8)
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
        "The unique identification of one of the assigned static IP address ranges
        "
     ::= { ihubLiTargetStaticIPEntry 1 }
     
ihubLiTargetStaticRowStatus OBJECT-TYPE
      SYNTAX     RowStatus
      MAX-ACCESS read-create
      STATUS     current
      DESCRIPTION
         "
         The status of this conceptual row. This object manages
	 creation and deletion of rows in this table.
	         
	 The values supported during a set operation are 'createAndGo' and'destroy' 
	 	        
	 The target under intercept represented by the ihubLiTargetIndex must exist
	 as an entry within the ihubLiTargetTable or row creation will fail.
         When the corresponding entry in the ihubLiTargetTable is deleted this row entry will
         also be deleted.
         "
     ::= { ihubLiTargetStaticIPEntry 2 }
     
ihubLiTargetStaticAddressType OBJECT-TYPE
     SYNTAX     InetAddressType
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The type of address. IPv4 or IPv6
         This parameter must be provided when creating a row.
        "
     ::= { ihubLiTargetStaticIPEntry 3 }

ihubLiTargetStaticAddress OBJECT-TYPE
     SYNTAX     InetAddress
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The IP Address from which to intercept traffic.
         This parameter must be provided when creating a row.
        "
     ::= { ihubLiTargetStaticIPEntry 4 }
 
 ihubLiTargetStaticAddressLength OBJECT-TYPE
      SYNTAX     InetAddressPrefixLength
      MAX-ACCESS read-create
      STATUS     current
