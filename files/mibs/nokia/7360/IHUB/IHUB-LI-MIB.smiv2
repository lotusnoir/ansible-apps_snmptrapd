    ::= { ihubLiIPv6FilterParamsEntry 3 }
    

ihubLiIPv6FilterParamsAction OBJECT-TYPE
     SYNTAX     INTEGER {
                drop(1),
                forward(2),
                default(3)
     }
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "The action to take for packets that match this filter entry. The value
         default(3) specifies this entry to inherit the behavior defined as the
         default for the filter."
     DEFVAL { drop }     
        ::= {ihubLiIPv6FilterParamsEntry 4 } 

ihubLiIPv6FilterParamsNextHeader OBJECT-TYPE
     SYNTAX     Integer32 (-1 | 0..255)
     MAX-ACCESS read-create
     STATUS     current
     DESCRIPTION
        "Pv6 next header to match.  set to -1 to disable matching IPv6 next
         header. If the next header is changed the next header specific parameters
         are reset.
         "
