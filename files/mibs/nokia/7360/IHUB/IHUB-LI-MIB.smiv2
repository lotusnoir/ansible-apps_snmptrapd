--
-- The target statistics table, allows to verify whether packets are sent out
--

ihubLiTargetStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IhubLiTargetStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The Intercept Target statistics Table lists statistics for each target 
         defined in ihubLiTargetTable. Statistices are only collected when ihubLiTargetEnableStats
         is set to true. When set to false 0 is returned.
        "
::= { ihubLiTarget 5 }


ihubLiTargetStatsEntry OBJECT-TYPE
    SYNTAX      IhubLiTargetStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A target stats entry contains statistics data of a single target. 
        "
    AUGMENTS { ihubLiTargetEntry }
::= { ihubLiTargetStatsTable 1 }


IhubLiTargetStatsEntry ::= SEQUENCE {
    ihubLiTargetToPackets            Counter64,
    ihubLiTargetFromPackets          Counter64,
    ihubLiTargetIPsOperational       Integer32,
    ihubLiTargetIPsIntercepted       Integer32
    }
 

ihubLiTargetToPackets OBJECT-TYPE
      SYNTAX     Counter64
      MAX-ACCESS read-only
      STATUS     current
      DESCRIPTION
         "The number of packets sent to the target that have been intercepted
          Allows to verify that interception of traffic is happening
         "
      ::= { ihubLiTargetStatsEntry 1 }
 
      
ihubLiTargetFromPackets OBJECT-TYPE
      SYNTAX     Counter64
      MAX-ACCESS read-only
      STATUS     current
      DESCRIPTION
         "The number of packets from the target that have been intercepted 
          Allows to verify that interception of traffic is happening
         "
     ::= { ihubLiTargetStatsEntry 2 }    
   
ihubLiTargetIPsOperational       OBJECT-TYPE
      SYNTAX     Integer32(0..64)
      MAX-ACCESS read-only
      STATUS     current
      DESCRIPTION
         "
          when target is operational down: 0
          when target is operational up: 
-	Number of DHCP sessions (=IP's) associated with given circuit-id or
-	Number of DHCP sessions (=IP's) associated with given remote-id or
-	Number of static address ranges configured  "
      ::= { ihubLiTargetStatsEntry 3 }

ihubLiTargetIPsIntercepted       OBJECT-TYPE
      SYNTAX     Integer32(0..64)
      MAX-ACCESS read-only
      STATUS     current
      DESCRIPTION
         " when target is operational down: 0
          when target is operational up: 
-	Number of DHCP sessions (=IP's) associated with given circuit-id being intercepted or
-	Number of DHCP sessions (=IP's) associated with given remote-id being intercepted or
-	Number of static address ranges being intercepted
          When IhubLiTargetIPsIntercepted < ihubLiTargetIPsOperational it means that the system is out
          of resources and does not intercept some IP addresses of the target.        
         "
      ::= { ihubLiTargetStatsEntry 4 }

--
-- Target free indexes
--

ihubLiTargetGetNextFreeIndex  OBJECT-TYPE
    SYNTAX  Integer32(0..16)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
         "The next Free Index value that can be used for creating
	 a new entry in the Target Table.
         This object contains an appropriate value to be used for 
         ihubLiTargetIndex  when creating entries in the ihubLiTargetTable. 
         The value 0 indicates that no unassigned entries are available. To 
         obtain the ihubLiTargetIndex  value for a new entry, the
         manager issues a management protocol retrieval operation to obtain the
         current-value of this object. After each retrieval, the agent should
         modify the value to the next unassigned index.
       "
    ::= { ihubLiTarget 6 }


ihubLiTargetStaticGetNextFreeIndexTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IhubLiTargetStaticGetNextFreeIndexEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "
          Contains free index for a particular static entry
        "
    ::= { ihubLiTarget 7 }
	

ihubLiTargetStaticGetNextFreeIndexEntry OBJECT-TYPE
    SYNTAX       IhubLiTargetStaticGetNextFreeIndexEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Information about static IP address ranges 
        "
    INDEX { ihubLiTargetIndex}
    ::= { ihubLiTargetStaticGetNextFreeIndexTable 1 }


IhubLiTargetStaticGetNextFreeIndexEntry::= SEQUENCE
    {
        ihubLiTargetStaticGetNextFreeIndex  Integer32
    }


ihubLiTargetStaticGetNextFreeIndex  OBJECT-TYPE
    SYNTAX  Integer32(0..8)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
         "The next Free Index value that can be used for creating
	 a new entry in the Target static IP Table.
         This object contains an appropriate value to be used for 
         ihubLiTargetStaticIPIndex when adding entries in the ihubLiTargetStaticIPTable
         for a particular ihubLiTargetIndex
         The value 0 indicates that no unassigned entries are available. To 
         obtain the ihubLiTargetStaticIPIndex value for a new entry, the
         manager issues a management protocol retrieval operation to obtain the
         current-value of this object. After each retrieval, the agent should
         modify the value to the next unassigned index.
       "
    ::= { ihubLiTargetStaticGetNextFreeIndexEntry 1 }

ihubLiTargetPending   OBJECT-TYPE
    SYNTAX      Integer32(0..64)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
          Set to 0 when the system is able to intercept all configured targets
          Set to any other value when some target IP's cannot be installed in HW and are pending
         "
         ::= { ihubLiTarget 8 }

